\v 11  \w Denn|strong="G1063"\w* mich \w verlangt|strong="G1971"\w*, \w euch|strong="G5209"\w* zu \w sehen|strong="G1492"\w*, auf \w daß|strong="G2443"\w* ich \w euch|strong="G5213"\w* \w mitteile|strong="G3330"\w* \w etwas|strong="G5100"\w* \w geistlicher|strong="G4152"\w* \w Gabe|strong="G5486"\w*, \w euch|strong="G5209"\w* \w zu|strong="G1519"\w* \w stärken|strong="G4741"\w*;  
\v 12  \w das|strong="G1161"\w* \w ist|strong="G2076"\w*, daß ich \w samt|strong="G1722"\w* \w euch|strong="G5213"\w* getröstet \w würde|strong="G4837"\w* \w durch|strong="G1223"\w* \w euren|strong="G5216"\w* \w und|strong="G2532"\w* \w meinen|strong="G1700"\w* \w Glauben|strong="G4102"\w*, den wir \w untereinander|strong="G0240"\w* haben.  
\v 13 Ich \w will|strong="G2309"\w* \w euch|strong="G5209"\w* \w aber|strong="G1161"\w* \w nicht|strong="G3756"\w* \w verhalten|strong="G0050"\w*, liebe \w Brüder|strong="G0080"\w*, \w daß|strong="G3754"\w* ich mir \w oft|strong="G4178"\w* habe \w vorgesetzt|strong="G4388"\w*, \w zu|strong="G4314"\w* \w euch|strong="G5209"\w* zu \w kommen|strong="G2064"\w* (\w bin|strong="G2967"\w* \w aber|strong="G2532"\w* \w verhindert|strong="G2967"\w* \w bisher|strong="G0891"\w*), \w daß|strong="G2443"\w* ich \w auch|strong="G2532"\w* \w unter|strong="G1722"\w* \w euch|strong="G5213"\w* \w Frucht|strong="G2590"\w* \w schaffte|strong="G2192"\w* \w gleichwie|strong="G2531"\w* \w unter|strong="G1722"\w* \w andern|strong="G3062"\w* \w Heiden|strong="G1484"\w*.  
\v 14 Ich \w bin|strong="G1510"\w* ein \w Schuldner|strong="G3781"\w* der \w Griechen|strong="G1672"\w* \w und|strong="G2532"\w* der \w Ungriechen|strong="G0915"\w*, der \w Weisen|strong="G4680"\w* \w und|strong="G2532"\w* der \w Unweisen|strong="G0453"\w*.  
\v 15  \w Darum|strong="G3779"\w*, \w soviel|strong="G2596"\w* an mir \w ist|strong="G1691"\w*, bin ich \w geneigt|strong="G4289"\w*, \w auch|strong="G2532"\w* \w euch|strong="G5213"\w* \w zu|strong="G1722"\w* \w Rom|strong="G4516"\w* das Evangelium zu \w predigen|strong="G2097"\w*.  
\v 16  \w Denn|strong="G1063"\w* ich schäme \w mich|strong="G1870"\w* des \w Evangeliums|strong="G2098"\w* von \w Christo|strong="G5547"\w* \w nicht|strong="G3756"\w*; \w denn|strong="G1063"\w* es \w ist|strong="G2076"\w* eine \w Kraft|strong="G1411"\w* \w Gottes|strong="G2316"\w*, die \w da|strong="G1519"\w* selig \w macht|strong="G4991"\w* \w alle|strong="G3956"\w*, die daran \w glauben|strong="G4100"\w*, die \w Juden|strong="G2453"\w* \w vornehmlich|strong="G4412"\w* \w und|strong="G5037"\w* \w auch|strong="G2532"\w* die \w Griechen|strong="G1672"\w*.  
\v 17  \w Sintemal|strong="G1063"\w* \w darin|strong="G1722"\w* offenbart \w wird|strong="G0601"\w* die \w Gerechtigkeit|strong="G1343"\w*, die vor \w Gott|strong="G2316"\w* gilt, welche kommt \w aus|strong="G1537"\w* \w Glauben|strong="G4102"\w* \w in|strong="G1519"\w* \w Glauben|strong="G4102"\w*; \w wie|strong="G2531"\w* denn geschrieben \w steht|strong="G1125"\w*: “Der \w Gerechte|strong="G1342"\w* \w wird|strong="G2198"\w* \w seines|strong="G1537"\w* \w Glaubens|strong="G4102"\w* \w leben|strong="G2198"\w*.”  
\v 18  \w Denn|strong="G1063"\w* \w Gottes|strong="G2316"\w* \w Zorn|strong="G3709"\w* \w vom|strong="G0575"\w* \w Himmel|strong="G3772"\w* wird \w offenbart|strong="G0601"\w* \w über|strong="G1909"\w* \w alles|strong="G3956"\w* gottlose \w Wesen|strong="G0763"\w* \w und|strong="G2532"\w* \w Ungerechtigkeit|strong="G0093"\w* der \w Menschen|strong="G0444"\w*, die die \w Wahrheit|strong="G0225"\w* \w in|strong="G1722"\w* \w Ungerechtigkeit|strong="G0093"\w* \w aufhalten|strong="G2722"\w*.  
\v 19  \w Denn|strong="G1360"\w* was man von \w Gott|strong="G2316"\w* \w weiß|strong="G1110"\w*, \w ist|strong="G2076"\w* \w ihnen|strong="G1722"\w* \w offenbar|strong="G5318"\w*; \w denn|strong="G1063"\w* \w Gott|strong="G2316"\w* \w hat|strong="G5319"\w* es \w ihnen|strong="G0846"\w* \w offenbart|strong="G5319"\w*,  
\v 20  \w damit|strong="G1063"\w* daß \w Gottes|strong="G0846"\w* unsichtbares \w Wesen|strong="G0517"\w*, das ist \w seine|strong="G0846"\w* \w ewige|strong="G0126"\w* \w Kraft|strong="G1411"\w* \w und|strong="G2532"\w* \w Gottheit|strong="G2305"\w*, wird \w ersehen|strong="G3539"\w*, so man des \w wahrnimmt|strong="G2529"\w*, an den \w Werken|strong="G4161"\w*, nämlich \w an|strong="G0575"\w* der \w Schöpfung|strong="G2937"\w* der \w Welt|strong="G2889"\w*; \w also|strong="G1519"\w* daß \w sie|strong="G0846"\w* keine \w Entschuldigung|strong="G0379"\w* \w haben|strong="G1511"\w*,  
\v 21  \w dieweil|strong="G1360"\w* sie \w wußten|strong="G1097"\w*, daß ein \w Gott|strong="G2316"\w* ist, und \w haben|strong="G1392"\w* ihn \w nicht|strong="G3756"\w* \w gepriesen|strong="G1392"\w* \w als|strong="G5613"\w* einen \w Gott|strong="G2316"\w* \w noch|strong="G2228"\w* ihm \w gedankt|strong="G2168"\w*, \w sondern|strong="G0235"\w* \w sind|strong="G3154"\w* \w in|strong="G1722"\w* \w ihrem|strong="G0846"\w* \w Dichten|strong="G1261"\w* eitel \w geworden|strong="G3154"\w*, \w und|strong="G2532"\w* \w ihr|strong="G0846"\w* \w unverständiges|strong="G0801"\w* \w Herz|strong="G2588"\w* ist \w verfinstert|strong="G4654"\w*.  
\v 22 Da sie \w sich|strong="G5335"\w* \w für|strong="G1511"\w* \w Weise|strong="G4680"\w* \w hielten|strong="G5335"\w*, sind sie zu Narren \w geworden|strong="G3471"\w*  
\v 23  \w und|strong="G2532"\w* haben \w verwandelt|strong="G0236"\w* die \w Herrlichkeit|strong="G1391"\w* des \w unvergänglichen|strong="G0862"\w* \w Gottes|strong="G2316"\w* \w in|strong="G1722"\w* ein \w Bild|strong="G1504"\w* \w gleich|strong="G3667"\w* dem \w vergänglichen|strong="G5349"\w* \w Menschen|strong="G0444"\w* \w und|strong="G2532"\w* der \w Vögel|strong="G4071"\w* \w und|strong="G2532"\w* der \w vierfüßigen|strong="G5074"\w* \w und|strong="G2532"\w* der kriechenden \w Tiere|strong="G2062"\w*.  
\v 24  \w Darum|strong="G1352"\w* \w hat|strong="G3860"\w* \w sie|strong="G0846"\w* \w auch|strong="G2532"\w* \w Gott|strong="G2316"\w* \w dahingegeben|strong="G3860"\w* \w in|strong="G1722"\w* \w ihrer|strong="G0846"\w* \w Herzen|strong="G2588"\w* \w Gelüste|strong="G1939"\w*, \w in|strong="G1519"\w* \w Unreinigkeit|strong="G0167"\w*, zu \w schänden|strong="G0818"\w* ihre \w eigenen|strong="G0846"\w* \w Leiber|strong="G4983"\w* \w an|strong="G1722"\w* sich \w selbst|strong="G1438"\w*,  
\v 25  \w sie|strong="G3748"\w*, die \w Gottes|strong="G2316"\w* \w Wahrheit|strong="G0225"\w* haben \w verwandelt|strong="G3337"\w* \w in|strong="G1722"\w* die \w Lüge|strong="G5579"\w* \w und|strong="G2532"\w* haben \w geehrt|strong="G4573"\w* \w und|strong="G2532"\w* \w gedient|strong="G3000"\w* dem \w Geschöpfe|strong="G2937"\w* mehr \w denn|strong="G3844"\w* dem \w Schöpfer|strong="G2936"\w*, \w der|strong="G3739"\w* da \w gelobt|strong="G2128"\w* \w ist|strong="G2076"\w* \w in|strong="G1519"\w* \w Ewigkeit|strong="G0165"\w*. \w Amen|strong="G0281"\w*.  
\v 26  \w Darum|strong="G1223"\w* \w hat|strong="G3860"\w* \w sie|strong="G0846"\w* auch \w Gott|strong="G2316"\w* \w dahingegeben|strong="G3860"\w* \w in|strong="G1519"\w* \w schändliche|strong="G0819"\w* \w Lüste|strong="G3806"\w*: \w denn|strong="G1063"\w* \w ihre|strong="G0846"\w* \w Weiber|strong="G2338"\w* haben \w verwandelt|strong="G3337"\w* den \w natürlichen|strong="G5446"\w* \w Brauch|strong="G5540"\w* \w in|strong="G1519"\w* den \w unnatürlichen|strong="G3844"\w*;  
\v 27  \w desgleichen|strong="G5037"\w* \w auch|strong="G2532"\w* die \w Männer|strong="G0730"\w* haben \w verlassen|strong="G0863"\w* den \w natürlichen|strong="G5446"\w* \w Brauch|strong="G5540"\w* des \w Weibes|strong="G2338"\w* und \w sind|strong="G1572"\w* \w aneinander|strong="G1519"\w* \w erhitzt|strong="G1572"\w* \w in|strong="G1722"\w* \w ihren|strong="G0846"\w* \w Lüsten|strong="G3715"\w* und \w haben|strong="G2716"\w* \w Mann|strong="G0730"\w* \w mit|strong="G1722"\w* \w Mann|strong="G0730"\w* \w Schande|strong="G0808"\w* \w getrieben|strong="G2716"\w* \w und|strong="G2532"\w* den \w Lohn|strong="G0489"\w* \w ihres|strong="G0846"\w* \w Irrtums|strong="G4106"\w* (wie \w es|strong="G3739"\w* denn sein \w sollte|strong="G1163"\w*) \w an|strong="G1722"\w* sich \w selbst|strong="G1438"\w* \w empfangen|strong="G0618"\w*.  
\v 28  \w Und|strong="G2532"\w* \w gleichwie|strong="G2531"\w* sie \w nicht|strong="G3756"\w* geachtet \w haben|strong="G1381"\w*, daß sie \w Gott|strong="G2316"\w* \w erkenneten|strong="G2192"\w*, hat \w sie|strong="G0846"\w* \w Gott|strong="G2316"\w* auch \w dahingegeben|strong="G3860"\w* \w in|strong="G1519"\w* \w verkehrten|strong="G0096"\w* \w Sinn|strong="G3563"\w*, zu \w tun|strong="G4160"\w*, was \w nicht|strong="G3361"\w* \w taugt|strong="G2520"\w*,  
\v 29  \w voll|strong="G4137"\w* \w alles|strong="G3956"\w* \w Ungerechten|strong="G0093"\w*, \w Hurerei|strong="G4202"\w*, \w Schalkheit|strong="G4189"\w*, \w Geizes|strong="G4124"\w*, \w Bosheit|strong="G2549"\w*, \w voll|strong="G3324"\w* \w Neides|strong="G5355"\w*, \w Mordes|strong="G5408"\w*, \w Haders|strong="G2054"\w*, \w List|strong="G1388"\w*, \w giftig|strong="G2550"\w*, \w Ohrenbläser|strong="G5588"\w*,  
\v 30  \w Verleumder|strong="G2637"\w*, \w Gottesverächter|strong="G2319"\w*, \w Frevler|strong="G5197"\w*, \w hoffärtig|strong="G5244"\w*, \w ruhmredig|strong="G0213"\w*, \w Schädliche|strong="G2182"\w*, den \w Eltern|strong="G1118"\w* \w ungehorsam|strong="G0545"\w*,  
\v 31  \w Unvernünftige|strong="G0801"\w*, \w Treulose|strong="G0802"\w*, \w Lieblose|strong="G0794"\w*, \w unversöhnlich|strong="G0786"\w*, \w unbarmherzig|strong="G0415"\w*.  
\v 32  \w Sie|strong="G3748"\w* \w wissen|strong="G1921"\w* \w Gottes|strong="G2316"\w* \w Gerechtigkeit|strong="G1345"\w*, \w daß|strong="G3754"\w*, die \w solches|strong="G5108"\w* \w tun|strong="G4238"\w*, des \w Todes|strong="G2288"\w* \w würdig|strong="G0514"\w* \w sind|strong="G1526"\w*, und \w tun|strong="G4160"\w* \w es|strong="G0846"\w* \w nicht|strong="G3756"\w* \w allein|strong="G3440"\w*, \w sondern|strong="G0235"\w* haben auch \w Gefallen|strong="G4909"\w* an denen, die es \w tun|strong="G4238"\w*.   
\c 2  
\p
\v 1  \w Darum|strong="G1352"\w*, \w o|strong="G5599"\w* \w Mensch|strong="G0444"\w*, \w kannst|strong="G1488"\w* du dich nicht \w entschuldigen|strong="G0379"\w*, \w wer|strong="G3956"\w* du auch bist, der da \w richtet|strong="G2919"\w*. \w Denn|strong="G1063"\w* \w worin|strong="G1722"\w* du einen \w andern|strong="G2087"\w* \w richtest|strong="G2919"\w*, \w verdammst|strong="G2632"\w* du dich \w selbst|strong="G4572"\w*; \w sintemal|strong="G1063"\w* du eben \w dasselbe|strong="G0846"\w* \w tust|strong="G4238"\w*, was du \w richtest|strong="G2919"\w*.  
\v 2  \w Denn|strong="G1161"\w* wir \w wissen|strong="G1492"\w*, \w daß|strong="G3754"\w* \w Gottes|strong="G2316"\w* \w Urteil|strong="G2917"\w* \w ist|strong="G2076"\w* \w recht|strong="G0225"\w* \w über|strong="G1909"\w* die, so \w solches|strong="G5108"\w* \w tun|strong="G4238"\w*.  
\v 3 Denkst \w du|strong="G3049"\w* \w aber|strong="G1161"\w*, \w o|strong="G5599"\w* \w Mensch|strong="G0444"\w*, der du \w richtest|strong="G2919"\w* die, die \w solches|strong="G5108"\w* \w tun|strong="G4238"\w*, \w und|strong="G2532"\w* \w tust|strong="G4160"\w* auch \w dasselbe|strong="G0846"\w*, \w daß|strong="G3754"\w* \w du|strong="G4771"\w* dem \w Urteil|strong="G2917"\w* \w Gottes|strong="G2316"\w* entrinnen \w werdest|strong="G1628"\w*?  
\v 4  \w Oder|strong="G2228"\w* verachtest \w du|strong="G2706"\w* den \w Reichtum|strong="G4149"\w* \w seiner|strong="G0846"\w* \w Güte|strong="G5544"\w*, \w Geduld|strong="G0463"\w* \w und|strong="G2532"\w* \w Langmütigkeit|strong="G3115"\w*? \w Weißt|strong="G0050"\w* du \w nicht|strong="G0050"\w*, \w daß|strong="G3754"\w* \w dich|strong="G4571"\w* \w Gottes|strong="G2316"\w* \w Güte|strong="G5543"\w* \w zur|strong="G1519"\w* \w Buße|strong="G3341"\w* \w leitet|strong="G0071"\w*?  
\v 5 Du \w aber|strong="G1161"\w* \w nach|strong="G2596"\w* \w deinem|strong="G4675"\w* \w verstockten|strong="G4643"\w* \w und|strong="G2532"\w* \w unbußfertigen|strong="G0279"\w* \w Herzen|strong="G2588"\w* \w häufest|strong="G2343"\w* dir \w selbst|strong="G4572"\w* den \w Zorn|strong="G3709"\w* \w auf|strong="G1722"\w* den \w Tag|strong="G2250"\w* des \w Zornes|strong="G3709"\w* \w und|strong="G2532"\w* der \w Offenbarung|strong="G0602"\w* des gerechten \w Gerichtes|strong="G1341"\w* \w Gottes|strong="G2316"\w*,  
\v 6  \w welcher|strong="G3739"\w* geben \w wird|strong="G0591"\w* einem \w jeglichen|strong="G1538"\w* \w nach|strong="G2596"\w* \w seinen|strong="G0846"\w* \w Werken|strong="G2041"\w*:  
