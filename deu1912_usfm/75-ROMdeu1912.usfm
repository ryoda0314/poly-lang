\v 1  \w Paulus|strong="G3972"\w*, ein \w Knecht|strong="G1401"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*, \w berufen|strong="G2822"\w* zum \w Apostel|strong="G0652"\w*, \w ausgesondert|strong="G0873"\w*, \w zu|strong="G1519"\w* predigen das \w Evangelium|strong="G2098"\w* \w Gottes|strong="G2316"\w*,  
\v 2  \w welches|strong="G3739"\w* er zuvor verheißen \w hat|strong="G4279"\w* \w durch|strong="G1223"\w* \w seine|strong="G0846"\w* \w Propheten|strong="G4396"\w* \w in|strong="G1722"\w* der \w heiligen|strong="G0040"\w* \w Schrift|strong="G1124"\w*,  
\v 3  \w von|strong="G4012"\w* \w seinem|strong="G0846"\w* \w Sohn|strong="G5207"\w*, \w der|strong="G3588"\w* geboren \w ist|strong="G1096"\w* \w von|strong="G1537"\w* dem \w Samen|strong="G4690"\w* \w Davids|strong="G1138"\w* \w nach|strong="G2596"\w* dem \w Fleisch|strong="G4561"\w*  
