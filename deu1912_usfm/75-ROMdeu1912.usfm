\v 5  \w durch|strong="G1223"\w* \w welchen|strong="G3739"\w* wir haben \w empfangen|strong="G2983"\w* \w Gnade|strong="G5485"\w* \w und|strong="G2532"\w* \w Apostelamt|strong="G0651"\w*, \w unter|strong="G1722"\w* \w allen|strong="G3956"\w* \w Heiden|strong="G1484"\w* \w den|strong="G1519"\w* \w Gehorsam|strong="G5218"\w* des \w Glaubens|strong="G4102"\w* aufzurichten \w unter|strong="G5228"\w* \w seinem|strong="G0846"\w* \w Namen|strong="G3686"\w*,  
\v 6  \w unter|strong="G1722"\w* \w welchen|strong="G3739"\w* \w ihr|strong="G5210"\w* \w auch|strong="G2532"\w* \w seid|strong="G2075"\w*, die da berufen \w sind|strong="G2822"\w* von \w Jesu|strong="G2424"\w* \w Christo|strong="G5547"\w*,  
\v 7  \w allen|strong="G3956"\w*, die \w zu|strong="G1722"\w* \w Rom|strong="G4516"\w* \w sind|strong="G5607"\w*, den \w Liebsten|strong="G0027"\w* \w Gottes|strong="G2316"\w* und \w berufenen|strong="G2822"\w* \w Heiligen|strong="G0040"\w*: \w Gnade|strong="G5485"\w* sei mit \w euch|strong="G5213"\w* \w und|strong="G2532"\w* \w Friede|strong="G1515"\w* \w von|strong="G0575"\w* \w Gott|strong="G2316"\w*, \w unserm|strong="G2257"\w* \w Vater|strong="G3962"\w*, \w und|strong="G2532"\w* dem \w HERRN|strong="G2962"\w* \w Jesus|strong="G2424"\w* \w Christus|strong="G5547"\w*!  
\v 8 Aufs \w erste|strong="G4412"\w* \w danke|strong="G2168"\w* ich \w meinem|strong="G3450"\w* \w Gott|strong="G2316"\w* \w durch|strong="G1223"\w* \w Jesum|strong="G2424"\w* \w Christum|strong="G5547"\w* \w euer|strong="G5216"\w* \w aller|strong="G3956"\w* \w halben|strong="G5228"\w*, \w daß|strong="G3754"\w* man von \w eurem|strong="G5216"\w* \w Glauben|strong="G4102"\w* \w in|strong="G1722"\w* \w aller|strong="G3650"\w* \w Welt|strong="G2889"\w* \w sagt|strong="G2605"\w*.  
\v 9  \w Denn|strong="G1063"\w* \w Gott|strong="G2316"\w* \w ist|strong="G2076"\w* \w mein|strong="G3450"\w* \w Zeuge|strong="G3144"\w*, \w welchem|strong="G3739"\w* ich \w diene|strong="G3000"\w* \w in|strong="G1722"\w* \w meinem|strong="G3450"\w* \w Geist|strong="G4151"\w* \w am|strong="G1722"\w* \w Evangelium|strong="G2098"\w* von \w seinem|strong="G0846"\w* \w Sohn|strong="G5207"\w*, \w daß|strong="G5613"\w* ich ohne \w Unterlaß|strong="G0089"\w* \w euer|strong="G5216"\w* \w gedenke|strong="G4160"\w*  
