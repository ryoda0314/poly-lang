\c 1  
\p
\v 1  \w Paulus|strong="G3972"\w*, \w berufen|strong="G2822"\w* zum \w Apostel|strong="G0652"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w* \w durch|strong="G1223"\w* den \w Willen|strong="G2307"\w* \w Gottes|strong="G2316"\w*, \w und|strong="G2532"\w* \w Bruder|strong="G0080"\w* \w Sosthenes|strong="G4988"\w*  
\v 2 der \w Gemeinde|strong="G1577"\w* \w zu|strong="G1722"\w* \w Korinth|strong="G2882"\w*, den \w Geheiligten|strong="G0037"\w* \w in|strong="G1722"\w* \w Christo|strong="G5547"\w* \w Jesu|strong="G2424"\w*, den \w berufenen|strong="G2822"\w* \w Heiligen|strong="G0040"\w* \w samt|strong="G4862"\w* \w allen|strong="G3956"\w* denen, die \w anrufen|strong="G1941"\w* den \w Namen|strong="G3686"\w* \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w* \w an|strong="G1722"\w* \w allen|strong="G3956"\w* \w ihren|strong="G0846"\w* \w und|strong="G2532"\w* \w unsern|strong="G2257"\w* \w Orten|strong="G5117"\w*:  
\v 3  \w Gnade|strong="G5485"\w* sei mit \w euch|strong="G5213"\w* \w und|strong="G2532"\w* \w Friede|strong="G1515"\w* \w von|strong="G0575"\w* \w Gott|strong="G2316"\w*, \w unserm|strong="G2257"\w* \w Vater|strong="G3962"\w*, \w und|strong="G2532"\w* dem \w HERRN|strong="G2962"\w* \w Jesus|strong="G2424"\w* \w Christus|strong="G5547"\w*!  
\v 4 Ich \w danke|strong="G2168"\w* \w meinem|strong="G3450"\w* \w Gott|strong="G2316"\w* \w allezeit|strong="G3842"\w* \w eurethalben|strong="G5216"\w* \w für|strong="G1909"\w* die \w Gnade|strong="G5485"\w* \w Gottes|strong="G2316"\w*, \w die|strong="G3588"\w* \w euch|strong="G5213"\w* gegeben \w ist|strong="G1325"\w* \w in|strong="G1722"\w* \w Christo|strong="G5547"\w* \w Jesu|strong="G2424"\w*,  
\v 5  \w daß|strong="G3754"\w* ihr \w seid|strong="G4148"\w* \w durch|strong="G1722"\w* \w ihn|strong="G0846"\w* \w an|strong="G1722"\w* allen \w Stücken|strong="G3956"\w* reich \w gemacht|strong="G4148"\w*, \w an|strong="G1722"\w* \w aller|strong="G3956"\w* \w Lehre|strong="G3056"\w* \w und|strong="G2532"\w* in \w aller|strong="G3956"\w* \w Erkenntnis|strong="G1108"\w*;  
\v 6 wie \w denn|strong="G2531"\w* die \w Predigt|strong="G3142"\w* von \w Christus|strong="G5547"\w* \w in|strong="G1722"\w* \w euch|strong="G5213"\w* kräftig geworden \w ist|strong="G0950"\w*,  
\v 7 also \w daß|strong="G5620"\w* \w ihr|strong="G5209"\w* \w keinen|strong="G3361"\w* Mangel \w habt|strong="G5302"\w* \w an|strong="G1722"\w* irgend einer \w Gabe|strong="G5486"\w* und \w wartet|strong="G0553"\w* nur auf die \w Offenbarung|strong="G0602"\w* \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*,  
\v 8  \w welcher|strong="G3739"\w* \w auch|strong="G2532"\w* \w wird|strong="G0950"\w* \w euch|strong="G5209"\w* fest \w erhalten|strong="G0950"\w* \w bis|strong="G2193"\w* ans \w Ende|strong="G5056"\w*, daß ihr unsträflich \w seid|strong="G0410"\w* \w auf|strong="G1722"\w* den \w Tag|strong="G2250"\w* \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*.  
\v 9 Denn \w Gott|strong="G2316"\w* ist \w treu|strong="G4103"\w*, \w durch|strong="G1223"\w* \w welchen|strong="G3739"\w* ihr berufen \w seid|strong="G2564"\w* \w zur|strong="G1519"\w* \w Gemeinschaft|strong="G2842"\w* \w seines|strong="G0846"\w* \w Sohnes|strong="G5207"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*, \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w*.  
\v 10 Ich \w ermahne|strong="G3870"\w* \w euch|strong="G5209"\w* \w aber|strong="G1161"\w*, liebe \w Brüder|strong="G0080"\w*, \w durch|strong="G1223"\w* den \w Namen|strong="G3686"\w* \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*, \w daß|strong="G2443"\w* ihr \w allzumal|strong="G3956"\w* \w einerlei|strong="G0846"\w* Rede \w führt|strong="G3004"\w* \w und|strong="G2532"\w* \w lasset|strong="G5600"\w* \w nicht|strong="G3361"\w* \w Spaltungen|strong="G4978"\w* \w unter|strong="G1722"\w* \w euch|strong="G5213"\w* \w sein|strong="G5600"\w*, \w sondern|strong="G1161"\w* \w haltet|strong="G5600"\w* fest \w aneinander|strong="G2675"\w* \w in|strong="G1722"\w* \w einem|strong="G0846"\w* \w Sinne|strong="G3563"\w* \w und|strong="G2532"\w* \w in|strong="G1722"\w* \w einerlei|strong="G0846"\w* \w Meinung|strong="G1106"\w*.  
\v 11  \w Denn|strong="G1063"\w* es \w ist|strong="G1213"\w* vor \w mich|strong="G3427"\w* \w gekommen|strong="G1213"\w*, \w liebe|strong="G3450"\w* \w Brüder|strong="G0080"\w*, \w durch|strong="G5259"\w* die aus \w Chloes|strong="G5514"\w* \w Gesinde|strong="G3588"\w* \w von|strong="G4012"\w* \w euch|strong="G5216"\w*, \w daß|strong="G3754"\w* \w Zank|strong="G2054"\w* \w unter|strong="G1722"\w* \w euch|strong="G5213"\w* \w sei|strong="G1526"\w*.  
\v 12 Ich \w sage|strong="G3004"\w* \w aber|strong="G1161"\w* \w davon|strong="G5124"\w*, \w daß|strong="G3754"\w* unter \w euch|strong="G5216"\w* \w einer|strong="G1538"\w* \w spricht|strong="G3004"\w*: \w Ich|strong="G1473"\w* \w bin|strong="G1510"\w* \w paulisch|strong="G3972"\w*, der \w andere|strong="G1161"\w*: \w Ich|strong="G1473"\w* bin \w apollisch|strong="G0625"\w*, der \w dritte|strong="G1161"\w*: \w Ich|strong="G1473"\w* bin \w kephisch|strong="G2786"\w*, der \w vierte|strong="G1161"\w*; \w Ich|strong="G1473"\w* bin \w christisch|strong="G5547"\w*.  
\v 13 Wie? \w Ist|strong="G3307"\w* \w Christus|strong="G5547"\w* nun \w zertrennt|strong="G3307"\w*? \w Ist|strong="G4717"\w* denn \w Paulus|strong="G3972"\w* \w für|strong="G5228"\w* \w euch|strong="G5216"\w* \w gekreuzigt|strong="G4717"\w*? \w Oder|strong="G2228"\w* \w seid|strong="G0907"\w* ihr \w auf|strong="G1519"\w* des \w Paulus|strong="G3972"\w* \w Namen|strong="G3686"\w* \w getauft|strong="G0907"\w*?  
\v 14 Ich \w danke|strong="G2168"\w* \w Gott|strong="G2316"\w*, \w daß|strong="G3754"\w* ich \w niemand|strong="G3762"\w* unter \w euch|strong="G5216"\w* getauft \w habe|strong="G0907"\w* \w außer|strong="G1508"\w* \w Krispus|strong="G2921"\w* \w und|strong="G2532"\w* \w Gajus|strong="G1050"\w*,  
\v 15 daß \w nicht|strong="G3363"\w* \w jemand|strong="G5100"\w* sagen \w möge|strong="G2036"\w*, ich \w hätte|strong="G0907"\w* \w auf|strong="G1519"\w* \w meinen|strong="G1699"\w* \w Namen|strong="G3686"\w* \w getauft|strong="G0907"\w*.  
\v 16 Ich \w habe|strong="G0907"\w* \w aber|strong="G1161"\w* \w auch|strong="G2532"\w* \w getauft|strong="G0907"\w* des \w Stephanas|strong="G4734"\w* \w Hausgesinde|strong="G3624"\w*; \w weiter|strong="G3063"\w* \w weiß|strong="G1492"\w* ich \w nicht|strong="G3756"\w*, ob ich \w etliche|strong="G1536"\w* \w andere|strong="G0243"\w* getauft \w habe|strong="G0907"\w*.  
\v 17  \w Denn|strong="G1063"\w* \w Christus|strong="G5547"\w* \w hat|strong="G0649"\w* \w mich|strong="G3165"\w* \w nicht|strong="G3756"\w* \w gesandt|strong="G0649"\w*, zu \w taufen|strong="G0907"\w*, \w sondern|strong="G0235"\w* das Evangelium zu \w predigen|strong="G2097"\w*, \w nicht|strong="G3756"\w* \w mit|strong="G1722"\w* \w klugen|strong="G4678"\w* \w Worten|strong="G3056"\w*, auf daß \w nicht|strong="G3363"\w* das \w Kreuz|strong="G4716"\w* \w Christi|strong="G5547"\w* zunichte \w werde|strong="G2758"\w*.  
\v 18  \w Denn|strong="G1063"\w* das \w Wort|strong="G3056"\w* vom \w Kreuz|strong="G4716"\w* \w ist|strong="G2076"\w* eine \w Torheit|strong="G3472"\w* \w denen|strong="G3303"\w*, die verloren \w werden|strong="G0622"\w*; \w uns|strong="G2254"\w* \w aber|strong="G1161"\w*, die wir selig \w werden|strong="G4982"\w* ist'\w s|strong="G2076"\w* eine \w Gotteskraft|strong="G1411"\w*.  
\v 19  \w Denn|strong="G1063"\w* es steht \w geschrieben|strong="G1125"\w*: “Ich will zunichte \w machen|strong="G0622"\w* die \w Weisheit|strong="G4678"\w* der \w Weisen|strong="G4680"\w*, \w und|strong="G2532"\w* den \w Verstand|strong="G4907"\w* der \w Verständigen|strong="G4908"\w* will ich \w verwerfen|strong="G0114"\w*.”  
\v 20  \w Wo|strong="G4226"\w* sind die \w Klugen|strong="G4680"\w*? \w Wo|strong="G4226"\w* sind die \w Schriftgelehrten|strong="G1122"\w*? \w Wo|strong="G4226"\w* sind die \w Weltweisen|strong="G4804"\w*? \w Hat|strong="G3471"\w* \w nicht|strong="G3780"\w* \w Gott|strong="G2316"\w* die \w Weisheit|strong="G4678"\w* \w dieser|strong="G5127"\w* \w Welt|strong="G2889"\w* zur Torheit \w gemacht|strong="G3471"\w*?  
\v 21  \w Denn|strong="G1063"\w* \w dieweil|strong="G1894"\w* die \w Welt|strong="G2889"\w* \w durch|strong="G1223"\w* ihre \w Weisheit|strong="G4678"\w* \w Gott|strong="G2316"\w* \w in|strong="G1722"\w* \w seiner|strong="G2316"\w* \w Weisheit|strong="G4678"\w* \w nicht|strong="G3756"\w* \w erkannte|strong="G1097"\w*, \w gefiel|strong="G2106"\w* es \w Gott|strong="G2316"\w* \w wohl|strong="G2106"\w*, \w durch|strong="G1223"\w* \w törichte|strong="G3472"\w* \w Predigt|strong="G2782"\w* selig zu \w machen|strong="G4982"\w* die, so daran \w glauben|strong="G4100"\w*.  
\v 22  \w Sintemal|strong="G2532"\w* die \w Juden|strong="G2453"\w* \w Zeichen|strong="G4592"\w* \w fordern|strong="G0154"\w* \w und|strong="G2532"\w* die \w Griechen|strong="G1672"\w* nach \w Weisheit|strong="G4678"\w* \w fragen|strong="G2212"\w*,  
\v 23  \w wir|strong="G2249"\w* \w aber|strong="G1161"\w* \w predigen|strong="G2784"\w* den \w gekreuzigten|strong="G4717"\w* \w Christus|strong="G5547"\w*, den \w Juden|strong="G2453"\w* ein \w Ärgernis|strong="G3303"\w* \w und|strong="G1161"\w* den \w Griechen|strong="G1672"\w* eine \w Torheit|strong="G3472"\w*;  
\v 24  \w denen|strong="G0846"\w* \w aber|strong="G1161"\w*, \w die|strong="G3588"\w* berufen \w sind|strong="G2822"\w*, \w Juden|strong="G2453"\w* \w und|strong="G2532"\w* \w Griechen|strong="G1672"\w*, predigen wir \w Christum|strong="G5547"\w*, \w göttliche|strong="G2316"\w* \w Kraft|strong="G1411"\w* \w und|strong="G2532"\w* \w göttliche|strong="G2316"\w* \w Weisheit|strong="G4678"\w*.  
\v 25  \w Denn|strong="G3754"\w* die \w göttliche|strong="G2316"\w* \w Torheit|strong="G3474"\w* \w ist|strong="G2076"\w* weiser, \w als|strong="G4680"\w* die \w Menschen|strong="G0444"\w* sind; \w und|strong="G2532"\w* die \w göttliche|strong="G2316"\w* \w Schwachheit|strong="G0772"\w* \w ist|strong="G2076"\w* stärker, \w als|strong="G2478"\w* die \w Menschen|strong="G0444"\w* sind.  
\v 26  \w Sehet|strong="G1063"\w* \w an|strong="G0991"\w*, liebe \w Brüder|strong="G0080"\w*, \w eure|strong="G5216"\w* \w Berufung|strong="G2821"\w*: \w nicht|strong="G3756"\w* \w viel|strong="G4183"\w* \w Weise|strong="G4680"\w* \w nach|strong="G2596"\w* dem \w Fleisch|strong="G4561"\w*, \w nicht|strong="G3756"\w* \w viel|strong="G4183"\w* \w Gewaltige|strong="G1415"\w*, \w nicht|strong="G3756"\w* \w viel|strong="G4183"\w* \w Edle|strong="G2104"\w* sind berufen.  
\v 27  \w Sondern|strong="G0235"\w* was \w töricht|strong="G3474"\w* ist vor der \w Welt|strong="G2889"\w*, das \w hat|strong="G1586"\w* \w Gott|strong="G2316"\w* \w erwählt|strong="G1586"\w*, \w daß|strong="G2443"\w* er die \w Weisen|strong="G4680"\w* zu Schanden \w mache|strong="G2617"\w*; \w und|strong="G2532"\w* was \w schwach|strong="G0772"\w* ist vor der \w Welt|strong="G2889"\w*, das \w hat|strong="G1586"\w* \w Gott|strong="G2316"\w* \w erwählt|strong="G1586"\w*, \w daß|strong="G2443"\w* er zu Schanden \w mache|strong="G2617"\w*, was stark \w ist|strong="G2478"\w*;  
\v 28  \w und|strong="G2532"\w* das \w Unedle|strong="G0036"\w* vor der \w Welt|strong="G2889"\w* \w und|strong="G2532"\w* das \w Verachtete|strong="G1848"\w* \w hat|strong="G1586"\w* \w Gott|strong="G2316"\w* \w erwählt|strong="G1586"\w*, \w und|strong="G2532"\w* das \w da|strong="G3588"\w* \w nichts|strong="G3361"\w* \w ist|strong="G5607"\w*, \w daß|strong="G2443"\w* er zunichte \w mache|strong="G2673"\w*, was etwas \w ist|strong="G5607"\w*,  
\v 29 auf \w daß|strong="G3704"\w* sich vor \w ihm|strong="G1799"\w* \w kein|strong="G3361"\w* \w Fleisch|strong="G4561"\w* \w rühme|strong="G2744"\w*.  
\v 30  \w Von|strong="G1537"\w* \w ihm|strong="G0846"\w* \w kommt|strong="G2075"\w* \w auch|strong="G1161"\w* \w ihr|strong="G5210"\w* her \w in|strong="G1722"\w* \w Christo|strong="G5547"\w* \w Jesu|strong="G2424"\w*, \w welcher|strong="G3739"\w* \w uns|strong="G2254"\w* gemacht \w ist|strong="G1096"\w* \w von|strong="G0575"\w* \w Gott|strong="G2316"\w* zur \w Weisheit|strong="G4678"\w* \w und|strong="G2532"\w* zur \w Gerechtigkeit|strong="G1343"\w* \w und|strong="G2532"\w* zur \w Heiligung|strong="G0038"\w* \w und|strong="G2532"\w* zur \w Erlösung|strong="G0629"\w*,  
\v 31 auf \w daß|strong="G2443"\w* (\w wie|strong="G2531"\w* geschrieben \w steht|strong="G1125"\w*), “wer sich \w rühmt|strong="G2744"\w*, der rühme \w sich|strong="G2744"\w* \w des|strong="G1722"\w* \w HERRN|strong="G2962"\w*!”   
\c 2  
\p
\v 1 Und \w ich|strong="G2504"\w*, liebe \w Brüder|strong="G0080"\w*, da ich \w zu|strong="G4314"\w* \w euch|strong="G5209"\w* \w kam|strong="G2064"\w*, \w kam|strong="G2064"\w* ich \w nicht|strong="G3756"\w* \w mit|strong="G2596"\w* \w hohen|strong="G5247"\w* \w Worten|strong="G3056"\w* \w oder|strong="G2228"\w* hoher \w Weisheit|strong="G4678"\w*, \w euch|strong="G5213"\w* zu \w verkündigen|strong="G2605"\w* die \w göttliche|strong="G2316"\w* \w Predigt|strong="G3142"\w*.  
\v 2  \w Denn|strong="G1063"\w* ich \w hielt|strong="G2919"\w* mich \w nicht|strong="G3756"\w* \w dafür|strong="G2919"\w*, daß ich \w etwas|strong="G5100"\w* \w wüßte|strong="G1492"\w* \w unter|strong="G1722"\w* \w euch|strong="G5213"\w*, als \w allein|strong="G1508"\w* \w Jesum|strong="G2424"\w* \w Christum|strong="G5547"\w*, \w den|strong="G5126"\w* \w Gekreuzigten|strong="G4717"\w*.  
\v 3  \w Und|strong="G2532"\w* \w ich|strong="G1473"\w* \w war|strong="G1096"\w* \w bei|strong="G4314"\w* \w euch|strong="G5209"\w* \w mit|strong="G1722"\w* \w Schwachheit|strong="G0769"\w* \w und|strong="G2532"\w* \w mit|strong="G1722"\w* \w Furcht|strong="G5401"\w* \w und|strong="G2532"\w* \w mit|strong="G1722"\w* \w großem|strong="G4183"\w* \w Zittern|strong="G5156"\w*;  
\v 4  \w und|strong="G2532"\w* \w mein|strong="G3450"\w* \w Wort|strong="G3056"\w* \w und|strong="G2532"\w* \w meine|strong="G3450"\w* \w Predigt|strong="G2782"\w* war \w nicht|strong="G3756"\w* \w in|strong="G1722"\w* \w vernünftigen|strong="G3981"\w* \w Reden|strong="G3056"\w* \w menschlicher|strong="G0442"\w* \w Weisheit|strong="G4678"\w*, \w sondern|strong="G0235"\w* \w in|strong="G1722"\w* \w Beweisung|strong="G0585"\w* des \w Geistes|strong="G4151"\w* \w und|strong="G2532"\w* der \w Kraft|strong="G1411"\w*,  
\v 5 auf \w daß|strong="G2443"\w* \w euer|strong="G5216"\w* \w Glaube|strong="G4102"\w* \w bestehe|strong="G5600"\w* \w nicht|strong="G3361"\w* \w auf|strong="G1722"\w* \w Menschenweisheit|strong="G4678"\w*, \w sondern|strong="G0235"\w* \w auf|strong="G1722"\w* \w Gottes|strong="G2316"\w* \w Kraft|strong="G1411"\w*.  
\v 6 Wovon wir \w aber|strong="G1161"\w* \w reden|strong="G2980"\w*, das ist \w dennoch|strong="G1161"\w* \w Weisheit|strong="G4678"\w* \w bei|strong="G1722"\w* den \w Vollkommenen|strong="G5046"\w*; \w nicht|strong="G3756"\w* eine \w Weisheit|strong="G4678"\w* \w dieser|strong="G5127"\w* \w Welt|strong="G0165"\w*, auch \w nicht|strong="G3761"\w* der \w Obersten|strong="G0758"\w* \w dieser|strong="G5127"\w* \w Welt|strong="G0165"\w*, welche \w vergehen|strong="G2673"\w*.  
\v 7  \w Sondern|strong="G0235"\w* wir \w reden|strong="G2980"\w* \w von|strong="G1722"\w* der \w heimlichen|strong="G3466"\w*, \w verborgenen|strong="G0613"\w* \w Weisheit|strong="G4678"\w* \w Gottes|strong="G2316"\w*, \w welche|strong="G3739"\w* \w Gott|strong="G2316"\w* verordnet \w hat|strong="G4309"\w* \w vor|strong="G4253"\w* der \w Welt|strong="G0165"\w* \w zu|strong="G1519"\w* \w unsrer|strong="G2257"\w* \w Herrlichkeit|strong="G1391"\w*,  
\v 8  \w welche|strong="G3739"\w* \w keiner|strong="G3762"\w* von den \w Obersten|strong="G0758"\w* \w dieser|strong="G5127"\w* \w Welt|strong="G0165"\w* erkannt \w hat|strong="G1097"\w*; \w denn|strong="G1487"\w* so sie die erkannt \w hätten|strong="G1097"\w*, \w hätten|strong="G4717"\w* \w sie|strong="G0302"\w* den \w HERRN|strong="G2962"\w* der \w Herrlichkeit|strong="G1391"\w* \w nicht|strong="G3756"\w* \w gekreuzigt|strong="G4717"\w*.  
