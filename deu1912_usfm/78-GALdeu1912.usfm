\c 2  
\p
\v 1  \w Darnach|strong="G1899"\w* über \w vierzehn|strong="G1180"\w* \w Jahre|strong="G2094"\w* \w zog|strong="G0305"\w* \w ich|strong="G1223"\w* \w abermals|strong="G3825"\w* \w hinauf|strong="G0305"\w* \w gen|strong="G1519"\w* \w Jerusalem|strong="G2414"\w* \w mit|strong="G3326"\w* \w Barnabas|strong="G0921"\w* und \w nahm|strong="G4838"\w* \w Titus|strong="G5103"\w* \w auch|strong="G2532"\w* \w mit|strong="G4838"\w* mir.  
\v 2 Ich \w zog|strong="G0305"\w* \w aber|strong="G1161"\w* \w hinauf|strong="G0305"\w* \w aus|strong="G2596"\w* einer \w Offenbarung|strong="G0602"\w* \w und|strong="G2532"\w* \w besprach|strong="G0394"\w* mich mit \w ihnen|strong="G0846"\w* über das \w Evangelium|strong="G2098"\w*, \w das|strong="G3739"\w* ich \w predige|strong="G2784"\w* \w unter|strong="G1722"\w* den \w Heiden|strong="G1484"\w*, \w besonders|strong="G2596"\w* \w aber|strong="G1161"\w* mit denen, die das Ansehen \w hatten|strong="G1380"\w*, auf daß ich \w nicht|strong="G3361"\w* \w vergeblich|strong="G1519"\w* \w liefe|strong="G5143"\w* \w oder|strong="G2228"\w* gelaufen \w wäre|strong="G5143"\w*.  
\v 3  \w Aber|strong="G0235"\w* es \w ward|strong="G3761"\w* \w auch|strong="G3761"\w* \w Titus|strong="G5103"\w* \w nicht|strong="G3761"\w* \w gezwungen|strong="G0315"\w*, \w sich|strong="G4059"\w* \w beschneiden|strong="G4059"\w* zu \w lassen|strong="G4059"\w*, \w der|strong="G3588"\w* \w mit|strong="G4862"\w* \w mir|strong="G1698"\w* \w war|strong="G1672"\w*, obwohl er \w ein|strong="G5607"\w* Grieche \w war|strong="G5607"\w*.  
\v 4  \w Denn|strong="G1161"\w* \w da|strong="G1223"\w* \w etliche|strong="G3920"\w* falsche \w Brüder|strong="G5569"\w* \w sich|strong="G3748"\w* mit eingedrängt \w hatten|strong="G3922"\w* und neben eingeschlichen waren, \w auszukundschaften|strong="G2684"\w* \w unsre|strong="G2257"\w* \w Freiheit|strong="G1657"\w*, \w die|strong="G3739"\w* \w wir|strong="G1722"\w* \w haben|strong="G2192"\w* in \w Christo|strong="G5547"\w* \w Jesu|strong="G2424"\w*, \w daß|strong="G2443"\w* sie \w uns|strong="G2248"\w* \w gefangennähmen|strong="G2615"\w*,  
\v 5  \w wichen|strong="G1502"\w* \w wir|strong="G3761"\w* \w denselben|strong="G3739"\w* \w nicht|strong="G3761"\w* eine \w Stunde|strong="G5610"\w*, ihnen \w untertan|strong="G5292"\w* zu \w sein|strong="G5292"\w*, auf \w daß|strong="G2443"\w* die \w Wahrheit|strong="G0225"\w* des \w Evangeliums|strong="G2098"\w* \w bei|strong="G4314"\w* \w euch|strong="G5209"\w* \w bestünde|strong="G1265"\w*.  
\v 6  \w Von|strong="G0575"\w* \w denen|strong="G1380"\w* \w aber|strong="G1161"\w*, die das Ansehen \w hatten|strong="G1380"\w*, \w welcherlei|strong="G3697"\w* sie \w weiland|strong="G4218"\w* \w gewesen|strong="G2258"\w* \w sind|strong="G1511"\w*, daran \w liegt|strong="G1308"\w* \w mir|strong="G3427"\w* \w nichts|strong="G3762"\w*; \w denn|strong="G1063"\w* \w Gott|strong="G2316"\w* \w achtet|strong="G2983"\w* das \w Ansehen|strong="G4383"\w* der \w Menschen|strong="G0444"\w* \w nicht|strong="G3756"\w*, mich \w haben|strong="G1698"\w* die, so das \w Ansehen|strong="G1380"\w* \w hatten|strong="G1380"\w*, \w nichts|strong="G3762"\w* anderes \w gelehrt|strong="G4323"\w*;  
\v 7  \w sondern|strong="G0235"\w* \w dagegen|strong="G5121"\w*, da sie \w sahen|strong="G1492"\w*, \w daß|strong="G3754"\w* mir \w vertraut|strong="G4100"\w* \w war|strong="G4100"\w* das \w Evangelium|strong="G2098"\w* an die \w Heiden|strong="G0203"\w*, \w gleichwie|strong="G2531"\w* dem \w Petrus|strong="G4074"\w* das Evangelium an die \w Juden|strong="G4061"\w*  
\v 8 (\w denn|strong="G1063"\w* der mit \w Petrus|strong="G4074"\w* kräftig \w gewesen|strong="G1754"\w* ist \w zum|strong="G1519"\w* \w Apostelamt|strong="G0651"\w* unter den \w Juden|strong="G4061"\w*, der \w ist|strong="G2532"\w* mit \w mir|strong="G1698"\w* auch kräftig \w gewesen|strong="G1754"\w* \w unter|strong="G1519"\w* den \w Heiden|strong="G1484"\w*),  
\v 9  \w und|strong="G2532"\w* da sie \w erkannten|strong="G1097"\w* die \w Gnade|strong="G5485"\w*, die \w mir|strong="G3427"\w* gegeben \w war|strong="G1325"\w*, \w Jakobus|strong="G2385"\w* \w und|strong="G2532"\w* \w Kephas|strong="G2786"\w* \w und|strong="G2532"\w* \w Johannes|strong="G2491"\w*, \w die|strong="G3588"\w* für \w Säulen|strong="G4769"\w* \w angesehen|strong="G1380"\w* \w waren|strong="G1511"\w*, gaben \w sie|strong="G1325"\w* \w mir|strong="G1698"\w* \w und|strong="G2532"\w* \w Barnabas|strong="G0921"\w* die \w rechte|strong="G1188"\w* \w Hand|strong="G2842"\w* und wurden mit uns eins, \w daß|strong="G2443"\w* \w wir|strong="G2249"\w* \w unter|strong="G1519"\w* die \w Heiden|strong="G1484"\w*, \w sie|strong="G1519"\w* \w aber|strong="G1161"\w* unter die \w Juden|strong="G4061"\w* gingen,  
\v 10  \w allein|strong="G3440"\w* \w daß|strong="G2443"\w* wir der \w Armen|strong="G4434"\w* \w gedächten|strong="G3421"\w*, \w welches|strong="G3739"\w* \w ich|strong="G2532"\w* \w auch|strong="G0846"\w* \w fleißig|strong="G4704"\w* bin \w gewesen|strong="G4704"\w* zu \w tun|strong="G4160"\w*.  
\v 11  \w Da|strong="G3753"\w* \w aber|strong="G1161"\w* \w Petrus|strong="G4074"\w* \w gen|strong="G1519"\w* \w Antiochien|strong="G0490"\w* \w kam|strong="G2064"\w*, widerstand \w ich|strong="G0436"\w* \w ihm|strong="G0846"\w* \w unter|strong="G2596"\w* \w Augen|strong="G4383"\w*; \w denn|strong="G3754"\w* es \w war|strong="G2258"\w* \w Klage|strong="G2607"\w* über ihn \w gekommen|strong="G2607"\w*.  
\v 12  \w Denn|strong="G1063"\w* \w zuvor|strong="G4253"\w*, ehe \w etliche|strong="G5100"\w* \w von|strong="G0575"\w* \w Jakobus|strong="G2385"\w* \w kamen|strong="G2064"\w*, aß \w er|strong="G4906"\w* \w mit|strong="G3326"\w* den \w Heiden|strong="G1484"\w*; \w da|strong="G3753"\w* \w sie|strong="G2064"\w* \w aber|strong="G1161"\w* \w kamen|strong="G2064"\w*, entzog \w er|strong="G5288"\w* sich \w und|strong="G2532"\w* \w sonderte|strong="G0873"\w* \w sich|strong="G1438"\w* \w ab|strong="G5399"\w*, darum daß er die \w aus|strong="G1537"\w* den \w Juden|strong="G4061"\w* \w fürchtete|strong="G5399"\w*.  
\v 13  \w Und|strong="G2532"\w* mit \w ihm|strong="G0846"\w* \w heuchelten|strong="G4942"\w* die \w andern|strong="G3062"\w* \w Juden|strong="G2453"\w*, \w also|strong="G5620"\w* daß \w auch|strong="G2532"\w* \w Barnabas|strong="G0921"\w* verführt \w ward|strong="G4879"\w*, mit \w ihnen|strong="G0846"\w* zu \w heucheln|strong="G5272"\w*.  
