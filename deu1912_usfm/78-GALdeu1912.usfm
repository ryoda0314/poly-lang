\v 1  \w Paulus|strong="G3972"\w*, ein \w Apostel|strong="G0652"\w* (\w nicht|strong="G3756"\w* \w von|strong="G0575"\w* \w Menschen|strong="G0444"\w*, auch \w nicht|strong="G3761"\w* \w durch|strong="G1223"\w* \w Menschen|strong="G0444"\w*, \w sondern|strong="G0235"\w* \w durch|strong="G1223"\w* \w Jesum|strong="G2424"\w* \w Christum|strong="G5547"\w* \w und|strong="G2532"\w* \w Gott|strong="G2316"\w*, den \w Vater|strong="G3962"\w*, \w der|strong="G3588"\w* \w ihn|strong="G0846"\w* \w auferweckt|strong="G1453"\w* \w hat|strong="G1453"\w* \w von|strong="G1537"\w* den \w Toten|strong="G3498"\w*),  
