\v 6 Mich \w wundert|strong="G2296"\w*, \w daß|strong="G3754"\w* ihr euch \w so|strong="G3779"\w* \w bald|strong="G5030"\w* abwenden lasset \w von|strong="G0575"\w* dem, der \w euch|strong="G5209"\w* \w berufen|strong="G2564"\w* \w hat|strong="G2564"\w* \w in|strong="G1722"\w* die \w Gnade|strong="G5485"\w* \w Christi|strong="G5547"\w*, \w zu|strong="G1519"\w* einem \w anderen|strong="G2087"\w* \w Evangelium|strong="G2098"\w*,  
\v 7  \w so|strong="G3739"\w* doch \w kein|strong="G3756"\w* \w anderes|strong="G0243"\w* \w ist|strong="G2076"\w*, \w außer|strong="G1508"\w*, daß \w etliche|strong="G5100"\w* \w sind|strong="G1526"\w*, die \w euch|strong="G5209"\w* \w verwirren|strong="G5015"\w* \w und|strong="G2532"\w* \w wollen|strong="G2309"\w* das \w Evangelium|strong="G2098"\w* \w Christi|strong="G5547"\w* \w verkehren|strong="G3344"\w*.  
\v 8  \w Aber|strong="G0235"\w* \w so|strong="G1437"\w* \w auch|strong="G2532"\w* \w wir|strong="G2249"\w* \w oder|strong="G2228"\w* ein \w Engel|strong="G0032"\w* \w vom|strong="G1537"\w* \w Himmel|strong="G3772"\w* \w euch|strong="G5213"\w* würde Evangelium \w predigen|strong="G2097"\w* \w anders|strong="G3844"\w*, \w denn|strong="G3739"\w* das wir \w euch|strong="G5213"\w* gepredigt \w haben|strong="G2097"\w*, der \w sei|strong="G2077"\w* \w verflucht|strong="G0331"\w*!  
\v 9  \w Wie|strong="G5613"\w* \w wir|strong="G4280"\w* jetzt gesagt \w haben|strong="G4280"\w*, \w so|strong="G0737"\w* sagen \w wir|strong="G3004"\w* \w abermals|strong="G3825"\w*: \w So|strong="G1536"\w* \w jemand|strong="G5209"\w* euch \w Evangelium|strong="G2097"\w* \w predigt|strong="G2097"\w* \w anders|strong="G3844"\w*, \w denn|strong="G3739"\w* das \w ihr|strong="G3880"\w* empfangen \w habt|strong="G3880"\w*, der \w sei|strong="G2077"\w* \w verflucht|strong="G0331"\w*!  
\v 10  \w Predige|strong="G3982"\w* \w ich|strong="G3982"\w* \w denn|strong="G1063"\w* \w jetzt|strong="G0737"\w* \w Menschen|strong="G0444"\w* \w oder|strong="G2228"\w* \w Gott|strong="G2316"\w* zu Dienst? \w Oder|strong="G2228"\w* gedenke \w ich|strong="G2212"\w*, \w Menschen|strong="G0444"\w* gefällig zu \w sein|strong="G0700"\w*? \w Wenn|strong="G1063"\w* \w ich|strong="G2089"\w* den \w Menschen|strong="G0444"\w* \w noch|strong="G1487"\w* \w gefällig|strong="G0700"\w* \w wäre|strong="G0700"\w*, \w so|strong="G0302"\w* wäre \w ich|strong="G2252"\w* \w Christi|strong="G5547"\w* \w Knecht|strong="G1401"\w* \w nicht|strong="G3756"\w*.  
\v 11 Ich tue \w euch|strong="G5213"\w* \w aber|strong="G1161"\w* \w kund|strong="G1107"\w*, liebe \w Brüder|strong="G0080"\w*, \w daß|strong="G3754"\w* das \w Evangelium|strong="G2098"\w*, \w das|strong="G3588"\w* \w von|strong="G5259"\w* \w mir|strong="G1700"\w* \w gepredigt|strong="G2097"\w* \w ist|strong="G2097"\w*, \w nicht|strong="G3756"\w* \w menschlich|strong="G0444"\w* \w ist|strong="G2076"\w*.  
\v 12  \w Denn|strong="G1063"\w* \w ich|strong="G1473"\w* habe \w es|strong="G0846"\w* \w von|strong="G3844"\w* \w keinem|strong="G3761"\w* \w Menschen|strong="G0444"\w* \w empfangen|strong="G3880"\w* \w noch|strong="G3777"\w* \w gelernt|strong="G1321"\w*, \w sondern|strong="G0235"\w* \w durch|strong="G1223"\w* die \w Offenbarung|strong="G0602"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w*.  
\v 13  \w Denn|strong="G1063"\w* \w ihr|strong="G1699"\w* habt ja wohl \w gehört|strong="G0191"\w* \w meinen|strong="G1699"\w* \w Wandel|strong="G0391"\w* \w weiland|strong="G4218"\w* \w im|strong="G1722"\w* \w Judentum|strong="G2454"\w*, \w wie|strong="G3754"\w* \w ich|strong="G2596"\w* \w über|strong="G5236"\w* die \w Maßen|strong="G0846"\w* die \w Gemeinde|strong="G1577"\w* \w Gottes|strong="G2316"\w* \w verfolgte|strong="G1377"\w* \w und|strong="G2532"\w* \w verstörte|strong="G4199"\w*  
\v 14  \w und|strong="G2532"\w* \w nahm|strong="G4298"\w* \w zu|strong="G4298"\w* \w im|strong="G1722"\w* \w Judentum|strong="G2454"\w* \w über|strong="G5228"\w* \w viele|strong="G4183"\w* \w meinesgleichen|strong="G4915"\w* \w in|strong="G1722"\w* \w meinem|strong="G3450"\w* \w Geschlecht|strong="G1085"\w* und \w eiferte|strong="G2207"\w* \w über|strong="G5225"\w* die \w Maßen|strong="G4056"\w* \w um|strong="G3450"\w* das \w väterliche|strong="G3967"\w* \w Gesetz|strong="G3862"\w*.  
\v 15  \w Da|strong="G3753"\w* es \w aber|strong="G1161"\w* \w Gott|strong="G2316"\w* wohl \w gefiel|strong="G2106"\w*, \w der|strong="G3588"\w* \w mich|strong="G3165"\w* \w von|strong="G1537"\w* \w meiner|strong="G3450"\w* \w Mutter|strong="G3384"\w* \w Leibe|strong="G2836"\w* an hat \w ausgesondert|strong="G0873"\w* \w und|strong="G2532"\w* \w berufen|strong="G2564"\w* \w durch|strong="G1223"\w* \w seine|strong="G0846"\w* \w Gnade|strong="G5485"\w*,  
\v 16 daß er \w seinen|strong="G0846"\w* \w Sohn|strong="G5207"\w* \w offenbarte|strong="G0601"\w* \w in|strong="G1722"\w* \w mir|strong="G1698"\w*, \w daß|strong="G2443"\w* ich \w ihn|strong="G0846"\w* durchs Evangelium \w verkündigen|strong="G2097"\w* \w sollte|strong="G2097"\w* \w unter|strong="G1722"\w* den \w Heiden|strong="G1484"\w*: \w alsobald|strong="G2112"\w* fuhr \w ich|strong="G2112"\w* zu und \w besprach|strong="G4323"\w* \w mich|strong="G4323"\w* \w nicht|strong="G3756"\w* darüber mit \w Fleisch|strong="G4561"\w* \w und|strong="G2532"\w* \w Blut|strong="G0129"\w*,  
\v 17 kam \w auch|strong="G0424"\w* \w nicht|strong="G3761"\w* \w gen|strong="G1519"\w* \w Jerusalem|strong="G2414"\w* \w zu|strong="G4314"\w* denen, die \w vor|strong="G4253"\w* \w mir|strong="G1700"\w* \w Apostel|strong="G0652"\w* \w waren|strong="G0235"\w*, sondern zog \w hin|strong="G0565"\w* \w nach|strong="G1519"\w* \w Arabien|strong="G0688"\w* \w und|strong="G2532"\w* \w kam|strong="G5290"\w* \w wiederum|strong="G3825"\w* \w gen|strong="G1519"\w* \w Damaskus|strong="G1154"\w*.  
\v 18  \w Darnach|strong="G1899"\w* \w über|strong="G1519"\w* \w drei|strong="G5140"\w* \w Jahre|strong="G2094"\w* \w kam|strong="G0424"\w* \w ich|strong="G0424"\w* \w nach|strong="G3326"\w* \w Jerusalem|strong="G2414"\w*, \w Petrus|strong="G4074"\w* zu \w schauen|strong="G2477"\w*, \w und|strong="G2532"\w* \w blieb|strong="G1961"\w* \w fünfzehn|strong="G1178"\w* \w Tage|strong="G2250"\w* \w bei|strong="G4314"\w* \w ihm|strong="G0846"\w*.  
\v 19 Der \w andern|strong="G2087"\w* \w Apostel|strong="G0652"\w* \w aber|strong="G1161"\w* \w sah|strong="G1492"\w* ich \w keinen|strong="G3756"\w* \w außer|strong="G1508"\w* \w Jakobus|strong="G2385"\w*, des \w HERRN|strong="G0080"\w* \w Bruder|strong="G2962"\w*.  
\v 20  \w Was|strong="G3739"\w* \w ich|strong="G5213"\w* \w euch|strong="G5213"\w* \w aber|strong="G1161"\w* \w schreibe|strong="G1125"\w*, \w siehe|strong="G2400"\w*, \w Gott|strong="G2316"\w* weiß, \w ich|strong="G1799"\w* \w lüge|strong="G5574"\w* \w nicht|strong="G3756"\w*!  
\v 21  \w Darnach|strong="G1899"\w* kam \w ich|strong="G2064"\w* \w in|strong="G1519"\w* die \w Länder|strong="G2824"\w* \w Syrien|strong="G4947"\w* \w und|strong="G2532"\w* \w Zilizien|strong="G2791"\w*.  
\v 22 Ich \w war|strong="G2252"\w* \w aber|strong="G1161"\w* \w unbekannt|strong="G0050"\w* von \w Angesicht|strong="G4383"\w* \w den|strong="G3588"\w* \w christlichen|strong="G5547"\w* \w Gemeinden|strong="G1577"\w* in \w Judäa|strong="G2449"\w*.  
\v 23 Sie \w hatten|strong="G2258"\w* \w aber|strong="G1161"\w* \w allein|strong="G3440"\w* \w gehört|strong="G0191"\w*, \w daß|strong="G3754"\w*, der \w uns|strong="G2248"\w* \w weiland|strong="G4218"\w* \w verfolgte|strong="G1377"\w*, der \w predigt|strong="G2097"\w* \w jetzt|strong="G3568"\w* den \w Glauben|strong="G4102"\w*, \w welchen|strong="G3739"\w* er \w weiland|strong="G4218"\w* \w verstörte|strong="G4199"\w*,  
