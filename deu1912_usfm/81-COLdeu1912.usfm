\v 1  \w Paulus|strong="G3972"\w*, ein \w Apostel|strong="G0652"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w* \w durch|strong="G1223"\w* den \w Willen|strong="G2307"\w* \w Gottes|strong="G2316"\w*, \w und|strong="G2532"\w* \w Bruder|strong="G0080"\w* \w Timotheus|strong="G5095"\w*  
\v 2 den \w Heiligen|strong="G0040"\w* \w zu|strong="G1722"\w* \w Kolossä|strong="G2857"\w* \w und|strong="G2532"\w* den \w gläubigen|strong="G4103"\w* \w Brüdern|strong="G0080"\w* \w in|strong="G1722"\w* \w Christo|strong="G5547"\w*: \w Gnade|strong="G5485"\w* \w sei|strong="G5213"\w* mit euch \w und|strong="G2532"\w* \w Friede|strong="G1515"\w* \w von|strong="G0575"\w* \w Gott|strong="G2316"\w*, \w unserm|strong="G2257"\w* \w Vater|strong="G3962"\w*, \w und|strong="G2532"\w* dem \w HERRN|strong="G2962"\w* \w Jesus|strong="G2424"\w* \w Christus|strong="G5547"\w*!  
\v 3 Wir \w danken|strong="G2168"\w* \w Gott|strong="G2316"\w* \w und|strong="G2532"\w* dem \w Vater|strong="G3962"\w* \w unsers|strong="G2257"\w* \w HERRN|strong="G2962"\w* \w Jesu|strong="G2424"\w* \w Christi|strong="G5547"\w* und \w beten|strong="G4336"\w* \w allezeit|strong="G3842"\w* \w für|strong="G4012"\w* \w euch|strong="G5216"\w*,  
