\v 5  \w um|strong="G1223"\w* der \w Hoffnung|strong="G1680"\w* willen, \w die|strong="G3588"\w* \w euch|strong="G5213"\w* beigelegt \w ist|strong="G0606"\w* \w im|strong="G1722"\w* \w Himmel|strong="G3772"\w*, von \w welcher|strong="G3739"\w* ihr zuvor gehört \w habt|strong="G4257"\w* \w durch|strong="G1722"\w* das \w Wort|strong="G3056"\w* der \w Wahrheit|strong="G0225"\w* im \w Evangelium|strong="G2098"\w*,  
\v 6  \w das|strong="G3588"\w* \w zu|strong="G1519"\w* \w euch|strong="G5209"\w* gekommen \w ist|strong="G3918"\w*, \w wie|strong="G2531"\w* \w auch|strong="G2532"\w* \w in|strong="G1722"\w* \w alle|strong="G3956"\w* \w Welt|strong="G2889"\w*, \w und|strong="G2532"\w* ist \w fruchtbar|strong="G2076"\w*, \w wie|strong="G2531"\w* \w auch|strong="G2532"\w* \w in|strong="G1722"\w* \w euch|strong="G5213"\w*, \w von|strong="G0575"\w* \w dem|strong="G3739"\w* \w Tage|strong="G2250"\w* an, da ihr's gehört \w habt|strong="G0191"\w* \w und|strong="G2532"\w* \w erkannt|strong="G1921"\w* die \w Gnade|strong="G5485"\w* \w Gottes|strong="G2316"\w* \w in|strong="G1722"\w* der \w Wahrheit|strong="G0225"\w*;  
\v 7  \w wie|strong="G2531"\w* \w ihr|strong="G2532"\w* denn gelernt \w habt|strong="G3129"\w* \w von|strong="G0575"\w* \w Epaphras|strong="G1889"\w*, \w unserm|strong="G2257"\w* \w lieben|strong="G0027"\w* \w Mitdiener|strong="G4889"\w*, \w welcher|strong="G3739"\w* \w ist|strong="G2076"\w* ein \w treuer|strong="G4103"\w* \w Diener|strong="G1249"\w* \w Christi|strong="G5547"\w* \w für|strong="G5228"\w* \w euch|strong="G5216"\w*,  
\v 8  \w der|strong="G3588"\w* \w uns|strong="G2254"\w* \w auch|strong="G2532"\w* eröffnet \w hat|strong="G1213"\w* \w eure|strong="G5216"\w* \w Liebe|strong="G0026"\w* \w im|strong="G1722"\w* \w Geist|strong="G4151"\w*.  
\v 9  \w Derhalben|strong="G1223"\w* \w auch|strong="G2532"\w* \w wir|strong="G2249"\w* \w von|strong="G0575"\w* dem \w Tage|strong="G2250"\w* an, da wir's gehört \w haben|strong="G0191"\w*, \w hören|strong="G3973"\w* wir \w nicht|strong="G3756"\w* \w auf|strong="G3973"\w*, \w für|strong="G5228"\w* \w euch|strong="G5216"\w* zu \w beten|strong="G4336"\w* \w und|strong="G2532"\w* zu \w bitten|strong="G0154"\w*, \w daß|strong="G2443"\w* ihr erfüllt \w werdet|strong="G4137"\w* mit \w Erkenntnis|strong="G1922"\w* \w seines|strong="G0846"\w* \w Willens|strong="G2307"\w* \w in|strong="G1722"\w* \w allerlei|strong="G3956"\w* \w geistlicher|strong="G4152"\w* \w Weisheit|strong="G4678"\w* \w und|strong="G2532"\w* \w Verständnis|strong="G4907"\w*,  
\v 10 daß \w ihr|strong="G5209"\w* \w wandelt|strong="G4043"\w* \w würdig|strong="G0516"\w* dem \w HERRN|strong="G2962"\w* \w zu|strong="G1519"\w* \w allem|strong="G3956"\w* \w Gefallen|strong="G0699"\w* und \w fruchtbar|strong="G2592"\w* seid \w in|strong="G1722"\w* \w allen|strong="G3956"\w* \w guten|strong="G0018"\w* \w Werken|strong="G2041"\w*  
\v 11  \w und|strong="G2532"\w* \w wachset|strong="G0837"\w* \w in|strong="G1519"\w* der \w Erkenntnis|strong="G1922"\w* \w Gottes|strong="G2316"\w* und gestärkt werdet \w mit|strong="G1722"\w* \w aller|strong="G3956"\w* \w Kraft|strong="G1411"\w* \w nach|strong="G2596"\w* \w seiner|strong="G0846"\w* \w herrlichen|strong="G1391"\w* \w Macht|strong="G2904"\w* \w zu|strong="G1519"\w* \w aller|strong="G3956"\w* \w Geduld|strong="G5281"\w* \w und|strong="G2532"\w* \w Langmütigkeit|strong="G3115"\w* \w mit|strong="G3326"\w* \w Freuden|strong="G5479"\w*,  
\v 12 und \w danksaget|strong="G2168"\w* dem \w Vater|strong="G3962"\w*, \w der|strong="G3588"\w* \w uns|strong="G2248"\w* tüchtig gemacht \w hat|strong="G2427"\w* \w zu|strong="G1519"\w* dem \w Erbteil|strong="G3310"\w* der \w Heiligen|strong="G0040"\w* \w im|strong="G1722"\w* \w Licht|strong="G5457"\w*;  
\v 13  \w welcher|strong="G3739"\w* \w uns|strong="G2248"\w* errettet \w hat|strong="G4506"\w* \w von|strong="G1537"\w* der \w Obrigkeit|strong="G1849"\w* der \w Finsternis|strong="G4655"\w* \w und|strong="G2532"\w* hat uns \w versetzt|strong="G3179"\w* \w in|strong="G1519"\w* das \w Reich|strong="G0932"\w* \w seines|strong="G0846"\w* \w lieben|strong="G0026"\w* \w Sohnes|strong="G5207"\w*,  
\v 14  \w an|strong="G1722"\w* \w welchem|strong="G3739"\w* wir \w haben|strong="G2192"\w* die \w Erlösung|strong="G0629"\w* \w durch|strong="G1223"\w* \w sein|strong="G0846"\w* \w Blut|strong="G0129"\w*, die \w Vergebung|strong="G0859"\w* der \w Sünden|strong="G0266"\w*;  
\v 15  \w welcher|strong="G3739"\w* \w ist|strong="G2076"\w* das \w Ebenbild|strong="G1504"\w* des \w unsichtbaren|strong="G0517"\w* \w Gottes|strong="G2316"\w*, der \w Erstgeborene|strong="G4416"\w* vor \w allen|strong="G3956"\w* \w Kreaturen|strong="G2937"\w*.  
\v 16  \w Denn|strong="G3754"\w* \w durch|strong="G1722"\w* \w ihn|strong="G0846"\w* ist \w alles|strong="G3956"\w* \w geschaffen|strong="G2936"\w*, was \w im|strong="G1722"\w* \w Himmel|strong="G3772"\w* \w und|strong="G2532"\w* \w auf|strong="G1909"\w* \w Erden|strong="G1093"\w* ist, das \w Sichtbare|strong="G3707"\w* \w und|strong="G2532"\w* das \w Unsichtbare|strong="G0517"\w*, es \w seien|strong="G1535"\w* \w Throne|strong="G2362"\w* \w oder|strong="G1535"\w* \w Herrschaften|strong="G2963"\w* \w oder|strong="G1535"\w* \w Fürstentümer|strong="G0746"\w* \w oder|strong="G1535"\w* \w Obrigkeiten|strong="G1849"\w*; es \w ist|strong="G1223"\w* \w alles|strong="G3956"\w* \w durch|strong="G1223"\w* \w ihn|strong="G0846"\w* \w und|strong="G2532"\w* \w zu|strong="G1519"\w* \w ihm|strong="G0846"\w* \w geschaffen|strong="G2936"\w*.  
\v 17  \w Und|strong="G2532"\w* er \w ist|strong="G2076"\w* \w vor|strong="G4253"\w* \w allem|strong="G3956"\w*, \w und|strong="G2532"\w* es \w besteht|strong="G4921"\w* \w alles|strong="G3956"\w* \w in|strong="G1722"\w* \w ihm|strong="G0846"\w*.  
\v 18  \w Und|strong="G2532"\w* er \w ist|strong="G2076"\w* das \w Haupt|strong="G2776"\w* des \w Leibes|strong="G4983"\w*, nämlich der \w Gemeinde|strong="G1577"\w*; \w er|strong="G3739"\w*, welcher \w ist|strong="G2076"\w* der \w Anfang|strong="G0746"\w* und der \w Erstgeborene|strong="G4416"\w* \w von|strong="G1537"\w* den \w Toten|strong="G3498"\w*, auf \w daß|strong="G2443"\w* er \w in|strong="G1722"\w* \w allen|strong="G3956"\w* Dingen den \w Vorrang|strong="G4409"\w* \w habe|strong="G1096"\w*.  
\v 19  \w Denn|strong="G3754"\w* es ist das Wohlgefallen \w gewesen|strong="G2106"\w*, daß \w in|strong="G1722"\w* \w ihm|strong="G0846"\w* \w alle|strong="G3956"\w* \w Fülle|strong="G4138"\w* wohnen \w sollte|strong="G2730"\w*  
\v 20  \w und|strong="G2532"\w* alles \w durch|strong="G1223"\w* \w ihn|strong="G0846"\w* versöhnt \w würde|strong="G1517"\w* \w zu|strong="G1519"\w* ihm \w selbst|strong="G0846"\w*, \w es|strong="G1535"\w* \w sei|strong="G3956"\w* \w auf|strong="G1909"\w* \w Erden|strong="G1093"\w* \w oder|strong="G1535"\w* \w im|strong="G1722"\w* \w Himmel|strong="G3772"\w*, damit daß er Frieden \w machte|strong="G0604"\w* \w durch|strong="G1223"\w* das \w Blut|strong="G0129"\w* an \w seinem|strong="G0846"\w* \w Kreuz|strong="G4716"\w*, \w durch|strong="G1223"\w* sich \w selbst|strong="G0846"\w*.  
\v 21  \w Und|strong="G2532"\w* \w euch|strong="G5209"\w*, die ihr \w weiland|strong="G4218"\w* \w Fremde|strong="G0526"\w* \w und|strong="G2532"\w* \w Feinde|strong="G2190"\w* \w waret|strong="G5607"\w* durch die \w Vernunft|strong="G1271"\w* \w in|strong="G1722"\w* \w bösen|strong="G4190"\w* \w Werken|strong="G2041"\w*,  
\v 22 hat er \w nun|strong="G3570"\w* \w versöhnt|strong="G0604"\w* \w mit|strong="G1722"\w* dem \w Leibe|strong="G4983"\w* \w seines|strong="G0846"\w* \w Fleisches|strong="G4561"\w* \w durch|strong="G1223"\w* den \w Tod|strong="G2288"\w*, auf daß er \w euch|strong="G5209"\w* \w darstellte|strong="G3936"\w* \w heilig|strong="G0040"\w* \w und|strong="G2532"\w* \w unsträflich|strong="G0299"\w* \w und|strong="G2532"\w* ohne \w Tadel|strong="G0410"\w* \w vor|strong="G2714"\w* ihm \w selbst|strong="G0846"\w*;  
\v 23  \w so|strong="G1487"\w* \w ihr|strong="G1065"\w* anders \w bleibet|strong="G1961"\w* im \w Glauben|strong="G4102"\w*, \w gegründet|strong="G2311"\w* \w und|strong="G2532"\w* \w fest|strong="G1476"\w* \w und|strong="G2532"\w* \w unbeweglich|strong="G3361"\w* \w von|strong="G0575"\w* der \w Hoffnung|strong="G1680"\w* des \w Evangeliums|strong="G2098"\w*, \w welches|strong="G3739"\w* ihr gehört \w habt|strong="G0191"\w*, \w welches|strong="G3588"\w* gepredigt \w ist|strong="G2784"\w* \w unter|strong="G1722"\w* \w aller|strong="G3956"\w* \w Kreatur|strong="G2937"\w*, die \w unter|strong="G5259"\w* dem \w Himmel|strong="G3772"\w* ist, \w dessen|strong="G3739"\w* \w Diener|strong="G1249"\w* \w ich|strong="G1473"\w*, \w Paulus|strong="G3972"\w*, geworden \w bin|strong="G1096"\w*.  
\v 24  \w Nun|strong="G3739"\w* \w freue|strong="G5463"\w* ich \w mich|strong="G3568"\w* \w in|strong="G1722"\w* \w meinem|strong="G3450"\w* \w Leiden|strong="G3804"\w*, das ich \w für|strong="G5228"\w* \w euch|strong="G5216"\w* leide, \w und|strong="G2532"\w* \w erstatte|strong="G0466"\w* \w an|strong="G1722"\w* \w meinem|strong="G3450"\w* \w Fleisch|strong="G4561"\w*, \w was|strong="G5303"\w* noch \w mangelt|strong="G5303"\w* an \w Trübsalen|strong="G2347"\w* in \w Christo|strong="G5547"\w*, \w für|strong="G5228"\w* \w seinen|strong="G0846"\w* \w Leib|strong="G4983"\w*, \w welcher|strong="G3603"\w* \w ist|strong="G2076"\w* die \w Gemeinde|strong="G1577"\w*,  
\v 25  \w deren|strong="G3739"\w* \w Diener|strong="G1249"\w* \w ich|strong="G1473"\w* geworden \w bin|strong="G1096"\w* \w nach|strong="G2596"\w* dem \w göttlichen|strong="G2316"\w* \w Predigtamt|strong="G3622"\w*, \w das|strong="G3588"\w* \w mir|strong="G3427"\w* gegeben \w ist|strong="G1325"\w* \w unter|strong="G1519"\w* \w euch|strong="G5209"\w*, daß ich das \w Wort|strong="G3056"\w* \w Gottes|strong="G2316"\w* reichlich \w predigen|strong="G4137"\w* \w soll|strong="G4137"\w*,  
\v 26 nämlich das \w Geheimnis|strong="G3466"\w*, \w das|strong="G3588"\w* verborgen gewesen \w ist|strong="G0613"\w* \w von|strong="G0575"\w* der \w Welt|strong="G0165"\w* her \w und|strong="G2532"\w* \w von|strong="G0575"\w* den \w Zeiten|strong="G1074"\w* her, \w nun|strong="G3570"\w* \w aber|strong="G1161"\w* ist es \w offenbart|strong="G5319"\w* \w seinen|strong="G0846"\w* \w Heiligen|strong="G0040"\w*,  
\v 27  \w denen|strong="G3739"\w* \w Gott|strong="G2316"\w* gewollt \w hat|strong="G2309"\w* \w kundtun|strong="G1107"\w*, \w welcher|strong="G5101"\w* da sei der \w herrliche|strong="G1391"\w* \w Reichtum|strong="G4149"\w* \w dieses|strong="G5127"\w* \w Geheimnisses|strong="G3466"\w* \w unter|strong="G1722"\w* den \w Heiden|strong="G1484"\w*, \w welches|strong="G3739"\w* \w ist|strong="G2076"\w* \w Christus|strong="G5547"\w* \w in|strong="G1722"\w* \w euch|strong="G5213"\w*, der da ist die \w Hoffnung|strong="G1680"\w* der \w Herrlichkeit|strong="G1391"\w*.  
\v 28  \w Den|strong="G3739"\w* \w verkündigen|strong="G2605"\w* \w wir|strong="G2249"\w* und \w vermahnen|strong="G3560"\w* \w alle|strong="G3956"\w* \w Menschen|strong="G0444"\w* \w und|strong="G2532"\w* \w lehren|strong="G1321"\w* \w alle|strong="G3956"\w* \w Menschen|strong="G0444"\w* \w mit|strong="G1722"\w* \w aller|strong="G3956"\w* \w Weisheit|strong="G4678"\w*, auf \w daß|strong="G2443"\w* wir \w darstellen|strong="G3936"\w* einen \w jeglichen|strong="G3956"\w* \w Menschen|strong="G0444"\w* \w vollkommen|strong="G5046"\w* \w in|strong="G1722"\w* \w Christo|strong="G5547"\w* \w Jesu|strong="G2424"\w*;  
\v 29  \w daran|strong="G1519"\w* ich \w auch|strong="G2532"\w* \w arbeite|strong="G2872"\w* und \w ringe|strong="G0075"\w*, \w nach|strong="G2596"\w* \w der|strong="G0846"\w* \w Wirkung|strong="G1753"\w* des, \w der|strong="G3588"\w* \w in|strong="G1722"\w* \w mir|strong="G1698"\w* \w kräftig|strong="G1411"\w* \w wirkt|strong="G1754"\w*.   
\c 2  
\p
\v 1 Ich \w lasse|strong="G2309"\w* \w euch|strong="G5209"\w* \w aber|strong="G1063"\w* \w wissen|strong="G1492"\w*, \w welch|strong="G2245"\w* einen \w Kampf|strong="G0073"\w* \w ich|strong="G4012"\w* \w habe|strong="G2192"\w* um \w euch|strong="G5216"\w* \w und|strong="G2532"\w* um die \w zu|strong="G1722"\w* \w Laodizea|strong="G2993"\w* \w und|strong="G2532"\w* alle, \w die|strong="G3745"\w* \w meine|strong="G3450"\w* \w Person|strong="G4383"\w* \w im|strong="G1722"\w* \w Fleisch|strong="G4561"\w* \w nicht|strong="G3756"\w* gesehen \w haben|strong="G3708"\w*,  
\v 2 auf \w daß|strong="G2443"\w* \w ihre|strong="G0846"\w* \w Herzen|strong="G2588"\w* \w ermahnt|strong="G3870"\w* und zusammengefaßt \w werden|strong="G4822"\w* \w in|strong="G1722"\w* der \w Liebe|strong="G0026"\w* \w und|strong="G2532"\w* \w zu|strong="G1519"\w* \w allem|strong="G3956"\w* \w Reichtum|strong="G4149"\w* des \w gewissen|strong="G4136"\w* \w Verständnisses|strong="G4907"\w*, \w zu|strong="G1519"\w* \w erkennen|strong="G1922"\w* das \w Geheimnis|strong="G3466"\w* \w Gottes|strong="G2316"\w*, \w des|strong="G2532"\w* \w Vaters|strong="G3962"\w* \w und|strong="G2532"\w* \w Christi|strong="G5547"\w*,  
\v 3  \w in|strong="G1722"\w* \w welchem|strong="G3739"\w* \w verborgen|strong="G0614"\w* \w liegen|strong="G1526"\w* \w alle|strong="G3956"\w* \w Schätze|strong="G2344"\w* der \w Weisheit|strong="G4678"\w* \w und|strong="G2532"\w* der \w Erkenntnis|strong="G1108"\w*.  
\v 4 Ich \w sage|strong="G3004"\w* \w aber|strong="G1161"\w* \w davon|strong="G5124"\w*, auf \w daß|strong="G3363"\w* \w euch|strong="G5209"\w* \w niemand|strong="G5100"\w* \w betrüge|strong="G3884"\w* \w mit|strong="G1722"\w* unvernünftigen \w Reden|strong="G4086"\w*.  
\v 5  \w Denn|strong="G1063"\w* \w ob|strong="G1499"\w* ich wohl nach dem \w Fleisch|strong="G4561"\w* nicht da \w bin|strong="G0548"\w*, \w so|strong="G0235"\w* bin \w ich|strong="G1510"\w* doch im \w Geist|strong="G4151"\w* \w bei|strong="G4862"\w* \w euch|strong="G5213"\w*, freue \w mich|strong="G5463"\w* \w und|strong="G2532"\w* \w sehe|strong="G0991"\w* \w eure|strong="G5216"\w* \w Ordnung|strong="G5010"\w* \w und|strong="G2532"\w* \w euren|strong="G5216"\w* \w festen|strong="G4733"\w* \w Glauben|strong="G4102"\w* \w an|strong="G1519"\w* \w Christum|strong="G5547"\w*.  
\v 6  \w Wie|strong="G5613"\w* ihr \w nun|strong="G3767"\w* \w angenommen|strong="G3880"\w* \w habt|strong="G3880"\w* den \w HERRN|strong="G2962"\w* \w Christus|strong="G5547"\w* \w Jesus|strong="G2424"\w*, so \w wandelt|strong="G4043"\w* \w in|strong="G1722"\w* \w ihm|strong="G0846"\w*  
\v 7 und seid \w gewurzelt|strong="G4492"\w* \w und|strong="G2532"\w* \w erbaut|strong="G2026"\w* \w in|strong="G1722"\w* \w ihm|strong="G0846"\w* \w und|strong="G2532"\w* \w fest|strong="G0950"\w* \w im|strong="G1722"\w* \w Glauben|strong="G4102"\w*, \w wie|strong="G2531"\w* ihr gelehrt \w seid|strong="G1321"\w*, und \w seid|strong="G4052"\w* \w in|strong="G1722"\w* \w demselben|strong="G0846"\w* \w reichlich|strong="G1722"\w* \w dankbar|strong="G2169"\w*.  
