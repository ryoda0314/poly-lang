\v 1  \w Nach|strong="H0310"\w* dem \w Tode|strong="H4194"\w* Mose'\w s|strong="H4872"\w*, des \w Knechts|strong="H5650"\w* des \w HERRN|strong="H3068"\w*, \w sprach|strong="H0559"\w* der \w HERR|strong="H3068"\w* \w zu|strong="H0413"\w* \w Josua|strong="H3091"\w*, dem \w Sohn|strong="H1121"\w* \w Nuns|strong="H5126"\w*, Mose'\w s|strong="H4872"\w* \w Diener|strong="H8334"\w*:  
\v 2 Mein \w Knecht|strong="H5650"\w* \w Mose|strong="H4872"\w* ist \w gestorben|strong="H4191"\w*; so \w mache|strong="H6965"\w* \w dich|strong="H0859"\w* nun \w auf|strong="H6965"\w* und zieh \w über|strong="H5674"\w* \w den|strong="H2088"\w* \w Jordan|strong="H3383"\w*, \w du|strong="H0859"\w* \w und|strong="H3605"\w* \w dies|strong="H2088"\w* \w ganze|strong="H3605"\w* \w Volk|strong="H5971"\w*, \w in|strong="H0413"\w* das \w Land|strong="H0776"\w*, \w das|strong="H0834"\w* \w ich|strong="H0595"\w* ihnen, den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w*, gegeben \w habe|strong="H5414"\w*.  
\v 3  \w Alle|strong="H3605"\w* \w Stätten|strong="H4725"\w*, \w darauf|strong="H0834"\w* eure \w Fußsohlen|strong="H3709"\w* treten \w werden|strong="H1869"\w*, habe ich euch \w gegeben|strong="H5414"\w*, \w wie|strong="H0834"\w* \w ich|strong="H0413"\w* \w Mose|strong="H4872"\w* geredet \w habe|strong="H1696"\w*.  
\v 4  \w Von|strong="H4480"\w* der \w Wüste|strong="H4057"\w* an und \w diesem|strong="H2088"\w* \w Libanon|strong="H3844"\w* bis \w an|strong="H5704"\w* das \w große|strong="H1419"\w* \w Wasser|strong="H5104"\w* \w Euphrat|strong="H6578"\w*, das \w ganze|strong="H3605"\w* \w Land|strong="H0776"\w* der \w Hethiter|strong="H2850"\w*, bis an \w das|strong="H5704"\w* \w große|strong="H1419"\w* \w Meer|strong="H3220"\w* gegen \w Abend|strong="H3996"\w* sollen eure \w Grenzen|strong="H1366"\w* \w sein|strong="H1961"\w*.  
\v 5 Es soll \w dir|strong="H6440"\w* \w niemand|strong="H0376"\w* \w widerstehen|strong="H3320"\w* dein \w Leben|strong="H2416"\w* \w lang|strong="H3117"\w*. \w Wie|strong="H0834"\w* ich \w mit|strong="H5973"\w* \w Mose|strong="H4872"\w* gewesen \w bin|strong="H1961"\w*, also will ich auch \w mit|strong="H5973"\w* dir \w sein|strong="H1961"\w*. Ich will dich \w nicht|strong="H3808"\w* \w verlassen|strong="H5800"\w* noch von \w dir|strong="H3808"\w* \w weichen|strong="H7503"\w*.  
\v 6 Sei \w getrost|strong="H2388"\w* und \w unverzagt|strong="H0553"\w*; \w denn|strong="H3588"\w* \w du|strong="H0859"\w* sollst \w diesem|strong="H2208"\w* \w Volk|strong="H5971"\w* das \w Land|strong="H0776"\w* \w austeilen|strong="H5157"\w*, \w das|strong="H0834"\w* ich ihren \w Vätern|strong="H0001"\w* geschworen \w habe|strong="H7650"\w*, daß ich's ihnen geben \w wollte|strong="H5414"\w*.  
\v 7 Sei nur \w getrost|strong="H2388"\w* \w und|strong="H0553"\w* \w sehr|strong="H3966"\w* \w freudig|strong="H0553"\w*, daß du \w haltest|strong="H8104"\w* und \w tust|strong="H6213"\w* allerdinge \w nach|strong="H3605"\w* dem \w Gesetz|strong="H8451"\w*, \w das|strong="H0834"\w* \w dir|strong="H6680"\w* \w Mose|strong="H4872"\w*, mein \w Knecht|strong="H5650"\w*, geboten \w hat|strong="H6680"\w*. \w Weiche|strong="H5493"\w* \w nicht|strong="H0408"\w* \w davon|strong="H4480"\w*, weder zur \w Rechten|strong="H3225"\w* noch zur \w Linken|strong="H8040"\w*, auf \w daß|strong="H4616"\w* du weise handeln \w mögest|strong="H7919"\w* in \w allem|strong="H3605"\w*, \w was|strong="H0834"\w* du tun \w sollst|strong="H3212"\w*.  
\v 8  \w Und|strong="H5612"\w* \w laß|strong="H4185"\w* dieses \w Buch|strong="H5612"\w* \w des|strong="H2088"\w* \w Gesetzes|strong="H8451"\w* \w nicht|strong="H3808"\w* \w von|strong="H4480"\w* deinem \w Munde|strong="H6310"\w* \w kommen|strong="H4185"\w*, sondern \w betrachte|strong="H1897"\w* es \w Tag|strong="H3119"\w* und \w Nacht|strong="H3915"\w*, auf \w daß|strong="H4616"\w* du \w haltest|strong="H8104"\w* und \w tust|strong="H6213"\w* allerdinge \w nach|strong="H3605"\w* dem, was darin geschrieben \w steht|strong="H3789"\w*. \w Alsdann|strong="H0227"\w* wird es \w dir|strong="H1870"\w* gelingen in allem, was du \w tust|strong="H6743"\w*, \w und|strong="H0227"\w* wirst weise handeln \w können|strong="H7919"\w*.  
\v 9 Siehe, ich habe \w dir|strong="H3808"\w* \w geboten|strong="H6680"\w*, daß du \w getrost|strong="H2388"\w* und freudig \w seist|strong="H0553"\w*. Laß \w dir|strong="H6206"\w* \w nicht|strong="H0408"\w* \w grauen|strong="H6206"\w* und entsetze \w dich|strong="H2865"\w* \w nicht|strong="H0408"\w*; \w denn|strong="H3588"\w* der \w HERR|strong="H3068"\w*, dein \w Gott|strong="H0430"\w*, ist mit dir in \w allem|strong="H3605"\w*, \w was|strong="H0834"\w* du tun \w wirst|strong="H3212"\w*.  
\v 10 Da \w gebot|strong="H6680"\w* \w Josua|strong="H3091"\w* den \w Hauptleuten|strong="H7860"\w* des \w Volks|strong="H5971"\w* und \w sprach|strong="H0559"\w*:  
\v 11  \w Geht|strong="H5674"\w* \w durch|strong="H7130"\w* das \w Lager|strong="H4264"\w* und \w gebietet|strong="H6680"\w* dem \w Volk|strong="H5971"\w* und \w sprecht|strong="H0559"\w*: \w Schafft|strong="H3559"\w* \w euch|strong="H3588"\w* \w Vorrat|strong="H6720"\w*; \w denn|strong="H3588"\w* über \w drei|strong="H7969"\w* \w Tage|strong="H3117"\w* werdet \w ihr|strong="H0859"\w* über \w diesen|strong="H2088"\w* \w Jordan|strong="H3383"\w* \w gehen|strong="H5674"\w*, daß ihr \w hineinkommt|strong="H0935"\w* \w und|strong="H3423"\w* das \w Land|strong="H0776"\w* \w einnehmt|strong="H3423"\w*, \w daß|strong="H0834"\w* \w euch|strong="H5414"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, geben \w wird|strong="H5414"\w*.  
\v 12 Und zu den \w Rubenitern|strong="H7206"\w*, \w Gaditern|strong="H1425"\w* und dem \w halben|strong="H2677"\w* \w Stamm|strong="H7626"\w* \w Manasse|strong="H4519"\w* \w sprach|strong="H0559"\w* \w Josua|strong="H3091"\w*:  
\v 13  \w Gedenket|strong="H2142"\w* an das \w Wort|strong="H1697"\w*, \w das|strong="H0834"\w* \w euch|strong="H0854"\w* \w Mose|strong="H4872"\w*, der \w Knecht|strong="H5650"\w* des \w HERRN|strong="H3068"\w*, \w sagte|strong="H6680"\w* und \w sprach|strong="H0559"\w*: Der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, hat euch zur Ruhe \w gebracht|strong="H5117"\w* \w und|strong="H5414"\w* \w dies|strong="H2063"\w* \w Land|strong="H0776"\w* \w gegeben|strong="H5414"\w*.  
\v 14 Eure \w Weiber|strong="H0802"\w* und \w Kinder|strong="H2945"\w* und \w Vieh|strong="H4735"\w* \w laßt|strong="H3427"\w* in dem \w Lande|strong="H0776"\w* \w bleiben|strong="H3427"\w*, \w das|strong="H0834"\w* \w euch|strong="H5676"\w* \w Mose|strong="H4872"\w* gegeben \w hat|strong="H5414"\w*, \w diesseit|strong="H5676"\w* des \w Jordans|strong="H3383"\w*; ihr \w aber|strong="H0859"\w* \w sollt|strong="H2571"\w* \w vor|strong="H6440"\w* euren \w Brüdern|strong="H0251"\w* her \w ziehen|strong="H5674"\w* \w gerüstet|strong="H2571"\w*, \w was|strong="H3605"\w* \w streitbare|strong="H2428"\w* \w Männer|strong="H1368"\w* \w sind|strong="H3605"\w*, und \w ihnen|strong="H0853"\w* \w helfen|strong="H5826"\w*,  
\v 15 bis \w daß|strong="H5704"\w* der \w HERR|strong="H3068"\w* eure \w Brüder|strong="H0251"\w* auch zur Ruhe \w bringt|strong="H5117"\w* wie euch, daß \w sie|strong="H1992"\w* \w auch|strong="H1571"\w* \w einnehmen|strong="H3423"\w* das \w Land|strong="H0776"\w*, \w das|strong="H0834"\w* \w ihnen|strong="H5414"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, geben \w wird|strong="H5414"\w*. Alsdann sollt ihr wieder \w umkehren|strong="H7725"\w* \w in|strong="H0776"\w* \w euer|strong="H3425"\w* \w Land|strong="H0776"\w*, \w das|strong="H0834"\w* \w euch|strong="H5414"\w* \w Mose|strong="H4872"\w*, der \w Knecht|strong="H3068"\w* des \w HERRN|strong="H5650"\w*, gegeben \w hat|strong="H5414"\w* zu \w besitzen|strong="H3423"\w* \w diesseit|strong="H5676"\w* des \w Jordans|strong="H3383"\w*, \w gegen|strong="H4480"\w* der \w Sonne|strong="H8121"\w* \w Aufgang|strong="H4217"\w*.  
\v 16 Und sie \w antworteten|strong="H6030"\w* \w Josua|strong="H3091"\w* und \w sprachen|strong="H0559"\w*: \w Alles|strong="H3605"\w*, \w was|strong="H0834"\w* du uns geboten \w hast|strong="H6680"\w*, das wollen wir \w tun|strong="H6213"\w*; \w und|strong="H0413"\w* \w wo|strong="H0834"\w* du uns \w hinsendest|strong="H7971"\w*, da wollen wir hin \w gehen|strong="H3212"\w*.  
\v 17 Wie \w wir|strong="H3605"\w* \w Mose|strong="H4872"\w* gehorsam sind \w gewesen|strong="H8085"\w*, \w so|strong="H3651"\w* wollen \w wir|strong="H0413"\w* dir auch gehorsam \w sein|strong="H8085"\w*; \w allein|strong="H7535"\w*, daß der \w HERR|strong="H3068"\w*, dein \w Gott|strong="H0430"\w*, nur \w mit|strong="H5973"\w* dir \w sei|strong="H1961"\w*, \w wie|strong="H0834"\w* er \w mit|strong="H5973"\w* \w Mose|strong="H4872"\w* \w war|strong="H1961"\w*.  
\v 18  \w Wer|strong="H0834"\w* deinem \w Mund|strong="H6310"\w* ungehorsam \w ist|strong="H4784"\w* \w und|strong="H1697"\w* \w nicht|strong="H3808"\w* \w gehorcht|strong="H8085"\w* deinen \w Worten|strong="H1697"\w* in \w allem|strong="H3605"\w*, \w was|strong="H0834"\w* du uns \w gebietest|strong="H6680"\w*, der soll \w sterben|strong="H4191"\w*. \w Sei|strong="H2388"\w* \w nur|strong="H7535"\w* \w getrost|strong="H2388"\w* und \w unverzagt|strong="H0553"\w*!   
\c 2  
\p
\v 1  \w Josua|strong="H3091"\w* aber, der \w Sohn|strong="H1121"\w* \w Nuns|strong="H5126"\w*, hatte \w zwei|strong="H8147"\w* \w Kundschafter|strong="H0582"\w* \w heimlich|strong="H2791"\w* \w ausgesandt|strong="H7971"\w* \w von|strong="H4480"\w* \w Sittim|strong="H7851"\w* und ihnen \w gesagt|strong="H0559"\w*: Geht \w hin|strong="H3212"\w*, \w beseht|strong="H7200"\w* das \w Land|strong="H0776"\w* und \w Jericho|strong="H3405"\w*. Die gingen \w hin|strong="H3212"\w* und \w kamen|strong="H0935"\w* in das \w Haus|strong="H0802"\w* \w einer|strong="H1004"\w* \w Hure|strong="H2181"\w*, die \w hieß|strong="H8034"\w* \w Rahab|strong="H7343"\w*, und kehrten zu ihr \w ein|strong="H7901"\w*.  
\v 2 Da ward dem \w König|strong="H4428"\w* von \w Jericho|strong="H3405"\w* \w gesagt|strong="H0559"\w*: \w Siehe|strong="H2009"\w*, es \w sind|strong="H2008"\w* in dieser \w Nacht|strong="H3915"\w* \w Männer|strong="H0582"\w* \w hereingekommen|strong="H0935"\w* \w von|strong="H4480"\w* den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w*, das \w Land|strong="H0776"\w* zu \w erkunden|strong="H2658"\w*.  
\v 3 Da \w sandte|strong="H7971"\w* der \w König|strong="H4428"\w* zu \w Jericho|strong="H3405"\w* \w zu|strong="H0413"\w* \w Rahab|strong="H7343"\w* und ließ ihr \w sagen|strong="H0559"\w*: \w Gib|strong="H3318"\w* die \w Männer|strong="H0582"\w* heraus, die \w zu|strong="H0413"\w* \w dir|strong="H0935"\w* in dein \w Haus|strong="H1004"\w* gekommen \w sind|strong="H0935"\w*; \w denn|strong="H3588"\w* sie sind \w gekommen|strong="H0935"\w*, das \w ganze|strong="H3605"\w* \w Land|strong="H0776"\w* zu \w erkunden|strong="H2658"\w*.  
\v 4 Aber das \w Weib|strong="H0802"\w* \w verbarg|strong="H6845"\w* die \w zwei|strong="H8147"\w* \w Männer|strong="H0582"\w* und \w sprach|strong="H0559"\w* \w also|strong="H3651"\w*: Es sind ja \w Männer|strong="H0582"\w* \w zu|strong="H0413"\w* mir \w hereingekommen|strong="H0935"\w*; aber ich \w wußte|strong="H3045"\w* \w nicht|strong="H3808"\w*, \w woher|strong="H4480"\w* sie waren.  
\v 5  \w Und|strong="H1961"\w* da man die \w Tore|strong="H8179"\w* wollte \w zuschließen|strong="H5462"\w*, da es \w finster|strong="H2822"\w* war, gingen \w sie|strong="H0582"\w* \w hinaus|strong="H3318"\w*, daß \w ich|strong="H3045"\w* \w nicht|strong="H3808"\w* \w weiß|strong="H3045"\w*, \w wo|strong="H0575"\w* \w sie|strong="H0582"\w* hingegangen \w sind|strong="H1980"\w*. \w Jagt|strong="H7291"\w* ihnen \w eilend|strong="H4118"\w* \w nach|strong="H0310"\w*, \w denn|strong="H3588"\w* ihr werdet sie \w ergreifen|strong="H5381"\w*.  
\v 6 Sie \w aber|strong="H1931"\w* ließ sie auf das \w Dach|strong="H1406"\w* \w steigen|strong="H5927"\w* und verdeckte \w sie|strong="H2934"\w* unter die \w Flachsstengel|strong="H6086"\w*, die sie \w auf|strong="H5921"\w* dem \w Dache|strong="H1406"\w* ausgebreitet \w hatte|strong="H6186"\w*.  
\v 7  \w Aber|strong="H7291"\w* die \w Männer|strong="H0582"\w* \w jagten|strong="H7291"\w* ihnen \w nach|strong="H0310"\w* auf dem \w Wege|strong="H1870"\w* zum \w Jordan|strong="H3383"\w* bis \w an|strong="H5921"\w* die \w Furt|strong="H4569"\w*; und man \w schloß|strong="H5462"\w* das \w Tor|strong="H8179"\w* zu, \w da|strong="H0310"\w* die hinaus \w waren|strong="H3318"\w*, die ihnen \w nachjagten|strong="H7291"\w*.  
\v 8 Und ehe \w denn|strong="H2962"\w* die \w Männer|strong="H1992"\w* sich schlafen \w legten|strong="H7901"\w*, \w stieg|strong="H5927"\w* \w sie|strong="H1931"\w* \w zu|strong="H5921"\w* ihnen hinauf \w auf|strong="H5921"\w* das \w Dach|strong="H1406"\w*  
\v 9 und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* \w ihnen|strong="H0582"\w*: Ich \w weiß|strong="H3045"\w*, \w daß|strong="H3588"\w* der \w HERR|strong="H3068"\w* euch das \w Land|strong="H0776"\w* gegeben \w hat|strong="H5414"\w*; \w denn|strong="H3588"\w* ein \w Schrecken|strong="H0367"\w* \w ist|strong="H5307"\w* \w über|strong="H5921"\w* uns \w gefallen|strong="H5307"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w*, \w und|strong="H3588"\w* \w alle|strong="H3605"\w* \w Einwohner|strong="H3427"\w* des \w Landes|strong="H0776"\w* sind feig \w geworden|strong="H4127"\w*.  
\v 10  \w Denn|strong="H3588"\w* wir haben \w gehört|strong="H8085"\w*, \w wie|strong="H0834"\w* der \w HERR|strong="H3068"\w* \w hat|strong="H3001"\w* das \w Wasser|strong="H4325"\w* im \w Schilfmeer|strong="H5488"\w* \w ausgetrocknet|strong="H3001"\w* \w vor|strong="H4480"\w* euch \w her|strong="H6440"\w*, da ihr \w aus|strong="H4480"\w* \w Ägypten|strong="H4714"\w* \w zoget|strong="H3318"\w*, und \w was|strong="H0834"\w* ihr den \w zwei|strong="H8147"\w* \w Königen|strong="H4428"\w* der \w Amoriter|strong="H0567"\w*, \w Sihon|strong="H5511"\w* und \w Og|strong="H5747"\w*, \w jenseit|strong="H5676"\w* des \w Jordans|strong="H3383"\w* getan \w habt|strong="H6213"\w*, \w wie|strong="H0834"\w* ihr sie verbannt \w habt|strong="H2763"\w*.  
\v 11 Und seit wir solches gehört \w haben|strong="H8085"\w*, \w ist|strong="H4549"\w* unser \w Herz|strong="H3824"\w* \w verzagt|strong="H4549"\w* und \w ist|strong="H6965"\w* \w kein|strong="H3808"\w* \w Mut|strong="H7307"\w* \w mehr|strong="H5750"\w* in \w jemand|strong="H0376"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w*; \w denn|strong="H3588"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, \w ist|strong="H1931"\w* \w Gott|strong="H0430"\w* \w oben|strong="H4480"\w* im \w Himmel|strong="H8064"\w* und \w unten|strong="H4480"\w* \w auf|strong="H5921"\w* \w Erden|strong="H0776"\w*.  
\v 12  \w So|strong="H6258"\w* \w schwört|strong="H7650"\w* mir \w nun|strong="H4994"\w* bei dem \w HERRN|strong="H3068"\w*, daß, \w weil|strong="H3588"\w* \w ich|strong="H2617"\w* \w an|strong="H5973"\w* \w euch|strong="H6213"\w* \w Barmherzigkeit|strong="H2617"\w* getan \w habe|strong="H6213"\w*, \w ihr|strong="H0859"\w* \w auch|strong="H1571"\w* \w an|strong="H5973"\w* meines \w Vaters|strong="H0001"\w* \w Hause|strong="H1004"\w* \w Barmherzigkeit|strong="H2617"\w* \w tut|strong="H6213"\w*; und \w gebt|strong="H5414"\w* mir ein gewisses \w Zeichen|strong="H0571"\w*,  
\v 13 daß \w ihr|strong="H0001"\w* leben \w lasset|strong="H2421"\w* meinen \w Vater|strong="H0001"\w*, meine \w Mutter|strong="H0517"\w*, meine \w Brüder|strong="H0251"\w* und meine \w Schwestern|strong="H0269"\w* und \w alles|strong="H3605"\w*, \w was|strong="H0834"\w* sie haben, \w und|strong="H5315"\w* \w errettet|strong="H5337"\w* unsere \w Seelen|strong="H5315"\w* \w vom|strong="H4480"\w* \w Tode|strong="H4194"\w*.  
\v 14 Die \w Männer|strong="H0582"\w* \w sprachen|strong="H0559"\w* zu \w ihr|strong="H5315"\w*: \w Tun|strong="H6213"\w* \w wir|strong="H5973"\w* nicht \w Barmherzigkeit|strong="H2617"\w* und \w Treue|strong="H0571"\w* \w an|strong="H5973"\w* \w dir|strong="H2617"\w*, \w wenn|strong="H3068"\w* \w uns|strong="H0776"\w* der \w HERR|strong="H3068"\w* das \w Land|strong="H0776"\w* \w gibt|strong="H5414"\w*, so \w soll|strong="H5973"\w* unsere \w Seele|strong="H5315"\w* für euch des Todes \w sein|strong="H4191"\w*, \w sofern|strong="H0518"\w* \w du|strong="H2088"\w* unser \w Geschäft|strong="H1697"\w* \w nicht|strong="H3808"\w* \w verrätst|strong="H5046"\w*.  
\v 15 Da ließ sie \w dieselben|strong="H3381"\w* am \w Seil|strong="H2256"\w* \w durchs|strong="H1157"\w* \w Fenster|strong="H2474"\w* \w hernieder|strong="H3381"\w*; \w denn|strong="H3588"\w* ihr \w Haus|strong="H1004"\w* war an der \w Stadtmauer|strong="H7023"\w*, und \w sie|strong="H1931"\w* \w wohnte|strong="H3427"\w* auch auf der \w Mauer|strong="H2346"\w*.  
\v 16 Und sie \w sprach|strong="H0559"\w* zu ihnen: \w Geht|strong="H3212"\w* auf das \w Gebirge|strong="H2022"\w*, \w daß|strong="H6435"\w* \w euch|strong="H6293"\w* \w nicht|strong="H6435"\w* \w begegnen|strong="H6293"\w*, die euch \w nachjagen|strong="H7291"\w*, und verbergt \w euch|strong="H2247"\w* \w daselbst|strong="H8033"\w* \w drei|strong="H7969"\w* \w Tage|strong="H3117"\w*, bis \w daß|strong="H5704"\w* sie \w wiederkommen|strong="H7725"\w*, die euch \w nachjagen|strong="H7291"\w*; \w darnach|strong="H0310"\w* \w geht|strong="H3212"\w* eure \w Straße|strong="H1870"\w*.  
