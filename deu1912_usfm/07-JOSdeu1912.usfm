\v 1  \w Und|strong="H1961"\w* nach \w langer|strong="H7227"\w* \w Zeit|strong="H3117"\w*, \w da|strong="H0310"\w* der \w HERR|strong="H3068"\w* \w hatte|strong="H5117"\w* \w Israel|strong="H3478"\w* zur Ruhe \w gebracht|strong="H5117"\w* vor \w allen|strong="H3605"\w* ihren \w Feinden|strong="H0341"\w* \w umher|strong="H5439"\w* und \w Josua|strong="H3091"\w* nun \w alt|strong="H2204"\w* und wohl betagt \w war|strong="H0935"\w*,  
\v 2  \w berief|strong="H7121"\w* \w er|strong="H3091"\w* das \w ganze|strong="H3605"\w* \w Israel|strong="H3478"\w*, ihre \w Ältesten|strong="H2205"\w*, \w Häupter|strong="H7218"\w*, \w Richter|strong="H8199"\w* und \w Amtleute|strong="H7860"\w*, und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihnen: \w Ich|strong="H0589"\w* bin \w alt|strong="H2204"\w* und wohl \w betagt|strong="H0935"\w*,  
\v 3 und \w ihr|strong="H0859"\w* habt \w gesehen|strong="H7200"\w* \w alles|strong="H3605"\w*, \w was|strong="H0834"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, getan \w hat|strong="H6213"\w* an \w allen|strong="H3605"\w* \w diesen|strong="H0428"\w* \w Völkern|strong="H1471"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w* her; \w denn|strong="H3588"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, \w hat|strong="H3898"\w* \w selber|strong="H1931"\w* für euch \w gestritten|strong="H3898"\w*.  
\v 4  \w Seht|strong="H7200"\w*, ich habe euch \w diese|strong="H0428"\w* noch \w übrigen|strong="H7604"\w* \w Völker|strong="H1471"\w* durchs Los \w zugeteilt|strong="H5307"\w*, einem jeglichen \w Stamm|strong="H7626"\w* sein \w Erbteil|strong="H5159"\w*, \w vom|strong="H4480"\w* \w Jordan|strong="H3383"\w* an, und \w alle|strong="H3605"\w* \w Völker|strong="H1471"\w*, \w die|strong="H0834"\w* ich ausgerottet \w habe|strong="H3772"\w*, und am \w großen|strong="H1419"\w* \w Meer|strong="H3220"\w* \w gegen|strong="H3996"\w* der \w Sonne|strong="H8121"\w* \w Untergang|strong="H3996"\w*.  
\v 5 Und der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, wird sie \w ausstoßen|strong="H1920"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w* und \w von|strong="H4480"\w* \w euch|strong="H6440"\w* \w vertreiben|strong="H3423"\w*, daß ihr ihr \w Land|strong="H0776"\w* \w einnehmt|strong="H3423"\w*, \w wie|strong="H0834"\w* \w euch|strong="H1696"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, geredet \w hat|strong="H1696"\w*.  
\v 6 So seid \w nun|strong="H2388"\w* \w sehr|strong="H3966"\w* \w getrost|strong="H2388"\w*, daß ihr \w haltet|strong="H8104"\w* und \w tut|strong="H6213"\w* \w alles|strong="H3605"\w*, was geschrieben \w steht|strong="H3789"\w* im \w Gesetzbuch|strong="H5612"\w* Mose'\w s|strong="H4872"\w*, \w daß|strong="H1115"\w* \w ihr|strong="H4480"\w* \w nicht|strong="H1115"\w* \w davon|strong="H4480"\w* \w weicht|strong="H5493"\w*, weder zur \w Rechten|strong="H3225"\w* noch zur \w Linken|strong="H8040"\w*,  
\v 7 auf daß ihr \w nicht|strong="H1115"\w* \w unter|strong="H0428"\w* \w diese|strong="H0428"\w* \w übrigen|strong="H7604"\w* \w Völker|strong="H1471"\w* \w kommt|strong="H0935"\w*, die \w bei|strong="H0854"\w* euch sind, \w und|strong="H2142"\w* \w nicht|strong="H3808"\w* \w gedenkt|strong="H2142"\w* \w noch|strong="H3808"\w* \w schwört|strong="H7650"\w* bei dem \w Namen|strong="H8034"\w* ihrer \w Götter|strong="H0430"\w* noch \w ihnen|strong="H3808"\w* \w dient|strong="H5647"\w* noch \w sie|strong="H3808"\w* \w anbetet|strong="H7812"\w*,  
\v 8  \w sondern|strong="H3588"\w* dem \w HERRN|strong="H3068"\w*, eurem \w Gott|strong="H0430"\w*, \w anhangt|strong="H1692"\w*, \w wie|strong="H0834"\w* ihr \w bis|strong="H5704"\w* auf \w diesen|strong="H2088"\w* \w Tag|strong="H3117"\w* getan \w habt|strong="H6213"\w*.  
\v 9 Der \w HERR|strong="H3068"\w* hat \w vor|strong="H4480"\w* \w euch|strong="H6440"\w* \w vertrieben|strong="H3423"\w* \w große|strong="H1419"\w* und \w mächtige|strong="H6099"\w* \w Völker|strong="H1471"\w*, \w und|strong="H0859"\w* \w niemand|strong="H3808"\w* hat \w euch|strong="H6440"\w* \w widerstanden|strong="H5975"\w* \w bis|strong="H5704"\w* auf \w diesen|strong="H2088"\w* \w Tag|strong="H3117"\w*.  
\v 10  \w Euer|strong="H0259"\w* \w einer|strong="H0376"\w* \w jagt|strong="H7291"\w* \w tausend|strong="H0505"\w*; \w denn|strong="H3588"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, streitet für \w euch|strong="H3898"\w*, \w wie|strong="H0834"\w* er euch geredet \w hat|strong="H1696"\w*.  
\v 11 Darum so \w behütet|strong="H8104"\w* \w aufs|strong="H5315"\w* \w fleißigste|strong="H3966"\w* eure \w Seelen|strong="H5315"\w*, daß ihr dem \w HERRN|strong="H3068"\w*, euren \w Gott|strong="H0430"\w*, \w liebhabt|strong="H0157"\w*.  
\v 12  \w Denn|strong="H3588"\w* \w wo|strong="H0518"\w* ihr \w euch|strong="H7725"\w* \w umwendet|strong="H7725"\w* \w und|strong="H3499"\w* \w diesen|strong="H0428"\w* \w Völkern|strong="H1471"\w* \w anhangt|strong="H1692"\w* und euch mit ihnen \w verheiratet|strong="H2859"\w*, daß ihr unter \w sie|strong="H0935"\w* und \w sie|strong="H1992"\w* unter euch \w kommen|strong="H0935"\w*:  
\v 13 so \w wisset|strong="H3045"\w*, \w daß|strong="H3588"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, \w wird|strong="H3254"\w* nicht \w mehr|strong="H3808"\w* alle \w diese|strong="H0428"\w* \w Völker|strong="H1471"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w* \w vertreiben|strong="H3423"\w*; sondern sie werden \w euch|strong="H1961"\w* zum \w Strick|strong="H6341"\w* und \w Netz|strong="H4170"\w* und zur \w Geißel|strong="H7850"\w* in euren Seiten \w werden|strong="H6654"\w* und zum \w Stachel|strong="H6796"\w* in eure \w Augen|strong="H5869"\w*, bis \w daß|strong="H5704"\w* er euch \w umbringe|strong="H0006"\w* hinweg \w von|strong="H4480"\w* \w dem|strong="H2063"\w* \w guten|strong="H2896"\w* \w Lande|strong="H0127"\w*, \w das|strong="H0834"\w* euch der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, gegeben \w hat|strong="H5414"\w*.  
\v 14  \w Siehe|strong="H2009"\w*, \w ich|strong="H0595"\w* \w gehe|strong="H1980"\w* \w heute|strong="H3117"\w* \w dahin|strong="H1870"\w* wie \w alle|strong="H3605"\w* \w Welt|strong="H0776"\w*; und ihr sollt \w wissen|strong="H3045"\w* von \w ganzem|strong="H3605"\w* \w Herzen|strong="H3824"\w* und von \w ganzer|strong="H3605"\w* \w Seele|strong="H5315"\w*, \w daß|strong="H3588"\w* \w nicht|strong="H3808"\w* \w ein|strong="H0259"\w* \w Wort|strong="H1697"\w* gefehlt \w hat|strong="H5307"\w* \w an|strong="H4480"\w* \w all|strong="H3605"\w* dem \w Guten|strong="H2896"\w*, \w das|strong="H0834"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, euch verheißen \w hat|strong="H1696"\w*. Es ist \w alles|strong="H3605"\w* \w gekommen|strong="H0935"\w* \w und|strong="H4480"\w* \w keins|strong="H3808"\w* \w ausgeblieben|strong="H5307"\w*.  
\v 15  Gleichwie \w nun|strong="H0834"\w* \w alles|strong="H3605"\w* \w Gute|strong="H2896"\w* gekommen \w ist|strong="H0935"\w*, \w das|strong="H0834"\w* der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, \w euch|strong="H0413"\w* verheißen \w hat|strong="H1696"\w*, \w also|strong="H3651"\w* wird der \w HERR|strong="H3068"\w* auch \w über|strong="H5921"\w* euch kommen \w lassen|strong="H0935"\w* \w alles|strong="H3605"\w* \w Böse|strong="H7451"\w*, \w bis|strong="H5704"\w* er euch \w vertilge|strong="H8045"\w* \w von|strong="H4480"\w* \w diesem|strong="H2063"\w* \w guten|strong="H2896"\w* \w Land|strong="H0127"\w*, \w das|strong="H0834"\w* euch der \w HERR|strong="H3068"\w*, euer \w Gott|strong="H0430"\w*, gegeben \w hat|strong="H5414"\w*,  
\v 16 wenn \w ihr|strong="H1285"\w* \w übertretet|strong="H5674"\w* den \w Bund|strong="H1285"\w* des \w HERRN|strong="H3068"\w*, eures \w Gottes|strong="H0430"\w*, \w den|strong="H0834"\w* er \w euch|strong="H0854"\w* geboten \w hat|strong="H6680"\w*, und \w hingeht|strong="H1980"\w* \w und|strong="H5647"\w* \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w* \w dient|strong="H5647"\w* und sie \w anbetet|strong="H7812"\w*, daß der \w Zorn|strong="H0639"\w* des \w HERRN|strong="H3068"\w* über euch \w ergrimmt|strong="H2734"\w* und euch \w bald|strong="H4120"\w* \w umbringt|strong="H0006"\w* hinweg \w von|strong="H4480"\w* dem \w guten|strong="H2896"\w* \w Land|strong="H0776"\w*, \w das|strong="H0834"\w* er euch gegeben \w hat|strong="H5414"\w*.   
\c 24  
\p
\v 1  \w Josua|strong="H3091"\w* \w versammelte|strong="H0622"\w* \w alle|strong="H3605"\w* \w Stämme|strong="H7626"\w* \w Israels|strong="H3478"\w* gen \w Sichem|strong="H7927"\w* und \w berief|strong="H7121"\w* die \w Ältesten|strong="H2205"\w* von \w Israel|strong="H3478"\w*, die \w Häupter|strong="H7218"\w*, \w Richter|strong="H8199"\w* und \w Amtleute|strong="H7860"\w*. Und da \w sie|strong="H3320"\w* \w vor|strong="H6440"\w* \w Gott|strong="H0430"\w* getreten \w waren|strong="H3320"\w*,  
\v 2  \w sprach|strong="H0559"\w* \w er|strong="H3091"\w* \w zum|strong="H0413"\w* \w ganzen|strong="H3605"\w* \w Volk|strong="H5971"\w*: \w So|strong="H3541"\w* \w sagt|strong="H0559"\w* der \w HERR|strong="H3068"\w*, der \w Gott|strong="H0430"\w* \w Israels|strong="H3478"\w*: Eure \w Väter|strong="H0001"\w* \w wohnten|strong="H3427"\w* \w vorzeiten|strong="H5769"\w* \w jenseit|strong="H5676"\w* des \w Stroms|strong="H5104"\w*, \w Tharah|strong="H8646"\w*, \w Abrahams|strong="H0085"\w* \w und|strong="H0001"\w* \w Nahors|strong="H5152"\w* \w Vater|strong="H0001"\w*, \w und|strong="H0312"\w* \w dienten|strong="H5647"\w* \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w*.  
\v 3 Da nahm \w ich|strong="H3947"\w* euren \w Vater|strong="H0001"\w* \w Abraham|strong="H0085"\w* \w jenseit|strong="H5676"\w* des \w Stroms|strong="H5104"\w* und ließ \w ihn|strong="H0853"\w* \w wandern|strong="H3212"\w* im \w ganzen|strong="H3605"\w* \w Land|strong="H0776"\w* \w Kanaan|strong="H3667"\w* und \w mehrte|strong="H7235"\w* ihm seinen \w Samen|strong="H2233"\w* und \w gab|strong="H5414"\w* ihm \w Isaak|strong="H3327"\w*.  
\v 4 Und \w Isaak|strong="H3327"\w* gab \w ich|strong="H5414"\w* \w Jakob|strong="H3290"\w* und \w Esau|strong="H6215"\w* und \w gab|strong="H5414"\w* \w Esau|strong="H6215"\w* das \w Gebirge|strong="H2022"\w* \w Seir|strong="H8165"\w* \w zu|strong="H0853"\w* \w besitzen|strong="H3423"\w*. Jakob \w aber|strong="H3290"\w* und seine \w Kinder|strong="H1121"\w* zogen \w hinab|strong="H3381"\w* nach \w Ägypten|strong="H4714"\w*.  
\v 5 Da sandte \w ich|strong="H7971"\w* \w Mose|strong="H4872"\w* und \w Aaron|strong="H0175"\w* und \w plagte|strong="H5062"\w* \w Ägypten|strong="H4714"\w*, \w wie|strong="H0834"\w* ich unter \w ihnen|strong="H7130"\w* getan \w habe|strong="H6213"\w*.  
\v 6  \w Darnach|strong="H0310"\w* \w führte|strong="H3318"\w* ich euch und eure \w Väter|strong="H0001"\w* \w aus|strong="H4480"\w* \w Ägypten|strong="H4714"\w*. Und da \w ihr|strong="H0935"\w* an das \w Meer|strong="H3220"\w* \w kamt|strong="H0935"\w* und die \w Ägypter|strong="H4714"\w* euren \w Vätern|strong="H0001"\w* \w nachjagten|strong="H7291"\w* mit \w Wagen|strong="H7393"\w* und \w Reitern|strong="H6571"\w* ans \w Schilfmeer|strong="H5488"\w*,  
\v 7 da schrieen \w sie|strong="H6817"\w* \w zum|strong="H0413"\w* \w HERRN|strong="H3068"\w*; der \w setzte|strong="H7760"\w* eine \w Finsternis|strong="H3990"\w* \w zwischen|strong="H0996"\w* euch \w und|strong="H0996"\w* die \w Ägypter|strong="H4713"\w* und \w führte|strong="H0935"\w* das \w Meer|strong="H3220"\w* \w über|strong="H5921"\w* sie, und es \w bedeckte|strong="H3680"\w* sie. Und eure \w Augen|strong="H5869"\w* haben \w gesehen|strong="H7200"\w*, \w was|strong="H0834"\w* ich an den \w Ägyptern|strong="H4714"\w* getan \w habe|strong="H6213"\w*. Und ihr \w habt|strong="H7227"\w* \w gewohnt|strong="H3427"\w* in der \w Wüste|strong="H4057"\w* eine \w lange|strong="H7227"\w* \w Zeit|strong="H3117"\w*.  
\v 8 Und ich \w habe|strong="H0935"\w* \w euch|strong="H0853"\w* \w gebracht|strong="H0935"\w* \w in|strong="H0413"\w* das \w Land|strong="H0776"\w* der \w Amoriter|strong="H0567"\w*, die \w jenseit|strong="H5676"\w* des \w Jordans|strong="H3383"\w* \w wohnten|strong="H3427"\w*; und da \w sie|strong="H3898"\w* \w wider|strong="H0854"\w* \w euch|strong="H5414"\w* \w stritten|strong="H3898"\w*, gab \w ich|strong="H5414"\w* \w sie|strong="H0853"\w* in eure \w Hände|strong="H3027"\w*, daß \w ihr|strong="H3423"\w* ihr \w Land|strong="H0776"\w* \w besaßet|strong="H3423"\w*, und \w vertilgte|strong="H8045"\w* sie vor euch \w her|strong="H6440"\w*.  
\v 9 Da machte sich \w auf|strong="H6965"\w* \w Balak|strong="H1111"\w*, der \w Sohn|strong="H1121"\w* \w Zippors|strong="H6834"\w*, der \w Moabiter|strong="H4124"\w* \w König|strong="H4428"\w*, und \w stritt|strong="H3898"\w* wider \w Israel|strong="H3478"\w* und sandte \w hin|strong="H7971"\w* und \w ließ|strong="H1109"\w* \w rufen|strong="H7121"\w* \w Bileam|strong="H1109"\w*, den \w Sohn|strong="H1121"\w* \w Beors|strong="H1160"\w*, daß er \w euch|strong="H0853"\w* \w verfluchte|strong="H7043"\w*.  
\v 10 Aber ich \w wollte|strong="H0014"\w* \w ihn|strong="H1109"\w* \w nicht|strong="H3808"\w* \w hören|strong="H8085"\w*. Und er \w segnete|strong="H1288"\w* \w euch|strong="H0853"\w* und ich \w errettete|strong="H5337"\w* \w euch|strong="H0853"\w* aus seinen \w Händen|strong="H3027"\w*.  
\v 11 Und da ihr über den \w Jordan|strong="H3383"\w* \w gingt|strong="H5674"\w* und \w gen|strong="H0413"\w* \w Jericho|strong="H3405"\w* \w kamt|strong="H0935"\w*, \w stritten|strong="H3898"\w* wider \w euch|strong="H0567"\w* die \w Bürger|strong="H1167"\w* von \w Jericho|strong="H3405"\w*, die \w Amoriter|strong="H0567"\w*, \w Pheresiter|strong="H6522"\w*, \w Kanaaniter|strong="H3669"\w*, \w Hethiter|strong="H2850"\w*, \w Girgasiter|strong="H1622"\w*, \w Heviter|strong="H2340"\w* und \w Jebusiter|strong="H2983"\w*; aber ich \w gab|strong="H5414"\w* \w sie|strong="H0853"\w* in eure \w Hände|strong="H3027"\w*.  
\v 12 Und \w sandte|strong="H7971"\w* \w Hornissen|strong="H6880"\w* \w vor|strong="H6440"\w* euch her; die \w trieben|strong="H1644"\w* \w sie|strong="H0853"\w* aus \w vor|strong="H4480"\w* euch \w her|strong="H6440"\w*, die \w zwei|strong="H8147"\w* \w Könige|strong="H4428"\w* der \w Amoriter|strong="H0567"\w*, \w nicht|strong="H3808"\w* durch dein \w Schwert|strong="H2719"\w* \w noch|strong="H3808"\w* durch deinen \w Bogen|strong="H7198"\w*.  
\v 13 Und ich habe euch ein \w Land|strong="H0776"\w* \w gegeben|strong="H5414"\w*, \w daran|strong="H0834"\w* ihr \w nicht|strong="H3808"\w* gearbeitet \w habt|strong="H3021"\w*, und \w Städte|strong="H5892"\w*, \w die|strong="H0834"\w* ihr \w nicht|strong="H3808"\w* gebaut \w habt|strong="H1129"\w*, daß \w ihr|strong="H3427"\w* \w darin|strong="H0398"\w* \w wohnt|strong="H3427"\w* und \w eßt|strong="H0398"\w* von \w Weinbergen|strong="H3754"\w* und \w Ölbäumen|strong="H2132"\w*, \w die|strong="H0834"\w* \w ihr|strong="H0859"\w* nicht gepflanzt \w habt|strong="H5193"\w*.  
\v 14  \w So|strong="H6258"\w* \w fürchtet|strong="H3372"\w* \w nun|strong="H6258"\w* den \w HERRN|strong="H3068"\w* und \w dient|strong="H5647"\w* ihm \w treulich|strong="H8549"\w* und \w rechtschaffen|strong="H0571"\w* und laßt \w fahren|strong="H5493"\w* die \w Götter|strong="H0430"\w*, \w denen|strong="H0834"\w* eure \w Väter|strong="H0001"\w* gedient \w haben|strong="H5647"\w* \w jenseit|strong="H5676"\w* des \w Stroms|strong="H5104"\w* und in \w Ägypten|strong="H4714"\w*, und \w dient|strong="H5647"\w* dem \w HERRN|strong="H3068"\w*.  
