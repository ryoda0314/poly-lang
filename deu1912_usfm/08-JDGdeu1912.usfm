\v 1  \w Nach|strong="H0310"\w* dem \w Tod|strong="H4194"\w* \w Josuas|strong="H3091"\w* \w fragten|strong="H7592"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* den \w HERRN|strong="H3068"\w* und \w sprachen|strong="H0559"\w*: \w Wer|strong="H4310"\w* soll unter uns \w zuerst|strong="H8462"\w* \w hinaufziehen|strong="H5927"\w*, Krieg zu führen \w wider|strong="H3898"\w* die \w Kanaaniter|strong="H3669"\w*?  
\v 2 Der \w HERR|strong="H3068"\w* \w sprach|strong="H0559"\w*: \w Juda|strong="H3063"\w* soll \w hinaufziehen|strong="H5927"\w*. \w Siehe|strong="H2009"\w*, ich habe das \w Land|strong="H0776"\w* in seine \w Hand|strong="H3027"\w* \w gegeben|strong="H5414"\w*.  
\v 3 Da \w sprach|strong="H0559"\w* \w Juda|strong="H3063"\w* zu seinem \w Bruder|strong="H0251"\w* \w Simeon|strong="H8095"\w*: Zieh \w mit|strong="H0854"\w* mir \w hinauf|strong="H5927"\w* in mein \w Los|strong="H1486"\w* und laß uns wider die \w Kanaaniter|strong="H3669"\w* \w streiten|strong="H3898"\w*, so will \w ich|strong="H0589"\w* \w wieder|strong="H1571"\w* \w mit|strong="H0854"\w* dir \w ziehen|strong="H1980"\w* in dein \w Los|strong="H1486"\w*. Also \w zog|strong="H3212"\w* \w Simeon|strong="H8095"\w* \w mit|strong="H0854"\w* ihm.  
\v 4 Da nun \w Juda|strong="H3063"\w* \w hinaufzog|strong="H5927"\w*, \w gab|strong="H5414"\w* der \w HERR|strong="H3068"\w* die \w Kanaaniter|strong="H3669"\w* und \w Pheresiter|strong="H6522"\w* in ihre \w Hände|strong="H3027"\w*, und sie \w schlugen|strong="H5221"\w* zu \w Besek|strong="H0966"\w* \w zehntausend|strong="H6235"\w* \w Mann|strong="H0376"\w*.  
\v 5 Und \w fanden|strong="H4672"\w* den Adoni-\w Besek|strong="H0137"\w* zu \w Besek|strong="H0966"\w* und \w stritten|strong="H3898"\w* wider ihn und \w schlugen|strong="H5221"\w* die \w Kanaaniter|strong="H3669"\w* und \w Pheresiter|strong="H6522"\w*.  
\v 6 Aber Adoni-\w Besek|strong="H0137"\w* \w floh|strong="H5127"\w*, und sie \w jagten|strong="H7291"\w* ihm \w nach|strong="H0310"\w*; und da sie ihn \w ergriffen|strong="H0270"\w*, \w hieben|strong="H7112"\w* sie ihm die \w Daumen|strong="H0931"\w* \w ab|strong="H7112"\w* an seinen \w Händen|strong="H3027"\w* und \w Füßen|strong="H7272"\w*.  
\v 7 Da \w sprach|strong="H0559"\w* Adoni-\w Besek|strong="H0137"\w*: \w Siebzig|strong="H7657"\w* \w Könige|strong="H4428"\w* mit \w abgehauenen|strong="H7112"\w* \w Daumen|strong="H0931"\w* ihrer \w Hände|strong="H3027"\w* und \w Füße|strong="H7272"\w* \w lasen|strong="H3950"\w* \w auf|strong="H3950"\w* \w unter|strong="H8478"\w* meinem \w Tisch|strong="H7979"\w*. \w Wie|strong="H0834"\w* ich nun getan \w habe|strong="H6213"\w*, \w so|strong="H3651"\w* hat mir \w Gott|strong="H0430"\w* wieder \w vergolten|strong="H7999"\w*. Und man \w brachte|strong="H0935"\w* ihn gen \w Jerusalem|strong="H3389"\w*; \w daselbst|strong="H8033"\w* starb \w er|strong="H4191"\w*.  
\v 8 Aber die \w Kinder|strong="H1121"\w* \w Juda|strong="H3063"\w* \w stritten|strong="H3898"\w* wider \w Jerusalem|strong="H3389"\w* und gewannen \w es|strong="H3920"\w* und \w schlugen|strong="H5221"\w* es mit der \w Schärfe|strong="H6310"\w* des \w Schwerts|strong="H2719"\w* und \w zündeten|strong="H0784"\w* die \w Stadt|strong="H5892"\w* \w an|strong="H7971"\w*.  
\v 9  \w Darnach|strong="H0310"\w* zogen die \w Kinder|strong="H1121"\w* \w Juda|strong="H3063"\w* \w herab|strong="H3381"\w*, zu \w streiten|strong="H3898"\w* wider die \w Kanaaniter|strong="H3669"\w*, die auf dem \w Gebirge|strong="H2022"\w* und gegen \w Mittag|strong="H5045"\w* und in den \w Gründen|strong="H8219"\w* \w wohnten|strong="H3427"\w*.  
\v 10 Und \w Juda|strong="H3063"\w* zog \w hin|strong="H3212"\w* \w wider|strong="H0413"\w* die \w Kanaaniter|strong="H3669"\w*, die zu \w Hebron|strong="H2275"\w* \w wohnten|strong="H3427"\w* (\w Hebron|strong="H2275"\w* aber \w hieß|strong="H8034"\w* \w vorzeiten|strong="H6440"\w* Kirjath-\w Arba|strong="H7153"\w*), und sie \w schlugen|strong="H5221"\w* den \w Sesai|strong="H8344"\w* und \w Ahiman|strong="H0289"\w* und \w Thalmai|strong="H8526"\w*.  
\v 11 Und \w zogen|strong="H3212"\w* \w von|strong="H4480"\w* \w da|strong="H8033"\w* \w wider|strong="H0413"\w* die \w Einwohner|strong="H3427"\w* zu \w Debir|strong="H1688"\w* (\w Debir|strong="H1688"\w* aber \w hieß|strong="H8034"\w* \w vorzeiten|strong="H6440"\w* Kirjath-\w Sepher|strong="H7158"\w*).  
\v 12 Und \w Kaleb|strong="H3612"\w* \w sprach|strong="H0559"\w*: \w Wer|strong="H0834"\w* Kirjath-\w Sepher|strong="H7158"\w* \w schlägt|strong="H5221"\w* und \w gewinnt|strong="H3920"\w*, dem will ich meine \w Tochter|strong="H1323"\w* \w Achsa|strong="H5915"\w* zum \w Weibe|strong="H0802"\w* \w geben|strong="H5414"\w*.  
\v 13 Da \w gewann|strong="H3920"\w* es \w Othniel|strong="H6274"\w*, der \w Sohn|strong="H1121"\w* des \w Kenas|strong="H7073"\w*, \w Kalebs|strong="H3612"\w* \w jüngerer|strong="H6996"\w* \w Bruder|strong="H0251"\w*. Und er \w gab|strong="H5414"\w* ihm sein \w Tochter|strong="H1323"\w* \w Achsa|strong="H5915"\w* zum \w Weibe|strong="H0802"\w*.  
\v 14  \w Und|strong="H1961"\w* es begab \w sich|strong="H0935"\w*, da sie \w einzog|strong="H5496"\w*, beredete sie ihn, einen \w Acker|strong="H7704"\w* zu \w fordern|strong="H7592"\w* \w von|strong="H4480"\w* ihrem \w Vater|strong="H0001"\w*. Und sie \w stieg|strong="H6795"\w* \w vom|strong="H4480"\w* \w Esel|strong="H2543"\w*; da \w sprach|strong="H0559"\w* \w Kaleb|strong="H3612"\w* zu ihr: \w Was|strong="H4100"\w* ist dir?  
\v 15 Sie \w sprach|strong="H0559"\w*: \w Gib|strong="H3051"\w* mir einen \w Segen|strong="H1293"\w*! \w Denn|strong="H3588"\w* du hast mir ein \w Mittagsland|strong="H5045"\w* \w gegeben|strong="H5414"\w*; \w gib|strong="H5414"\w* mir auch \w Wasserquellen|strong="H1543"\w*! Da \w gab|strong="H5414"\w* \w er|strong="H3612"\w* ihr die \w Quellen|strong="H1543"\w* \w oben|strong="H5942"\w* und \w unten|strong="H8482"\w*.  
\v 16 Und die \w Kinder|strong="H1121"\w* des \w Keniters|strong="H7017"\w*, Mose'\w s|strong="H4872"\w* \w Schwagers|strong="H2859"\w*, zogen \w herauf|strong="H5927"\w* \w aus|strong="H4480"\w* der \w Palmenstadt|strong="H5892"\w* mit den \w Kindern|strong="H1121"\w* \w Juda|strong="H3063"\w* in die \w Wüste|strong="H4057"\w* \w Juda|strong="H3063"\w*, \w die|strong="H0834"\w* da liegt gegen \w Mittag|strong="H5045"\w* der Stadt \w Arad|strong="H6166"\w*, und gingen \w hin|strong="H3212"\w* und \w wohnten|strong="H3427"\w* \w unter|strong="H0854"\w* dem \w Volk|strong="H5971"\w*.  
\v 17 Und \w Juda|strong="H3063"\w* \w zog|strong="H3212"\w* hin \w mit|strong="H0854"\w* seinem \w Bruder|strong="H0251"\w* \w Simeon|strong="H8095"\w*, und \w schlugen|strong="H5221"\w* die \w Kanaaniter|strong="H3669"\w* \w zu|strong="H3427"\w* \w Zephath|strong="H6857"\w* und \w verbannten|strong="H2763"\w* sie und \w nannten|strong="H7121"\w* die \w Stadt|strong="H5892"\w* \w Horma|strong="H2767"\w*.  
\v 18 Dazu \w gewann|strong="H3920"\w* \w Juda|strong="H3063"\w* \w Gaza|strong="H5804"\w* mit seinem \w Zugehör|strong="H1366"\w* und \w Askalon|strong="H0831"\w* mit seinem \w Zugehör|strong="H1366"\w* und \w Ekron|strong="H6138"\w* mit seinem \w Zugehör|strong="H1366"\w*.  
\v 19 Und der \w HERR|strong="H3068"\w* \w war|strong="H1961"\w* mit \w Juda|strong="H3063"\w*, daß er das \w Gebirge|strong="H2022"\w* \w einnahm|strong="H3423"\w*; denn er konnte die \w Einwohner|strong="H3427"\w* im \w Grunde|strong="H6010"\w* \w nicht|strong="H3808"\w* \w vertreiben|strong="H3423"\w*, \w darum|strong="H3588"\w* daß sie \w eiserne|strong="H1270"\w* \w Wagen|strong="H7393"\w* hatten.  
\v 20 Und sie \w gaben|strong="H5414"\w* dem \w Kaleb|strong="H3612"\w* \w Hebron|strong="H2275"\w*, \w wie|strong="H0834"\w* \w Mose|strong="H4872"\w* gesagt \w hatte|strong="H1696"\w*; und er \w vertrieb|strong="H3423"\w* \w daraus|strong="H4480"\w* die \w drei|strong="H7969"\w* \w Söhne|strong="H1121"\w* des \w Enak|strong="H6061"\w*.  
\v 21 Aber die \w Kinder|strong="H1121"\w* \w Benjamin|strong="H1144"\w* \w vertrieben|strong="H3423"\w* die \w Jebusiter|strong="H2983"\w* \w nicht|strong="H3808"\w*, die zu \w Jerusalem|strong="H3389"\w* \w wohnten|strong="H3427"\w*; sondern die \w Jebusiter|strong="H2983"\w* \w wohnten|strong="H3427"\w* bei den \w Kindern|strong="H1121"\w* \w Benjamin|strong="H1144"\w* zu \w Jerusalem|strong="H3389"\w* \w bis|strong="H5704"\w* auf \w diesen|strong="H2088"\w* \w Tag|strong="H3117"\w*.  
\v 22  \w Desgleichen|strong="H1004"\w* \w zogen|strong="H5927"\w* \w auch|strong="H1571"\w* die Kinder Joseph \w hinauf|strong="H5927"\w* gen Beth-\w El|strong="H1008"\w*, und der \w HERR|strong="H3068"\w* war \w mit|strong="H5973"\w* ihnen.  
\v 23 Und das \w Haus|strong="H1004"\w* \w Josephs|strong="H3130"\w* ließ \w auskundschaften|strong="H8446"\w* Beth-\w El|strong="H1008"\w*, \w das|strong="H5892"\w* \w vorzeiten|strong="H6440"\w* \w Lus|strong="H3870"\w* \w hieß|strong="H8034"\w*.  
\v 24 Und die \w Wächter|strong="H8104"\w* \w sahen|strong="H7200"\w* einen \w Mann|strong="H0376"\w* \w aus|strong="H4480"\w* der \w Stadt|strong="H5892"\w* \w gehen|strong="H3318"\w* und \w sprachen|strong="H0559"\w* zu ihm: \w Weise|strong="H7200"\w* \w uns|strong="H4994"\w*, wo wir \w in|strong="H3996"\w* die \w Stadt|strong="H5892"\w* \w kommen|strong="H3996"\w*, so wollen wir \w Barmherzigkeit|strong="H2617"\w* \w an|strong="H5973"\w* dir \w tun|strong="H6213"\w*.  
\v 25 Und da er ihnen \w zeigte|strong="H7200"\w*, \w wo|strong="H3996"\w* sie in die \w Stadt|strong="H5892"\w* \w kämen|strong="H3996"\w*, schlugen \w sie|strong="H5221"\w* die \w Stadt|strong="H5892"\w* mit der \w Schärfe|strong="H6310"\w* des \w Schwerts|strong="H2719"\w*; aber den \w Mann|strong="H0376"\w* und all \w sein|strong="H3605"\w* \w Geschlecht|strong="H4940"\w* ließen sie \w gehen|strong="H7971"\w*.  
\v 26 Da \w zog|strong="H3212"\w* derselbe \w Mann|strong="H0376"\w* ins \w Land|strong="H0776"\w* der \w Hethiter|strong="H2850"\w* und \w baute|strong="H1129"\w* eine \w Stadt|strong="H5892"\w* und \w hieß|strong="H7121"\w* \w sie|strong="H8034"\w* \w Lus|strong="H3870"\w*; \w die|strong="H1931"\w* \w heißt|strong="H8034"\w* \w noch|strong="H5704"\w* \w heutigestages|strong="H2088"\w* also.  
\v 27 Und \w Manasse|strong="H4519"\w* \w vertrieb|strong="H3423"\w* \w nicht|strong="H3808"\w* Beth-\w Sean|strong="H1052"\w* mit den zugehörigen \w Orten|strong="H1323"\w* noch \w Thaanach|strong="H8590"\w* mit den zugehörigen \w Orten|strong="H1323"\w* noch die \w Einwohner|strong="H3427"\w* zu \w Dor|strong="H1756"\w* mit den zugehörigen \w Orten|strong="H1323"\w* noch die \w Einwohner|strong="H3427"\w* zu \w Jibleam|strong="H2991"\w* mit den zugehörigen \w Orten|strong="H1323"\w* noch die \w Einwohner|strong="H3427"\w* zu \w Megiddo|strong="H4023"\w* mit den zugehörigen \w Orten|strong="H1323"\w*; und die \w Kanaaniter|strong="H3669"\w* \w blieben|strong="H2974"\w* \w wohnen|strong="H3427"\w* \w im|strong="H2063"\w* \w Land|strong="H0776"\w*.  
\v 28  \w Da|strong="H1961"\w* \w aber|strong="H3588"\w* \w Israel|strong="H3478"\w* mächtig \w war|strong="H2388"\w*, \w machte|strong="H7760"\w* es die \w Kanaaniter|strong="H3669"\w* \w zinsbar|strong="H4522"\w* und \w vertrieb|strong="H3423"\w* sie \w nicht|strong="H3808"\w*.  
\v 29 Desgleichen \w vertrieb|strong="H3423"\w* auch \w Ephraim|strong="H0669"\w* die \w Kanaaniter|strong="H3669"\w* \w nicht|strong="H3808"\w*, die zu \w Geser|strong="H1507"\w* \w wohnten|strong="H3427"\w*, sondern die \w Kanaaniter|strong="H3669"\w* \w wohnten|strong="H3427"\w* \w unter|strong="H7130"\w* ihnen zu \w Geser|strong="H1507"\w*.  
\v 30  \w Sebulon|strong="H2074"\w* \w vertrieb|strong="H3423"\w* auch \w nicht|strong="H3808"\w* die \w Einwohner|strong="H3427"\w* von \w Kitron|strong="H7003"\w* \w und|strong="H3427"\w* \w Nahalol|strong="H5096"\w*; sondern die \w Kanaaniter|strong="H3669"\w* \w wohnten|strong="H3427"\w* \w unter|strong="H7130"\w* ihnen und \w waren|strong="H1961"\w* \w zinsbar|strong="H4522"\w*.  
\v 31  \w Asser|strong="H0836"\w* \w vertrieb|strong="H3423"\w* die \w Einwohner|strong="H3427"\w* zu \w Akko|strong="H5910"\w* \w nicht|strong="H3808"\w* noch die \w Einwohner|strong="H3427"\w* zu \w Sidon|strong="H6721"\w*, zu \w Ahelab|strong="H0303"\w*, zu \w Achsib|strong="H0392"\w*, zu \w Helba|strong="H2462"\w*, zu \w Aphik|strong="H0663"\w* und zu \w Rehob|strong="H7340"\w*;  
\v 32 sondern die \w Asseriter|strong="H0843"\w* \w wohnten|strong="H3427"\w* \w unter|strong="H7130"\w* den \w Kanaanitern|strong="H3669"\w*, die im \w Lande|strong="H0776"\w* \w wohnten|strong="H3427"\w*, \w denn|strong="H3588"\w* sie \w vertrieben|strong="H3423"\w* sie \w nicht|strong="H3808"\w*.  
\v 33  \w Naphthali|strong="H5321"\w* \w vertrieb|strong="H3423"\w* die \w Einwohner|strong="H3427"\w* \w nicht|strong="H3808"\w* zu Beth-\w Semes|strong="H1053"\w* \w noch|strong="H3427"\w* zu Beth-\w Anath|strong="H1043"\w*, sondern \w wohnte|strong="H3427"\w* \w unter|strong="H7130"\w* den \w Kanaanitern|strong="H3669"\w*, die im \w Lande|strong="H0776"\w* \w wohnten|strong="H3427"\w*. Aber \w die|strong="H3427"\w* zu Beth-\w Semes|strong="H1053"\w* und zu Beth-\w Anath|strong="H1043"\w* \w wurden|strong="H1961"\w* \w zinsbar|strong="H4522"\w*.  
\v 34 Und die \w Amoriter|strong="H0567"\w* \w drängten|strong="H3905"\w* die \w Kinder|strong="H1121"\w* \w Dan|strong="H1835"\w* aufs \w Gebirge|strong="H2022"\w* \w und|strong="H3588"\w* \w ließen|strong="H5414"\w* \w nicht|strong="H3808"\w* \w zu|strong="H5414"\w*, daß sie \w herunter|strong="H3381"\w* in den \w Grund|strong="H6010"\w* \w kämen|strong="H3381"\w*.  
\v 35 Und die \w Amoriter|strong="H0567"\w* \w blieben|strong="H2974"\w* \w wohnen|strong="H3427"\w* auf dem \w Gebirge|strong="H2022"\w* \w Heres|strong="H2776"\w*, zu \w Ajalon|strong="H0357"\w* und \w Saalbim|strong="H8169"\w*. Doch ward ihnen die \w Hand|strong="H3027"\w* des \w Hauses|strong="H1004"\w* \w Joseph|strong="H3130"\w* zu \w schwer|strong="H3513"\w*, und \w wurden|strong="H1961"\w* \w zinsbar|strong="H4522"\w*.  
\v 36 Und die \w Grenze|strong="H1366"\w* der \w Amoriter|strong="H0567"\w* war, \w da|strong="H4480"\w* man nach \w Akrabbim|strong="H4610"\w* \w hinaufgeht|strong="H4608"\w*, \w von|strong="H4480"\w* dem \w Fels|strong="H5553"\w* an und weiter \w hinauf|strong="H4605"\w*.   
\c 2  
\p
\v 1 Es \w kam|strong="H5927"\w* aber der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w herauf|strong="H5927"\w* \w von|strong="H4480"\w* \w Gilgal|strong="H1537"\w* \w gen|strong="H0413"\w* \w Bochim|strong="H1066"\w* und \w sprach|strong="H0559"\w*: Ich habe \w euch|strong="H0853"\w* \w aus|strong="H4480"\w* \w Ägypten|strong="H4714"\w* \w heraufgeführt|strong="H5927"\w* \w und|strong="H0853"\w* \w in|strong="H0413"\w* das \w Land|strong="H0776"\w* \w gebracht|strong="H0935"\w*, \w das|strong="H0834"\w* ich euren \w Vätern|strong="H0001"\w* geschworen \w habe|strong="H7650"\w*, und \w sprach|strong="H0559"\w*, ich wollte meinen \w Bund|strong="H1285"\w* \w mit|strong="H0854"\w* euch \w nicht|strong="H3808"\w* \w brechen|strong="H6565"\w* \w ewiglich|strong="H5769"\w*;  
\v 2  \w ihr|strong="H0859"\w* aber solltet \w keinen|strong="H3808"\w* \w Bund|strong="H1285"\w* \w machen|strong="H3772"\w* mit den \w Einwohnern|strong="H3427"\w* \w dieses|strong="H2063"\w* \w Landes|strong="H0776"\w* und ihre \w Altäre|strong="H4196"\w* \w zerbrechen|strong="H5422"\w*. Aber ihr habt meiner \w Stimme|strong="H6963"\w* \w nicht|strong="H3808"\w* \w gehorcht|strong="H8085"\w*. \w Warum|strong="H4100"\w* habt ihr \w das|strong="H2063"\w* \w getan|strong="H6213"\w*?  
\v 3 Da \w sprach|strong="H0559"\w* ich \w auch|strong="H1571"\w*: Ich will \w sie|strong="H0853"\w* \w nicht|strong="H3808"\w* \w vertreiben|strong="H1644"\w* \w vor|strong="H4480"\w* \w euch|strong="H6440"\w*, daß sie euch zum \w Strick|strong="H4170"\w* \w werden|strong="H1961"\w* und ihre \w Götter|strong="H0430"\w* zum \w Netz|strong="H6654"\w*.  
\v 4 Und \w da|strong="H1961"\w* der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w solche|strong="H0428"\w* \w Worte|strong="H1697"\w* geredet \w hatte|strong="H1696"\w* \w zu|strong="H0413"\w* \w allen|strong="H3605"\w* \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w*, \w hob|strong="H5375"\w* das \w Volk|strong="H5971"\w* seine \w Stimme|strong="H6963"\w* \w auf|strong="H5375"\w* und \w weinte|strong="H1058"\w*,  
\v 5 und \w hießen|strong="H7121"\w* \w die|strong="H1931"\w* \w Stätte|strong="H4725"\w* \w Bochim|strong="H1066"\w* und \w opferten|strong="H2076"\w* \w daselbst|strong="H8033"\w* dem \w HERRN|strong="H3068"\w*.  
\v 6 Als \w Josua|strong="H3091"\w* das \w Volk|strong="H5971"\w* von sich gelassen \w hatte|strong="H7971"\w* und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* hingezogen \w waren|strong="H3212"\w*, ein \w jeglicher|strong="H0376"\w* in sein \w Erbteil|strong="H5159"\w*, das \w Land|strong="H0776"\w* \w einzunehmen|strong="H3423"\w*,  
\v 7  \w diente|strong="H5647"\w* das \w Volk|strong="H5971"\w* dem \w HERRN|strong="H3068"\w*, \w solange|strong="H3605"\w* \w Josua|strong="H3091"\w* \w lebte|strong="H3117"\w* \w und|strong="H3605"\w* die \w Ältesten|strong="H2205"\w*, \w die|strong="H0834"\w* noch \w lange|strong="H3117"\w* \w nach|strong="H0310"\w* \w Josua|strong="H3091"\w* \w lebten|strong="H0748"\w* und \w alle|strong="H3605"\w* \w die|strong="H0834"\w* \w großen|strong="H1419"\w* \w Werke|strong="H4639"\w* des \w HERRN|strong="H3068"\w* gesehen \w hatten|strong="H7200"\w*, \w die|strong="H0834"\w* \w er|strong="H3478"\w* getan \w hatte|strong="H6213"\w*.  
\v 8 Da nun \w Josua|strong="H3091"\w*, der \w Sohn|strong="H1121"\w* \w Nuns|strong="H5126"\w*, gestorben \w war|strong="H4191"\w*, der \w Knecht|strong="H5650"\w* des \w HERRN|strong="H3068"\w*, als er \w hundertundzehn|strong="H3967"\w* \w Jahre|strong="H8141"\w* \w alt|strong="H1121"\w* war,  
