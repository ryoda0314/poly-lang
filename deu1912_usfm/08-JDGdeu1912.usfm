\v 29 Und \w er|strong="H8123"\w* \w faßte|strong="H3943"\w* die \w zwei|strong="H8147"\w* \w Mittelsäulen|strong="H8432"\w*, \w auf|strong="H5921"\w* \w welche|strong="H0834"\w* das \w Haus|strong="H1004"\w* gesetzt \w war|strong="H3559"\w* und \w darauf|strong="H5921"\w* es sich \w hielt|strong="H5564"\w*, \w eine|strong="H0259"\w* in seine \w rechte|strong="H3225"\w* und die \w andere|strong="H0259"\w* in seine linke \w Hand|strong="H8040"\w*,  
\v 30  \w und|strong="H8123"\w* \w sprach|strong="H0559"\w*: Meine \w Seele|strong="H5315"\w* \w sterbe|strong="H4191"\w* \w mit|strong="H5973"\w* den \w Philistern|strong="H6430"\w*! und neigte \w sich|strong="H5186"\w* \w kräftig|strong="H3581"\w*. Da \w fiel|strong="H5307"\w* das \w Haus|strong="H1004"\w* \w auf|strong="H5921"\w* die \w Fürsten|strong="H5633"\w* und \w auf|strong="H5921"\w* \w alles|strong="H3605"\w* \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* darin war, daß der \w Toten|strong="H4191"\w* \w mehr|strong="H7227"\w* \w waren|strong="H1961"\w*, \w die|strong="H0834"\w* in seinem \w Tod|strong="H4194"\w* \w starben|strong="H4191"\w*, \w denn|strong="H4480"\w* \w die|strong="H0834"\w* bei seinem \w Leben|strong="H2416"\w* \w starben|strong="H4191"\w*.  
\v 31 Da kamen sein \w Brüder|strong="H0251"\w* \w hernieder|strong="H3381"\w* und seines \w Vaters|strong="H0001"\w* \w ganzes|strong="H3605"\w* \w Haus|strong="H1004"\w* und \w hoben|strong="H5375"\w* \w ihn|strong="H0853"\w* \w auf|strong="H5375"\w* und trugen ihn \w hinauf|strong="H5927"\w* und \w begruben|strong="H6912"\w* \w ihn|strong="H0853"\w* in seines \w Vaters|strong="H0001"\w* \w Manoahs|strong="H4495"\w* \w Grab|strong="H6913"\w*, \w zwischen|strong="H0996"\w* \w Zora|strong="H6881"\w* \w und|strong="H0996"\w* \w Esthaol|strong="H0847"\w*. \w Er|strong="H1931"\w* \w richtete|strong="H8199"\w* aber \w Israel|strong="H3478"\w* \w zwanzig|strong="H6242"\w* \w Jahre|strong="H8141"\w*.   
\c 17  
\p
\v 1 Es \w war|strong="H1961"\w* ein \w Mann|strong="H0376"\w* \w auf|strong="H4480"\w* dem \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w*, mit \w Namen|strong="H8034"\w* \w Micha|strong="H4321"\w*.  
\v 2 Der \w sprach|strong="H0559"\w* zu seiner \w Mutter|strong="H0517"\w*: Die \w tausendundhundert|strong="H0505"\w* \w Silberlinge|strong="H3701"\w*, \w die|strong="H0834"\w* dir genommen worden \w sind|strong="H3947"\w* und derenthalben \w du|strong="H0859"\w* den \w Fluch|strong="H0422"\w* gesprochen und \w auch|strong="H1571"\w* vor meinen \w Ohren|strong="H0241"\w* gesagt \w hast|strong="H0559"\w*, \w sieh|strong="H2009"\w*, das \w Geld|strong="H3701"\w* ist \w bei|strong="H0854"\w* mir; \w ich|strong="H0589"\w* habe es \w genommen|strong="H3947"\w*. Da \w sprach|strong="H0559"\w* sein \w Mutter|strong="H0517"\w*: \w Gesegnet|strong="H1288"\w* sei mein \w Sohn|strong="H1121"\w* dem \w HERRN|strong="H3068"\w*!  
\v 3 Also \w gab|strong="H7725"\w* er seiner \w Mutter|strong="H0517"\w* die \w tausendundhundert|strong="H0505"\w* \w Silberlinge|strong="H3701"\w* \w wieder|strong="H7725"\w*. Und seine \w Mutter|strong="H0517"\w* \w sprach|strong="H0559"\w*: Ich habe das \w Geld|strong="H3701"\w* dem \w HERRN|strong="H3068"\w* \w geheiligt|strong="H6942"\w* \w von|strong="H4480"\w* meiner \w Hand|strong="H3027"\w* für meinen \w Sohn|strong="H1121"\w*, daß man ein \w Bildnis|strong="H6459"\w* und einen \w Abgott|strong="H4541"\w* \w machen|strong="H6213"\w* soll; darum so \w gebe|strong="H7725"\w* ich's dir \w nun|strong="H6258"\w* \w wieder|strong="H7725"\w*.  
\v 4 Aber er \w gab|strong="H7725"\w* seiner \w Mutter|strong="H0517"\w* das \w Geld|strong="H3701"\w* \w wieder|strong="H7725"\w*. Da \w nahm|strong="H3947"\w* seine \w Mutter|strong="H0517"\w* \w zweihundert|strong="H3967"\w* \w Silberlinge|strong="H3701"\w* und \w tat|strong="H5414"\w* sie zu dem \w Goldschmied|strong="H6884"\w*; der \w machte|strong="H6213"\w* ihr ein \w Bild|strong="H6459"\w* und einen \w Abgott|strong="H4541"\w*, das \w war|strong="H1961"\w* darnach im \w Hause|strong="H1004"\w* \w Michas|strong="H4321"\w*.  
\v 5 Und der \w Mann|strong="H0376"\w* \w Micha|strong="H4318"\w* hatte also ein \w Gotteshaus|strong="H1004"\w*; und \w machte|strong="H6213"\w* einen \w Leibrock|strong="H0646"\w* und \w Hausgötzen|strong="H8655"\w* und \w füllte|strong="H4390"\w* \w seiner|strong="H4480"\w* \w Söhne|strong="H1121"\w* \w einem|strong="H0259"\w* die \w Hand|strong="H3027"\w*, daß er sein \w Priester|strong="H3548"\w* \w ward|strong="H1961"\w*.  
\v 6  \w Zu|strong="H1992"\w* der \w Zeit|strong="H3117"\w* war \w kein|strong="H0369"\w* \w König|strong="H4428"\w* in \w Israel|strong="H3478"\w*, und ein \w jeglicher|strong="H0376"\w* \w tat|strong="H6213"\w*, was ihn \w recht|strong="H3477"\w* \w deuchte|strong="H5869"\w*.  
\v 7 Es \w war|strong="H1961"\w* aber ein \w Jüngling|strong="H5288"\w* \w von|strong="H4480"\w* Bethlehem-\w Juda|strong="H1035"\w* \w unter|strong="H4480"\w* dem \w Geschlecht|strong="H4940"\w* Juda'\w s|strong="H3063"\w*, \w und|strong="H1931"\w* er war ein \w Levit|strong="H3881"\w* \w und|strong="H1931"\w* war \w fremd|strong="H1481"\w* \w daselbst|strong="H8033"\w*.  
\v 8  \w Er|strong="H0376"\w* \w zog|strong="H4480"\w* \w aus|strong="H4480"\w* der \w Stadt|strong="H5892"\w* Bethlehem-\w Juda|strong="H1035"\w*, zu \w wandern|strong="H1481"\w*, \w wo|strong="H0834"\w* er hin \w konnte|strong="H4672"\w*. Und da er aufs \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w* \w kam|strong="H0935"\w* \w zum|strong="H5704"\w* \w Hause|strong="H1004"\w* \w Michas|strong="H4318"\w*, daß er seinen \w Weg|strong="H1870"\w* \w ginge|strong="H6213"\w*,  
\v 9  \w fragte|strong="H0559"\w* ihn \w Micha|strong="H4318"\w*: \w Wo|strong="H4480"\w* kommst du \w her|strong="H0935"\w*? Er \w antwortete|strong="H0559"\w* \w ihm|strong="H0413"\w*: \w Ich|strong="H0595"\w* bin ein \w Levit|strong="H3881"\w* \w von|strong="H4480"\w* Bethlehem-\w Juda|strong="H1035"\w* \w und|strong="H0595"\w* \w wandere|strong="H1980"\w*, \w wo|strong="H0834"\w* ich hin \w kann|strong="H4672"\w*.  
\v 10  \w Micha|strong="H4318"\w* aber \w sprach|strong="H0559"\w* zu ihm: \w Bleibe|strong="H3427"\w* \w bei|strong="H5978"\w* mir, du sollst mein \w Vater|strong="H0001"\w* und mein \w Priester|strong="H3548"\w* \w sein|strong="H1961"\w*; \w ich|strong="H0595"\w* will dir \w jährlich|strong="H3117"\w* \w zehn|strong="H6235"\w* \w Silberlinge|strong="H3701"\w* und \w deine|strong="H6187"\w* \w Kleidung|strong="H0899"\w* und \w Nahrung|strong="H4241"\w* \w geben|strong="H5414"\w*. Und der \w Levit|strong="H3881"\w* \w ging|strong="H3212"\w* hin.  
\v 11 Der \w Levit|strong="H3881"\w* trat \w an|strong="H2974"\w*, zu \w bleiben|strong="H3427"\w* bei dem \w Mann|strong="H0376"\w*; und er \w hielt|strong="H1961"\w* den \w Jüngling|strong="H5288"\w* gleich wie \w einen|strong="H0259"\w* \w Sohn|strong="H1121"\w*.  
\v 12 Und \w Micha|strong="H4318"\w* \w füllte|strong="H4390"\w* dem \w Leviten|strong="H3881"\w* die \w Hand|strong="H3027"\w*, \w daß|strong="H5288"\w* er sein \w Priester|strong="H3548"\w* \w ward|strong="H1961"\w*, und \w war|strong="H1961"\w* also im \w Haus|strong="H1004"\w* \w Michas|strong="H4318"\w*.  
\v 13 Und \w Micha|strong="H4318"\w* \w sprach|strong="H0559"\w*: \w Nun|strong="H6258"\w* weiß \w ich|strong="H3045"\w*, \w daß|strong="H3588"\w* mir der \w HERR|strong="H3068"\w* wird \w wohltun|strong="H3190"\w*, \w weil|strong="H3588"\w* ich einen \w Leviten|strong="H3881"\w* zum \w Priester|strong="H3548"\w* \w habe|strong="H1961"\w*.   
\c 18  
\p
\v 1  \w Zu|strong="H1992"\w* der \w Zeit|strong="H3117"\w* war \w kein|strong="H0369"\w* \w König|strong="H4428"\w* in \w Israel|strong="H3478"\w*. \w Und|strong="H1992"\w* der \w Stamm|strong="H7626"\w* der \w Daniter|strong="H1839"\w* \w suchte|strong="H1245"\w* sich ein \w Erbteil|strong="H5159"\w*, da sie \w wohnen|strong="H3427"\w* möchten; \w denn|strong="H3588"\w* es war bis \w auf|strong="H5704"\w* \w den|strong="H1931"\w* \w Tag|strong="H3117"\w* \w noch|strong="H3808"\w* kein \w Erbe|strong="H5159"\w* auf sie \w gefallen|strong="H5307"\w* \w unter|strong="H8432"\w* den \w Stämmen|strong="H7626"\w* \w Israels|strong="H3478"\w*.  
\v 2 Und die \w Kinder|strong="H1121"\w* \w Dan|strong="H1835"\w* \w sandten|strong="H7971"\w* \w aus|strong="H4480"\w* ihren \w Geschlechtern|strong="H4940"\w* \w von|strong="H4480"\w* ihren \w Enden|strong="H7098"\w* \w fünf|strong="H2568"\w* streitbare \w Männer|strong="H0582"\w* \w von|strong="H4480"\w* \w Zora|strong="H6881"\w* \w und|strong="H4480"\w* \w Esthaol|strong="H0847"\w*, das \w Land|strong="H0776"\w* zu \w erkunden|strong="H7270"\w* und zu \w erforschen|strong="H2713"\w*, und \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* ihnen: \w Ziehet|strong="H3212"\w* hin und \w erforschet|strong="H2713"\w* das \w Land|strong="H0776"\w*. Und sie \w kamen|strong="H0935"\w* auf das \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w* \w ans|strong="H5704"\w* \w Haus|strong="H1004"\w* \w Michas|strong="H4318"\w* und blieben über \w Nacht|strong="H3885"\w* \w daselbst|strong="H8033"\w*.  
\v 3 Und da sie \w bei|strong="H5973"\w* dem \w Gesinde|strong="H1004"\w* \w Michas|strong="H4318"\w* \w waren|strong="H1992"\w*, \w erkannten|strong="H5234"\w* \w sie|strong="H1992"\w* die \w Stimme|strong="H6963"\w* des \w Jünglings|strong="H5288"\w*, des \w Leviten|strong="H3881"\w*; und sie \w wichen|strong="H5493"\w* von ihrem Weg \w dahin|strong="H8033"\w* und \w sprachen|strong="H0559"\w* zu ihm: \w Wer|strong="H4310"\w* hat dich \w hierhergebracht|strong="H1988"\w*? \w Was|strong="H4100"\w* \w machst|strong="H6213"\w* \w du|strong="H0859"\w* \w da|strong="H2088"\w*? Und \w was|strong="H4100"\w* hast du \w hier|strong="H6311"\w*?  
\v 4 Er \w antwortete|strong="H0559"\w* \w ihnen|strong="H0413"\w*: \w So|strong="H2090"\w* und \w so|strong="H2090"\w* hat \w Micha|strong="H4318"\w* an mir \w getan|strong="H6213"\w* und hat mich \w gedingt|strong="H7936"\w*, daß ich sein \w Priester|strong="H3548"\w* \w sei|strong="H1961"\w*.  
\v 5 Sie \w sprachen|strong="H0559"\w* zu ihm: \w Frage|strong="H7592"\w* \w doch|strong="H4994"\w* \w Gott|strong="H0430"\w*, daß wir \w erfahren|strong="H3045"\w*, ob unser \w Weg|strong="H1870"\w*, \w den|strong="H5921"\w* \w wir|strong="H0587"\w* \w wandeln|strong="H1980"\w*, auch wohl geraten \w werde|strong="H6743"\w*.  
\v 6 Der \w Priester|strong="H3548"\w* \w antwortete|strong="H0559"\w* ihnen: \w Ziehet|strong="H3212"\w* hin mit \w Frieden|strong="H7965"\w*; euer \w Weg|strong="H1870"\w*, \w den|strong="H0834"\w* ihr \w ziehet|strong="H3212"\w*, ist recht \w vor|strong="H5227"\w* dem \w HERRN|strong="H3068"\w*.  
\v 7 Da \w gingen|strong="H3212"\w* die \w fünf|strong="H2568"\w* \w Männer|strong="H0582"\w* hin und \w kamen|strong="H0935"\w* gen \w Lais|strong="H3919"\w* und \w sahen|strong="H7200"\w* das \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w darin|strong="H7130"\w* war, \w sicher|strong="H0983"\w* \w wohnen|strong="H3427"\w* auf die \w Weise|strong="H4941"\w* wie die \w Sidonier|strong="H6722"\w*, \w still|strong="H8252"\w* und \w sicher|strong="H0982"\w*; und war \w niemand|strong="H0369"\w*, der \w ihnen|strong="H1697"\w* Leid \w täte|strong="H3637"\w* im \w Land|strong="H0776"\w* oder Herr über sie wäre, und \w waren|strong="H1992"\w* \w ferne|strong="H7350"\w* \w von|strong="H4480"\w* den \w Sidoniern|strong="H6722"\w* und \w hatten|strong="H0369"\w* \w nichts|strong="H1697"\w* mit \w Leuten|strong="H0120"\w* zu \w tun|strong="H5973"\w*.  
\v 8 Und sie \w kamen|strong="H0935"\w* \w zu|strong="H0413"\w* ihren \w Brüdern|strong="H0251"\w* gen \w Zora|strong="H6881"\w* und \w Esthaol|strong="H0847"\w*; und ihre \w Brüder|strong="H0251"\w* \w sprachen|strong="H0559"\w* zu ihnen: \w Wie|strong="H4100"\w* steht's mit \w euch|strong="H0859"\w*?  
\v 9 Sie \w sprachen|strong="H0559"\w*: \w Auf|strong="H6965"\w*, laßt uns \w zu|strong="H5921"\w* ihnen \w hinaufziehen|strong="H5927"\w*! \w denn|strong="H3588"\w* wir haben das \w Land|strong="H0776"\w* \w besehen|strong="H7200"\w*, das ist \w sehr|strong="H3966"\w* \w gut|strong="H2896"\w*. Darum \w eilt|strong="H2814"\w* und \w seid|strong="H0859"\w* \w nicht|strong="H0408"\w* \w faul|strong="H6101"\w* zu \w ziehen|strong="H3212"\w*, daß ihr \w kommt|strong="H0935"\w*, das \w Land|strong="H0776"\w* \w einzunehmen|strong="H3423"\w*.  
\v 10 Wenn ihr \w kommt|strong="H0935"\w*, werdet ihr \w zu|strong="H0413"\w* einem \w sichern|strong="H0982"\w* \w Volke|strong="H5971"\w* \w kommen|strong="H0935"\w*, und das \w Land|strong="H0776"\w* ist weit und \w breit|strong="H7342"\w*; \w denn|strong="H3588"\w* \w Gott|strong="H0430"\w* hat's in eure \w Hände|strong="H3027"\w* \w gegeben|strong="H5414"\w*, einen \w solchen|strong="H0834"\w* \w Ort|strong="H4725"\w*, da \w nichts|strong="H0369"\w* \w gebricht|strong="H4270"\w* an \w alle|strong="H3605"\w* \w dem|strong="H1697"\w*, \w was|strong="H0834"\w* auf \w Erden|strong="H0776"\w* ist.  
\v 11 Da \w zogen|strong="H5265"\w* \w von|strong="H4480"\w* \w da|strong="H8033"\w* \w aus|strong="H4480"\w* den \w Geschlechtern|strong="H4940"\w* \w Dan|strong="H1839"\w* \w von|strong="H4480"\w* \w Zora|strong="H6881"\w* \w und|strong="H4480"\w* \w Esthaol|strong="H0847"\w* \w sechshundert|strong="H8337"\w* \w Mann|strong="H0376"\w*, \w gerüstet|strong="H2296"\w* mit ihren \w Waffen|strong="H3627"\w* zum \w Streit|strong="H4421"\w*,  
\v 12 und zogen \w hinauf|strong="H5927"\w* und lagerten \w sich|strong="H2583"\w* zu Kirjath-\w Jearim|strong="H7157"\w* in \w Juda|strong="H3063"\w*. \w Daher|strong="H5921"\w* nannten \w sie|strong="H7121"\w* \w die|strong="H1931"\w* \w Stätte|strong="H4725"\w* das Lager \w Dan|strong="H4265"\w* bis \w auf|strong="H5704"\w* \w diesen|strong="H2088"\w* \w Tag|strong="H3117"\w*, \w das|strong="H2088"\w* \w hinter|strong="H0310"\w* Kirjath-\w Jearim|strong="H7157"\w* \w liegt|strong="H0310"\w*.  
\v 13 Und \w von|strong="H4480"\w* \w da|strong="H8033"\w* gingen \w sie|strong="H5674"\w* auf das \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w* und \w kamen|strong="H0935"\w* \w zum|strong="H5704"\w* \w Hause|strong="H1004"\w* \w Michas|strong="H4318"\w*.  
\v 14 Da \w antworteten|strong="H6030"\w* die \w fünf|strong="H2568"\w* \w Männer|strong="H0582"\w*, die ausgegangen \w waren|strong="H1980"\w*, das \w Land|strong="H0776"\w* \w Lais|strong="H3919"\w* zu \w erkunden|strong="H7270"\w*, und \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* ihren \w Brüdern|strong="H0251"\w*: Wißt \w ihr|strong="H3045"\w* auch, \w daß|strong="H3588"\w* in \w diesen|strong="H0428"\w* \w Häusern|strong="H1004"\w* ein \w Leibrock|strong="H0646"\w*, \w Hausgötzen|strong="H8655"\w*, \w Bildnis|strong="H6459"\w* und \w Abgott|strong="H4541"\w* \w sind|strong="H3426"\w*? \w Nun|strong="H6258"\w* möget ihr \w denken|strong="H3045"\w*, \w was|strong="H4100"\w* euch zu tun \w ist|strong="H6213"\w*.  
\v 15 Sie \w kehrten|strong="H5493"\w* \w da|strong="H8033"\w* ein und \w kamen|strong="H0935"\w* \w an|strong="H0413"\w* das \w Haus|strong="H1004"\w* des \w Jünglings|strong="H5288"\w*, des \w Leviten|strong="H3881"\w*, in \w Michas|strong="H4318"\w* \w Haus|strong="H1004"\w* und \w grüßten|strong="H7592"\w* ihn \w freundlich|strong="H7965"\w*.  
\v 16 Aber die \w sechshundert|strong="H8337"\w* \w Gerüsteten|strong="H0376"\w* mit ihren \w Waffen|strong="H3627"\w*, \w die|strong="H0834"\w* \w von|strong="H4480"\w* den \w Kindern|strong="H1121"\w* \w Dan|strong="H1835"\w* \w waren|strong="H2296"\w*, \w standen|strong="H5324"\w* \w vor|strong="H6607"\w* dem \w Tor|strong="H8179"\w*.  
\v 17 Und die \w fünf|strong="H2568"\w* \w Männer|strong="H0582"\w*, die das \w Land|strong="H0776"\w* zu \w erkunden|strong="H1980"\w* ausgezogen \w waren|strong="H7270"\w*, gingen \w hinauf|strong="H5927"\w* und \w kamen|strong="H0935"\w* \w dahin|strong="H8033"\w* und \w nahmen|strong="H3947"\w* das \w Bild|strong="H6459"\w*, den \w Leibrock|strong="H0646"\w*, die \w Hausgötzen|strong="H8655"\w* und den \w Abgott|strong="H4541"\w*. Dieweil \w stand|strong="H5324"\w* der \w Priester|strong="H3548"\w* \w vor|strong="H6607"\w* dem \w Tor|strong="H8179"\w* bei den \w sechshundert|strong="H8337"\w* \w Gerüsteten|strong="H0376"\w* mit ihren \w Waffen|strong="H3627"\w*.  
\v 18  \w Als|strong="H0428"\w* nun jene ins \w Haus|strong="H1004"\w* \w Michas|strong="H4318"\w* gekommen \w waren|strong="H0935"\w* und \w nahmen|strong="H3947"\w* das \w Bild|strong="H6459"\w*, den \w Leibrock|strong="H0646"\w*, die \w Hausgötzen|strong="H8655"\w* und den \w Abgott|strong="H4541"\w*, \w sprach|strong="H0559"\w* der \w Priester|strong="H3548"\w* \w zu|strong="H0413"\w* ihnen: \w Was|strong="H4100"\w* \w macht|strong="H6213"\w* \w ihr|strong="H0859"\w*?  
\v 19 Sie \w antworteten|strong="H0559"\w* ihm: \w Schweige|strong="H2790"\w* und \w halte|strong="H7760"\w* das \w Maul|strong="H6310"\w* \w zu|strong="H5921"\w* und \w ziehe|strong="H3212"\w* \w mit|strong="H5973"\w* uns, daß du unser \w Vater|strong="H0001"\w* und \w Priester|strong="H3548"\w* \w seist|strong="H1961"\w*. Ist dir's \w besser|strong="H2896"\w*, daß du in des \w einen|strong="H0259"\w* \w Mannes|strong="H0376"\w* \w Haus|strong="H1004"\w* \w Priester|strong="H3548"\w* \w seist|strong="H1961"\w* \w oder|strong="H3548"\w* unter derer einem ganzen \w Stamm|strong="H7626"\w* und \w Geschlecht|strong="H4940"\w* in \w Israel|strong="H3478"\w*?  
\v 20 Das \w gefiel|strong="H3820"\w* dem \w Priester|strong="H3548"\w* \w wohl|strong="H3190"\w*, und er \w nahm|strong="H3947"\w* den \w Leibrock|strong="H0646"\w*, die \w Hausgötzen|strong="H8655"\w* und das \w Bild|strong="H6459"\w* und \w kam|strong="H0935"\w* mit \w unter|strong="H7130"\w* das \w Volk|strong="H5971"\w*.  
