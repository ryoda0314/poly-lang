\v 21  \w Sebah|strong="H2078"\w* aber und \w Zalmuna|strong="H6759"\w* \w sprachen|strong="H0559"\w*: Stehe \w du|strong="H0859"\w* \w auf|strong="H6965"\w* und mache dich an \w uns|strong="H6293"\w*; \w denn|strong="H3588"\w* darnach der \w Mann|strong="H0376"\w* ist, ist auch seine \w Kraft|strong="H1369"\w*. Also stand \w Gideon|strong="H1439"\w* \w auf|strong="H6965"\w* und \w erwürgte|strong="H2026"\w* \w Sebah|strong="H2078"\w* und \w Zalmuna|strong="H6759"\w* und \w nahm|strong="H3947"\w* die \w Spangen|strong="H7720"\w*, \w die|strong="H0834"\w* an ihrer \w Kamele|strong="H1581"\w* \w Hälsen|strong="H6677"\w* waren.  
\v 22 Da \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* \w Gideon|strong="H1439"\w* \w etliche|strong="H0376"\w* in \w Israel|strong="H3478"\w*: Sei \w Herr|strong="H4910"\w* über \w uns|strong="H1571"\w*, \w du|strong="H0859"\w* \w und|strong="H1571"\w* dein \w Sohn|strong="H1121"\w* \w und|strong="H1571"\w* deines \w Sohnes|strong="H1121"\w* \w Sohn|strong="H1121"\w*, \w weil|strong="H3588"\w* du uns von der \w Midianiter|strong="H4080"\w* \w Hand|strong="H3027"\w* erlöst \w hast|strong="H3467"\w*.  
\v 23 Aber \w Gideon|strong="H1439"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihnen: \w Ich|strong="H0589"\w* will \w nicht|strong="H3808"\w* Herr \w sein|strong="H4910"\w* über euch, und auch mein \w Sohn|strong="H1121"\w* soll \w nicht|strong="H3808"\w* Herr über euch \w sein|strong="H4910"\w*, sondern der \w HERR|strong="H3068"\w* soll Herr über euch \w sein|strong="H4910"\w*.  
\v 24  \w Gideon|strong="H1439"\w* aber \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* Ihnen: \w Eins|strong="H7596"\w* \w begehre|strong="H7592"\w* ich \w von|strong="H4480"\w* euch: ein \w jeglicher|strong="H0376"\w* \w gebe|strong="H5414"\w* mir die \w Stirnbänder|strong="H5141"\w*, die er geraubt \w hat|strong="H7998"\w*. (\w Denn|strong="H3588"\w* \w weil|strong="H3588"\w* \w es|strong="H1992"\w* \w Ismaeliter|strong="H3459"\w* waren, hatten sie \w goldene|strong="H2091"\w* \w Stirnbänder|strong="H5141"\w*.)  
\v 25 Sie \w sprachen|strong="H0559"\w*: Die wollen \w wir|strong="H5414"\w* \w geben|strong="H5414"\w*; und \w breiteten|strong="H6566"\w* ein \w Kleid|strong="H8071"\w* aus, und ein \w jeglicher|strong="H0376"\w* \w warf|strong="H7993"\w* die \w Stirnbänder|strong="H5141"\w* \w darauf|strong="H8033"\w*, die er geraubt \w hatte|strong="H7998"\w*.  
\v 26 Und die \w goldenen|strong="H2091"\w* \w Stirnbänder|strong="H5141"\w*, \w die|strong="H0834"\w* er \w forderte|strong="H7592"\w*, \w machten|strong="H1961"\w* am \w Gewicht|strong="H4948"\w* \w tausendsiebenhundert|strong="H0505"\w* Lot \w Gold|strong="H2091"\w*, \w ohne|strong="H0909"\w* die \w Spangen|strong="H7720"\w* und \w Ketten|strong="H5188"\w* und \w Purpurkleider|strong="H0713"\w*, \w die|strong="H5921"\w* der \w Midianiter|strong="H4080"\w* \w Könige|strong="H4428"\w* tragen, und \w ohne|strong="H0909"\w* \w die|strong="H0834"\w* \w Halsbänder|strong="H6060"\w* ihrer \w Kamele|strong="H1581"\w*.  
\v 27 Und \w Gideon|strong="H1439"\w* \w machte|strong="H6213"\w* einen \w Leibrock|strong="H0646"\w* \w daraus|strong="H0853"\w* und \w setzte|strong="H3322"\w* \w ihn|strong="H0853"\w* in seine \w Stadt|strong="H5892"\w* zu \w Ophra|strong="H6084"\w*. Und \w ganz|strong="H3605"\w* \w Israel|strong="H3478"\w* \w trieb|strong="H2181"\w* damit \w Abgötterei|strong="H0310"\w* \w daselbst|strong="H8033"\w*, und er \w geriet|strong="H1961"\w* \w Gideon|strong="H1439"\w* und seinem \w Hause|strong="H1004"\w* zum \w Fall|strong="H4170"\w*.  
\v 28 Also wurden die \w Midianiter|strong="H4080"\w* \w gedemütigt|strong="H3665"\w* \w vor|strong="H6440"\w* den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w* und hoben ihren \w Kopf|strong="H7218"\w* \w nicht|strong="H3808"\w* \w mehr|strong="H3254"\w* \w empor|strong="H5375"\w*. Und das \w Land|strong="H0776"\w* war \w still|strong="H8252"\w* \w vierzig|strong="H0705"\w* \w Jahre|strong="H8141"\w*, solange \w Gideon|strong="H1439"\w* \w lebte|strong="H3117"\w*.  
\v 29 Und \w Jerubbaal|strong="H3378"\w*, der \w Sohn|strong="H1121"\w* des \w Joas|strong="H3101"\w*, ging \w hin|strong="H3212"\w* und \w wohnte|strong="H3427"\w* in seinem \w Hause|strong="H1004"\w*.  
\v 30 Und \w Gideon|strong="H1439"\w* \w hatte|strong="H1961"\w* \w siebzig|strong="H7657"\w* \w Söhne|strong="H1121"\w*, die aus seiner \w Hüfte|strong="H3409"\w* gekommen \w waren|strong="H3318"\w*; \w denn|strong="H3588"\w* er \w hatte|strong="H1961"\w* \w viele|strong="H7227"\w* \w Weiber|strong="H0802"\w*.  
\v 31 Und sein \w Kebsweib|strong="H6370"\w*, \w das|strong="H0834"\w* er zu \w Sichem|strong="H7927"\w* hatte, \w gebar|strong="H3205"\w* ihm \w auch|strong="H1571"\w* einen \w Sohn|strong="H1121"\w*; den \w nannte|strong="H7760"\w* \w er|strong="H8034"\w* \w Abimelech|strong="H0040"\w*.  
\v 32 Und \w Gideon|strong="H1439"\w*, der \w Sohn|strong="H1121"\w* des \w Joas|strong="H3101"\w*, \w starb|strong="H4191"\w* in \w gutem|strong="H2896"\w* \w Alter|strong="H7872"\w* und ward \w begraben|strong="H6912"\w* in seines \w Vaters|strong="H0001"\w* \w Joas|strong="H3101"\w* \w Grab|strong="H6913"\w* zu \w Ophra|strong="H6084"\w*, der Stadt der \w Abiesriter|strong="H0033"\w*.  
\v 33  \w Da|strong="H1961"\w* aber \w Gideon|strong="H1439"\w* gestorben \w war|strong="H4191"\w*, \w kehrten|strong="H7725"\w* sich die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w um|strong="H7725"\w* und \w liefen|strong="H2181"\w* dem \w Baalim|strong="H1168"\w* \w nach|strong="H2181"\w* und \w machten|strong="H7760"\w* sich Baal-\w Berith|strong="H1170"\w* zum \w Gott|strong="H0430"\w*.  
\v 34 Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w gedachten|strong="H2142"\w* \w nicht|strong="H3808"\w* an den \w HERRN|strong="H3068"\w*, ihren \w Gott|strong="H0430"\w*, der \w sie|strong="H0853"\w* errettet \w hatte|strong="H5337"\w* von der \w Hand|strong="H3027"\w* \w aller|strong="H3605"\w* ihrer \w Feinde|strong="H0341"\w* \w umher|strong="H4480"\w*,  
\v 35 und \w taten|strong="H6213"\w* \w nicht|strong="H3808"\w* \w Barmherzigkeit|strong="H2617"\w* \w an|strong="H5973"\w* dem \w Hause|strong="H1004"\w* des \w Jerubbaal|strong="H3378"\w* \w Gideon|strong="H1439"\w*, \w wie|strong="H0834"\w* er \w alles|strong="H3605"\w* \w Gute|strong="H2896"\w* \w an|strong="H5973"\w* \w Israel|strong="H3478"\w* getan \w hatte|strong="H6213"\w*.   
