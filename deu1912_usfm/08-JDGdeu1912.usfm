\v 18 und \w ihr|strong="H0859"\w* \w lehnt|strong="H6965"\w* euch \w auf|strong="H6965"\w* \w heute|strong="H3117"\w* \w wider|strong="H5921"\w* meines \w Vaters|strong="H0001"\w* \w Haus|strong="H1004"\w* und \w erwürgt|strong="H2026"\w* seine \w Kinder|strong="H1121"\w*, \w siebzig|strong="H7657"\w* \w Mann|strong="H0376"\w*, \w auf|strong="H5921"\w* \w einem|strong="H0259"\w* \w Stein|strong="H0068"\w* und macht euch \w Abimelech|strong="H0040"\w*, seiner \w Magd|strong="H0519"\w* \w Sohn|strong="H1121"\w*, zum \w König|strong="H4427"\w* \w über|strong="H5921"\w* die \w Männer|strong="H1167"\w* zu \w Sichem|strong="H7927"\w*, \w weil|strong="H3588"\w* \w er|strong="H1931"\w* euer \w Bruder|strong="H0251"\w* ist);  
\v 19  \w und|strong="H0518"\w* habt ihr nun \w recht|strong="H0571"\w* und \w redlich|strong="H8549"\w* \w gehandelt|strong="H6213"\w* \w an|strong="H5973"\w* \w Jerubbaal|strong="H3378"\w* und \w an|strong="H5973"\w* seinem \w Hause|strong="H1004"\w* an \w diesem|strong="H2088"\w* \w Tage|strong="H3117"\w*: so seid \w fröhlich|strong="H8055"\w* über \w Abimelech|strong="H0040"\w* und \w er|strong="H1931"\w* \w sei|strong="H1571"\w* \w fröhlich|strong="H8055"\w* über euch;  
\v 20  \w wo|strong="H0518"\w* \w nicht|strong="H0369"\w*, so \w gehe|strong="H3318"\w* \w Feuer|strong="H0784"\w* \w aus|strong="H3318"\w* \w von|strong="H4480"\w* \w Abimelech|strong="H0040"\w* und \w verzehre|strong="H0398"\w* die \w Männer|strong="H1167"\w* zu \w Sichem|strong="H7927"\w* und das \w Haus|strong="H1004"\w* \w Millo|strong="H4407"\w*, und \w gehe|strong="H3318"\w* auch \w Feuer|strong="H0784"\w* \w aus|strong="H3318"\w* \w von|strong="H4480"\w* den \w Männern|strong="H1167"\w* zu \w Sichem|strong="H7927"\w* und \w vom|strong="H4480"\w* \w Haus|strong="H1004"\w* \w Millo|strong="H4407"\w* und \w verzehre|strong="H0398"\w* \w Abimelech|strong="H0040"\w*.  
\v 21 Und \w Jotham|strong="H3147"\w* \w floh|strong="H1272"\w* \w vor|strong="H4480"\w* seinem \w Bruder|strong="H0251"\w* \w Abimelech|strong="H0040"\w* und \w entwich|strong="H5127"\w* und \w ging|strong="H3212"\w* gen \w Beer|strong="H0876"\w* und \w wohnte|strong="H3427"\w* \w daselbst|strong="H8033"\w*.  
\v 22 Als nun \w Abimelech|strong="H0040"\w* \w drei|strong="H7969"\w* \w Jahre|strong="H8141"\w* \w über|strong="H5921"\w* \w Israel|strong="H3478"\w* geherrscht \w hatte|strong="H7786"\w*,  
\v 23  \w sandte|strong="H7971"\w* \w Gott|strong="H0430"\w* einen \w bösen|strong="H7451"\w* \w Willen|strong="H7307"\w* \w zwischen|strong="H0996"\w* \w Abimelech|strong="H0040"\w* \w und|strong="H0996"\w* den \w Männern|strong="H1167"\w* zu \w Sichem|strong="H7927"\w*. Und die \w Männer|strong="H1167"\w* zu \w Sichem|strong="H7927"\w* wurden \w Abimelech|strong="H0040"\w* \w untreu|strong="H0898"\w*,  
\v 24 auf daß der \w Frevel|strong="H2555"\w*, an den \w siebzig|strong="H7657"\w* \w Söhnen|strong="H1121"\w* \w Jerubbaals|strong="H3378"\w* \w begangen|strong="H0935"\w*, und ihr \w Blut|strong="H1818"\w* \w käme|strong="H7760"\w* \w auf|strong="H5921"\w* \w Abimelech|strong="H0040"\w*, ihren \w Bruder|strong="H0251"\w*, \w der|strong="H0834"\w* \w sie|strong="H0853"\w* erwürgt \w hatte|strong="H2026"\w*, und \w auf|strong="H5921"\w* die \w Männer|strong="H1167"\w* zu \w Sichem|strong="H7927"\w*, \w die|strong="H0834"\w* ihm seine \w Hand|strong="H3027"\w* dazu gestärkt \w hatten|strong="H2388"\w*, daß er seine \w Brüder|strong="H0251"\w* \w erwürgte|strong="H2026"\w*.  
\v 25 Und die \w Männer|strong="H1167"\w* zu \w Sichem|strong="H7927"\w* \w stellten|strong="H7760"\w* einen \w Hinterhalt|strong="H0693"\w* \w auf|strong="H5921"\w* den \w Spitzen|strong="H7218"\w* der \w Berge|strong="H2022"\w* und \w beraubten|strong="H1497"\w* \w alle|strong="H3605"\w*, \w die|strong="H0834"\w* \w auf|strong="H5921"\w* der \w Straße|strong="H1870"\w* zu ihnen \w wandelten|strong="H5674"\w*. Und es ward \w Abimelech|strong="H0040"\w* \w angesagt|strong="H5046"\w*.  
\v 26 Es \w kam|strong="H0935"\w* aber \w Gaal|strong="H1603"\w*, der \w Sohn|strong="H1121"\w* \w Ebeds|strong="H5651"\w*, und seine \w Brüder|strong="H0251"\w* und \w zogen|strong="H5674"\w* zu \w Sichem|strong="H7927"\w* ein. Und die \w Männer|strong="H1167"\w* von \w Sichem|strong="H7927"\w* verließen \w sich|strong="H0982"\w* auf ihn  
\v 27 und zogen \w heraus|strong="H3318"\w* aufs \w Feld|strong="H7704"\w* und lasen \w ab|strong="H1219"\w* ihre \w Weinberge|strong="H3754"\w* und \w kelterten|strong="H1869"\w* und \w machten|strong="H6213"\w* einen \w Tanz|strong="H1974"\w* und \w gingen|strong="H0935"\w* in ihres \w Gottes|strong="H0430"\w* \w Haus|strong="H1004"\w* und \w aßen|strong="H0398"\w* und \w tranken|strong="H8354"\w* und \w fluchten|strong="H7043"\w* dem \w Abimelech|strong="H0040"\w*.  
\v 28 Und \w Gaal|strong="H1603"\w*, der \w Sohn|strong="H1121"\w* \w Ebeds|strong="H5651"\w*, \w sprach|strong="H0559"\w*: \w Wer|strong="H4310"\w* ist \w Abimelech|strong="H0040"\w*, und \w was|strong="H4310"\w* ist \w Sichem|strong="H7927"\w*, \w daß|strong="H3588"\w* wir ihm dienen \w sollten|strong="H5647"\w*? Ist er \w nicht|strong="H3808"\w* \w Jerubbaals|strong="H3378"\w* \w Sohn|strong="H1121"\w* und hat \w Sebul|strong="H2083"\w*, seinen \w Knecht|strong="H6496"\w*, \w hergesetzt|strong="H5647"\w*? Dienet den \w Leuten|strong="H0582"\w* \w Hemors|strong="H2544"\w*, des \w Vaters|strong="H0001"\w* \w Sichems|strong="H7927"\w*! \w Warum|strong="H4069"\w* sollten \w wir|strong="H0587"\w* jenen \w dienen|strong="H5647"\w*?  
\v 29 Wollte \w Gott|strong="H4310"\w*, \w das|strong="H2088"\w* \w Volk|strong="H5971"\w* wäre unter meiner \w Hand|strong="H3027"\w*, daß ich \w Abimelech|strong="H0040"\w* \w vertriebe|strong="H5493"\w*! Und es ward \w Abimelech|strong="H0040"\w* \w gesagt|strong="H0559"\w*: \w Mehre|strong="H7235"\w* dein \w Heer|strong="H6635"\w* und zieh \w aus|strong="H3318"\w*!  
\v 30 Denn \w Sebul|strong="H2083"\w*, der \w Oberste|strong="H8269"\w* in der \w Stadt|strong="H5892"\w*, da er die \w Worte|strong="H1697"\w* \w Gaals|strong="H1603"\w*, des \w Sohnes|strong="H1121"\w* \w Ebeds|strong="H5651"\w*, \w hörte|strong="H8085"\w*, \w ergrimmte|strong="H2734"\w* er in seinem \w Zorn|strong="H0639"\w*  
\v 31 und \w sandte|strong="H7971"\w* \w Botschaft|strong="H4397"\w* \w zu|strong="H0413"\w* \w Abimelech|strong="H0040"\w* \w heimlich|strong="H8649"\w* und ließ ihm \w sagen|strong="H0559"\w*: \w Siehe|strong="H2009"\w*, \w Gaal|strong="H1603"\w*, der \w Sohn|strong="H1121"\w* \w Ebeds|strong="H5651"\w*, und seine \w Brüder|strong="H0251"\w* sind gen \w Sichem|strong="H7927"\w* \w gekommen|strong="H0935"\w* \w und|strong="H2009"\w* machen \w dir|strong="H5921"\w* die \w Stadt|strong="H5892"\w* \w aufrührerisch|strong="H6696"\w*.  
\v 32 So \w mache|strong="H6965"\w* dich \w nun|strong="H6258"\w* \w auf|strong="H6965"\w* bei der \w Nacht|strong="H3915"\w*, \w du|strong="H0859"\w* und dein \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w bei|strong="H0854"\w* dir ist, und mache einen \w Hinterhalt|strong="H0693"\w* auf sie im \w Felde|strong="H7704"\w*.  
\v 33  \w Und|strong="H1961"\w* des \w Morgens|strong="H1242"\w*, wenn die \w Sonne|strong="H8121"\w* \w aufgeht|strong="H2224"\w*, so mache dich früh \w auf|strong="H7925"\w* und \w überfalle|strong="H6584"\w* die \w Stadt|strong="H5892"\w*. \w Und|strong="H2009"\w* wo \w er|strong="H1931"\w* und das \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w bei|strong="H0854"\w* ihm ist, \w zu|strong="H0413"\w* dir \w hinauszieht|strong="H3318"\w*, so \w tue|strong="H6213"\w* mit ihm, \w wie|strong="H0834"\w* es deine \w Hand|strong="H3027"\w* \w findet|strong="H4672"\w*.  
\v 34  \w Abimelech|strong="H0040"\w* stand \w auf|strong="H6965"\w* bei der \w Nacht|strong="H3915"\w* und \w alles|strong="H3605"\w* \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w bei|strong="H0854"\w* ihm war, und \w hielt|strong="H0693"\w* \w auf|strong="H5921"\w* \w Sichem|strong="H7927"\w* mit \w vier|strong="H0702"\w* \w Haufen|strong="H7218"\w*.  
\v 35 Und \w Gaal|strong="H1603"\w*, der \w Sohn|strong="H1121"\w* \w Ebeds|strong="H5651"\w*, zog \w heraus|strong="H3318"\w* und \w trat|strong="H5975"\w* vor die \w Tür|strong="H6607"\w* an der Stadt \w Tor|strong="H8179"\w*. Aber \w Abimelech|strong="H0040"\w* machte sich \w auf|strong="H6965"\w* \w aus|strong="H4480"\w* dem \w Hinterhalt|strong="H3993"\w* samt dem \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w mit|strong="H0854"\w* ihm war.  
\v 36 Da nun \w Gaal|strong="H1603"\w* das \w Volk|strong="H5971"\w* \w sah|strong="H7200"\w*, sprach \w er|strong="H0559"\w* \w zu|strong="H0413"\w* \w Sebul|strong="H2083"\w*: \w Siehe|strong="H2009"\w*, da \w kommt|strong="H3381"\w* ein \w Volk|strong="H5971"\w* \w von|strong="H4480"\w* der \w Höhe|strong="H7218"\w* des \w Gebirges|strong="H2022"\w* hernieder. \w Sebul|strong="H2083"\w* aber \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* Ihm: \w Du|strong="H0859"\w* \w siehst|strong="H7200"\w* die \w Schatten|strong="H6738"\w* der \w Berge|strong="H2022"\w* für \w Leute|strong="H0582"\w* an.  
\v 37  \w Gaal|strong="H1603"\w* \w redete|strong="H1696"\w* \w noch|strong="H5750"\w* \w mehr|strong="H3254"\w* und \w sprach|strong="H0559"\w*: \w Siehe|strong="H2009"\w*, ein \w Volk|strong="H5971"\w* kommt \w hernieder|strong="H3381"\w* \w aus|strong="H4480"\w* der \w Mitte|strong="H5973"\w* des \w Landes|strong="H0776"\w*, und \w ein|strong="H0259"\w* \w Haufe|strong="H7218"\w* \w kommt|strong="H0935"\w* \w auf|strong="H4480"\w* dem \w Wege|strong="H1870"\w* zur \w Zaubereiche|strong="H0436"\w*.  
\v 38 Da \w sprach|strong="H0559"\w* \w Sebul|strong="H2083"\w* \w zu|strong="H0413"\w* ihm: \w Wo|strong="H0346"\w* ist \w nun|strong="H0645"\w* hier dein \w Maul|strong="H6310"\w*, \w das|strong="H0834"\w* da \w sagte|strong="H0559"\w*: \w Wer|strong="H4310"\w* ist \w Abimelech|strong="H0040"\w*, \w daß|strong="H3588"\w* wir ihm dienen \w sollten|strong="H5647"\w*? Ist das \w nicht|strong="H3808"\w* \w das|strong="H2088"\w* \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* du verachtet \w hast|strong="H3988"\w*? Zieh \w nun|strong="H6258"\w* \w aus|strong="H3318"\w* und \w streite|strong="H3898"\w* mit ihm!  
\v 39  \w Gaal|strong="H1603"\w* zog \w aus|strong="H3318"\w* \w vor|strong="H6440"\w* den \w Männern|strong="H1167"\w* zu \w Sichem|strong="H7927"\w* her und \w stritt|strong="H3898"\w* mit \w Abimelech|strong="H0040"\w*.  
\v 40 Aber \w Abimelech|strong="H0040"\w* jagte \w ihn|strong="H7291"\w*, \w daß|strong="H3588"\w* er \w floh|strong="H5127"\w* \w vor|strong="H4480"\w* \w ihm|strong="H6440"\w*; und \w fielen|strong="H5307"\w* \w viel|strong="H7227"\w* \w Erschlagene|strong="H2491"\w* \w bis|strong="H5704"\w* an die \w Tür|strong="H6607"\w* des \w Tors|strong="H8179"\w*.  
\v 41 Und \w Abimelech|strong="H0040"\w* \w blieb|strong="H3427"\w* zu \w Aruma|strong="H0725"\w*. \w Sebul|strong="H2083"\w* aber \w verjagte|strong="H1644"\w* den \w Gaal|strong="H1603"\w* und seine \w Brüder|strong="H0251"\w*, die zu \w Sichem|strong="H7927"\w* \w nicht|strong="H4480"\w* durften \w bleiben|strong="H3427"\w*.  
\v 42 Am \w Morgen|strong="H4283"\w* \w aber|strong="H1961"\w* \w ging|strong="H3318"\w* das \w Volk|strong="H5971"\w* \w heraus|strong="H3318"\w* aufs \w Feld|strong="H7704"\w*. Da das \w Abimelech|strong="H0040"\w* ward \w angesagt|strong="H5046"\w*,  
\v 43 nahm \w er|strong="H3947"\w* das \w Kriegsvolk|strong="H5971"\w* und \w teilte|strong="H2673"\w* es in \w drei|strong="H7969"\w* \w Haufen|strong="H7218"\w* und machte einen \w Hinterhalt|strong="H0693"\w* auf sie im \w Felde|strong="H7704"\w*. Als er nun \w sah|strong="H7200"\w*, daß das \w Volk|strong="H5971"\w* \w aus|strong="H4480"\w* der \w Stadt|strong="H5892"\w* \w ging|strong="H3318"\w*, erhob er sich \w über|strong="H5921"\w* \w sie|strong="H6965"\w* und \w schlug|strong="H5221"\w* sie.  
\v 44  \w Abimelech|strong="H0040"\w* aber und die \w Haufen|strong="H7218"\w*, \w die|strong="H0834"\w* \w bei|strong="H5973"\w* ihm waren, \w überfielen|strong="H6584"\w* sie und \w traten|strong="H5975"\w* an die \w Tür|strong="H6607"\w* des \w Stadttors|strong="H8179"\w*; und \w zwei|strong="H8147"\w* der \w Haufen|strong="H7218"\w* \w überfielen|strong="H6584"\w* \w alle|strong="H3605"\w*, \w die|strong="H0834"\w* auf dem \w Felde|strong="H7704"\w* waren, und \w schlugen|strong="H5221"\w* sie.  
\v 45 Da \w stritt|strong="H3898"\w* \w Abimelech|strong="H0040"\w* wider die \w Stadt|strong="H5892"\w* \w denselben|strong="H1931"\w* \w Tag|strong="H3117"\w* und \w gewann|strong="H3920"\w* \w sie|strong="H5892"\w* und \w erwürgte|strong="H2026"\w* das \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* darin war, und \w zerbrach|strong="H5422"\w* die \w Stadt|strong="H5892"\w* und \w säte|strong="H2232"\w* \w Salz|strong="H4417"\w* darauf.  
\v 46 Da das \w hörten|strong="H8085"\w* \w alle|strong="H3605"\w* \w Männer|strong="H1167"\w* des \w Turms|strong="H4026"\w* zu \w Sichem|strong="H7927"\w*, gingen \w sie|strong="H0935"\w* \w in|strong="H0413"\w* die \w Festung|strong="H6877"\w* des \w Hauses|strong="H1004"\w* des \w Gottes|strong="H0410"\w* \w Berith|strong="H1286"\w*.  
\v 47 Da das \w Abimelech|strong="H0040"\w* \w hörte|strong="H5046"\w*, \w daß|strong="H3588"\w* sich \w alle|strong="H3605"\w* \w Männer|strong="H1167"\w* des \w Turms|strong="H4026"\w* zu \w Sichem|strong="H7927"\w* versammelt \w hatten|strong="H6908"\w*,  
\v 48  \w ging|strong="H5927"\w* \w er|strong="H0040"\w* \w auf|strong="H5927"\w* den \w Berg|strong="H2022"\w* \w Zalmon|strong="H6756"\w* \w mit|strong="H1931"\w* \w allem|strong="H3605"\w* seinem \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w bei|strong="H0854"\w* ihm war \w und|strong="H0040"\w* \w nahm|strong="H3947"\w* eine \w Axt|strong="H7134"\w* in seine \w Hand|strong="H3027"\w* und \w hieb|strong="H3772"\w* einen \w Ast|strong="H7754"\w* von den \w Bäumen|strong="H6086"\w* und \w legte|strong="H5375"\w* ihn \w auf|strong="H5921"\w* seine \w Achsel|strong="H7926"\w* \w und|strong="H7760"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* allem \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w mit|strong="H5973"\w* ihm war: \w Was|strong="H4100"\w* ihr gesehen \w habt|strong="H7200"\w*, daß ich \w tue|strong="H6213"\w*, das \w tut|strong="H6213"\w* auch ihr \w eilend|strong="H4116"\w* wie \w ich|strong="H3644"\w*.  
\v 49 Da \w hieb|strong="H3772"\w* \w alles|strong="H3605"\w* \w Volk|strong="H5971"\w* ein \w jeglicher|strong="H0376"\w* einen \w Ast|strong="H7754"\w* ab und \w folgten|strong="H3212"\w* \w Abimelech|strong="H0040"\w* \w nach|strong="H0310"\w* und \w legten|strong="H7760"\w* sie \w an|strong="H5921"\w* die \w Festung|strong="H6877"\w* und steckten'\w s|strong="H3341"\w* \w an|strong="H5921"\w* mit \w Feuer|strong="H0784"\w*, daß \w auch|strong="H1571"\w* \w alle|strong="H3605"\w* \w Männer|strong="H0582"\w* des \w Turms|strong="H4026"\w* zu \w Sichem|strong="H7927"\w* \w starben|strong="H4191"\w*, bei \w tausend|strong="H0505"\w* \w Mann|strong="H0376"\w* und \w Weib|strong="H0802"\w*.  
\v 50  \w Abimelech|strong="H0040"\w* aber \w zog|strong="H3212"\w* \w gen|strong="H0413"\w* \w Thebez|strong="H8405"\w* und \w belagerte|strong="H2583"\w* \w es|strong="H8405"\w* und gewann \w es|strong="H3920"\w*.  
\v 51 Es \w war|strong="H1961"\w* aber ein \w starker|strong="H5797"\w* \w Turm|strong="H4026"\w* \w mitten|strong="H8432"\w* in der \w Stadt|strong="H5892"\w*. Auf den \w flohen|strong="H5127"\w* \w alle|strong="H3605"\w* \w Männer|strong="H0582"\w* und \w Weiber|strong="H0802"\w* und \w alle|strong="H3605"\w* \w Bürger|strong="H1167"\w* der \w Stadt|strong="H5892"\w* und \w schlossen|strong="H5462"\w* \w hinter|strong="H1157"\w* sich zu und \w stiegen|strong="H5927"\w* \w auf|strong="H5921"\w* das \w Dach|strong="H1406"\w* des \w Turms|strong="H4026"\w*.  
\v 52 Da \w kam|strong="H0935"\w* \w Abimelech|strong="H0040"\w* \w zum|strong="H5704"\w* \w Turm|strong="H4026"\w* und \w stritt|strong="H3898"\w* dawider und \w nahte|strong="H5066"\w* sich \w zur|strong="H5704"\w* \w Tür|strong="H6607"\w* des \w Turms|strong="H4026"\w*, daß er ihn mit \w Feuer|strong="H0784"\w* \w verbrannte|strong="H8313"\w*.  
\v 53 Aber \w ein|strong="H0259"\w* \w Weib|strong="H0802"\w* \w warf|strong="H7993"\w* einen \w Mühlstein|strong="H6400"\w* \w Abimelech|strong="H0040"\w* \w auf|strong="H5921"\w* den \w Kopf|strong="H7218"\w* und \w zerbrach|strong="H7533"\w* ihm den \w Schädel|strong="H1538"\w*.  
\v 54 Da \w rief|strong="H7121"\w* Abimelech \w eilend|strong="H4120"\w* dem \w Diener|strong="H5288"\w*, der seine \w Waffen|strong="H3627"\w* \w trug|strong="H5375"\w*, und \w sprach|strong="H0559"\w* zu ihm: \w Zieh|strong="H8025"\w* dein \w Schwert|strong="H2719"\w* aus und \w töte|strong="H4191"\w* mich, daß man \w nicht|strong="H6435"\w* von mir \w sage|strong="H0559"\w*: Ein \w Weib|strong="H0802"\w* hat ihn \w erwürgt|strong="H2026"\w*. Da \w durchstach|strong="H1856"\w* ihn sein \w Diener|strong="H5288"\w*, und er \w starb|strong="H4191"\w*.  
\v 55 Da \w aber|strong="H0376"\w* die \w Israeliten|strong="H3478"\w*, die mit ihm waren, \w sahen|strong="H7200"\w*, \w daß|strong="H3588"\w* \w Abimelech|strong="H0040"\w* tot \w war|strong="H4191"\w*, \w ging|strong="H3212"\w* ein \w jeglicher|strong="H0376"\w* an seinen \w Ort|strong="H4725"\w*.  
\v 56 Also \w bezahlte|strong="H7725"\w* \w Gott|strong="H0430"\w* \w Abimelech|strong="H0040"\w* das \w Übel|strong="H7451"\w*, \w das|strong="H0834"\w* er an seinem \w Vater|strong="H0001"\w* getan \w hatte|strong="H6213"\w*, da er seine \w siebzig|strong="H7657"\w* \w Brüder|strong="H0251"\w* \w erwürgte|strong="H2026"\w*;  
\v 57 desgleichen \w alles|strong="H3605"\w* \w Übel|strong="H7451"\w* der \w Männer|strong="H0582"\w* \w Sichems|strong="H7927"\w* \w vergalt|strong="H7725"\w* ihnen \w Gott|strong="H0430"\w* auf ihren \w Kopf|strong="H7218"\w*, und es \w kam|strong="H0935"\w* über sie der \w Fluch|strong="H7045"\w* \w Jothams|strong="H3147"\w*, des \w Sohnes|strong="H1121"\w* \w Serubbaals|strong="H3378"\w*.   
\c 10  
\p
\v 1  \w Nach|strong="H0310"\w* \w Abimelech|strong="H0040"\w* machte sich \w auf|strong="H6965"\w*, zu \w helfen|strong="H3467"\w* \w Israel|strong="H3478"\w*, \w Thola|strong="H8439"\w*, ein \w Mann|strong="H0376"\w* von \w Isaschar|strong="H3485"\w*, ein \w Sohn|strong="H1121"\w* \w Phuas|strong="H6312"\w*, des \w Sohnes|strong="H1121"\w* \w Dodos|strong="H1734"\w*. Und \w er|strong="H1931"\w* \w wohnte|strong="H3427"\w* zu \w Samir|strong="H8069"\w* auf dem \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w*  
\v 2 und \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w dreiundzwanzig|strong="H7969"\w* \w Jahre|strong="H8141"\w* und \w starb|strong="H4191"\w* und wurde \w begraben|strong="H6912"\w* zu \w Samir|strong="H8069"\w*.  
\v 3  \w Nach|strong="H0310"\w* ihm machte sich \w auf|strong="H6965"\w* \w Jair|strong="H2971"\w*, ein \w Gileaditer|strong="H1569"\w*, und \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w zweiundzwanzig|strong="H8147"\w* \w Jahre|strong="H8141"\w*.  
\v 4 Der \w hatte|strong="H1961"\w* \w dreißig|strong="H7970"\w* \w Söhne|strong="H1121"\w* \w auf|strong="H5921"\w* \w dreißig|strong="H7970"\w* \w Eselsfüllen|strong="H5895"\w* \w reiten|strong="H7392"\w*; und sie hatten \w dreißig|strong="H7970"\w* \w Städte|strong="H5892"\w*, die \w hießen|strong="H7121"\w* Dörfer \w Jairs|strong="H2334"\w* bis \w auf|strong="H5704"\w* \w diesen|strong="H2088"\w* \w Tag|strong="H3117"\w* und \w liegen|strong="H0776"\w* in \w Gilead|strong="H1568"\w*.  
\v 5 Und \w Jair|strong="H2971"\w* \w starb|strong="H4191"\w* und ward \w begraben|strong="H6912"\w* zu \w Kamon|strong="H7056"\w*.  
\v 6 Aber die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w taten|strong="H6213"\w* \w fürder|strong="H3254"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w* und \w dienten|strong="H5647"\w* den \w Baalim|strong="H1168"\w* und den \w Astharoth|strong="H6252"\w* und den \w Göttern|strong="H0430"\w* von \w Syrien|strong="H0758"\w* und den \w Göttern|strong="H0430"\w* von \w Sidon|strong="H6721"\w* und den \w Göttern|strong="H0430"\w* \w Moabs|strong="H4124"\w* und den \w Göttern|strong="H0430"\w* der \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* und den \w Göttern|strong="H0430"\w* der \w Philister|strong="H6430"\w* und \w verließen|strong="H5800"\w* den \w HERRN|strong="H3068"\w* und \w dienten|strong="H5647"\w* ihm \w nicht|strong="H3808"\w*.  
\v 7 Da \w ergrimmte|strong="H2734"\w* der \w Zorn|strong="H0639"\w* des \w HERRN|strong="H3068"\w* über \w Israel|strong="H3478"\w*, und er \w verkaufte|strong="H4376"\w* sie unter die \w Hand|strong="H3027"\w* der \w Philister|strong="H6430"\w* \w und|strong="H3027"\w* der \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*.  
\v 8 Und sie \w zertraten|strong="H7492"\w* und \w zerschlugen|strong="H7533"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w von|strong="H1931"\w* dem \w Jahr|strong="H8141"\w* an wohl \w achtzehn|strong="H8083"\w* \w Jahre|strong="H8141"\w*, nämlich \w alle|strong="H3605"\w* \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w jenseit|strong="H5676"\w* des \w Jordans|strong="H3383"\w*, im \w Lande|strong="H0776"\w* der \w Amoriter|strong="H0567"\w*, \w das|strong="H0834"\w* in \w Gilead|strong="H1568"\w* liegt.  
\v 9 Dazu \w zogen|strong="H5674"\w* die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* \w über|strong="H5674"\w* den \w Jordan|strong="H3383"\w* und \w stritten|strong="H3898"\w* \w wider|strong="H1571"\w* \w Juda|strong="H3063"\w*, \w Benjamin|strong="H1144"\w* und das \w Haus|strong="H1004"\w* \w Ephraim|strong="H0669"\w*, also daß \w Israel|strong="H3478"\w* \w sehr|strong="H3966"\w* geängstet \w ward|strong="H3334"\w*.  
\v 10 Da \w schrieen|strong="H2199"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w zu|strong="H0413"\w* dem \w HERRN|strong="H3068"\w* und \w sprachen|strong="H0559"\w*: Wir haben an dir \w gesündigt|strong="H2398"\w*; \w denn|strong="H3588"\w* wir haben unsern \w Gott|strong="H0430"\w* \w verlassen|strong="H5800"\w* und den \w Baalim|strong="H1168"\w* \w gedient|strong="H5647"\w*.  
\v 11 Aber der \w HERR|strong="H3068"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w*: Haben \w euch|strong="H0853"\w* \w nicht|strong="H3808"\w* auch gezwungen \w die|strong="H4480"\w* \w Ägypter|strong="H4714"\w*, \w die|strong="H4480"\w* \w Amoriter|strong="H0567"\w*, \w die|strong="H4480"\w* \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*, \w die|strong="H4480"\w* \w Philister|strong="H6430"\w*,  
\v 12 die \w Sidonier|strong="H6722"\w*, die \w Amalekiter|strong="H6002"\w* und \w Maoniter|strong="H4584"\w*, und ich \w half|strong="H3467"\w* euch \w aus|strong="H4480"\w* ihren \w Händen|strong="H3027"\w*, da ihr \w zu|strong="H0413"\w* mir \w schrieet|strong="H6817"\w*?  
\v 13 Und doch habt \w ihr|strong="H0859"\w* \w mich|strong="H0853"\w* \w verlassen|strong="H5800"\w* und \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w* \w gedient|strong="H5647"\w*; \w darum|strong="H3651"\w* will ich euch \w nicht|strong="H3808"\w* \w mehr|strong="H3254"\w* \w helfen|strong="H3467"\w*.  
\v 14 Geht \w hin|strong="H3212"\w* und \w schreit|strong="H2199"\w* die \w Götter|strong="H0430"\w* \w an|strong="H0413"\w*, \w die|strong="H0834"\w* ihr erwählt \w habt|strong="H0977"\w*; \w laßt|strong="H1992"\w* euch dieselben \w helfen|strong="H3467"\w* zur \w Zeit|strong="H6256"\w* eurer \w Trübsal|strong="H6869"\w*.  
\v 15 Aber die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* dem \w HERRN|strong="H3068"\w*: Wir haben \w gesündigt|strong="H2398"\w*, \w mache|strong="H6213"\w* \w es|strong="H0859"\w* nur du mit \w uns|strong="H3605"\w*, \w wie|strong="H2896"\w* es dir \w gefällt|strong="H5869"\w*; \w allein|strong="H0389"\w* \w errette|strong="H5337"\w* \w uns|strong="H4994"\w* zu \w dieser|strong="H2088"\w* \w Zeit|strong="H3117"\w*.  
\v 16 Und sie \w taten|strong="H5493"\w* von \w sich|strong="H5493"\w* die \w fremden|strong="H5236"\w* \w Götter|strong="H0430"\w* und \w dienten|strong="H5647"\w* dem \w HERRN|strong="H3068"\w*. Und es \w jammerte|strong="H7114"\w* \w ihn|strong="H5315"\w*, daß \w Israel|strong="H3478"\w* so geplagt \w ward|strong="H5999"\w*.  
\v 17 Und die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* kamen \w zuhauf|strong="H6817"\w* und lagerten \w sich|strong="H2583"\w* in \w Gilead|strong="H1568"\w*; aber die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* versammelten \w sich|strong="H0622"\w* und lagerten \w sich|strong="H2583"\w* zu \w Mizpa|strong="H4709"\w*.  
\v 18 Und die \w Obersten|strong="H8269"\w* des \w Volks|strong="H5971"\w* zu \w Gilead|strong="H1568"\w* \w sprachen|strong="H0559"\w* \w untereinander|strong="H0376"\w*: \w Welcher|strong="H4310"\w* \w anfängt|strong="H2490"\w* zu \w streiten|strong="H3898"\w* wider die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*, der soll das \w Haupt|strong="H7218"\w* \w sein|strong="H1961"\w* über \w alle|strong="H3605"\w*, die in \w Gilead|strong="H1568"\w* \w wohnen|strong="H3427"\w*.   
\c 11  
\p
\v 1  \w Jephthah|strong="H3316"\w*, ein \w Gileaditer|strong="H1569"\w*, \w war|strong="H1961"\w* ein \w streibarer|strong="H2428"\w* \w Held|strong="H1368"\w*, \w aber|strong="H1931"\w* ein \w Hurenkind|strong="H1121"\w*. \w Gilead|strong="H1568"\w* aber hatte \w Jephthah|strong="H3316"\w* \w gezeugt|strong="H3205"\w*.  
\v 2 Da aber das \w Weib|strong="H0802"\w* \w Gileads|strong="H1568"\w* ihm \w Kinder|strong="H1121"\w* \w gebar|strong="H3205"\w* und des \w Weibes|strong="H0802"\w* \w Kinder|strong="H1121"\w* groß \w wurden|strong="H1431"\w*, \w stießen|strong="H1644"\w* sie \w Jephthah|strong="H3316"\w* \w aus|strong="H1644"\w* und \w sprachen|strong="H0559"\w* zu ihm: Du sollst \w nicht|strong="H3808"\w* \w erben|strong="H5157"\w* in unsers \w Vaters|strong="H0001"\w* \w Haus|strong="H1004"\w*; \w denn|strong="H3588"\w* \w du|strong="H0859"\w* bist eines \w andern|strong="H0312"\w* \w Weibes|strong="H0802"\w* \w Sohn|strong="H1121"\w*.  
\v 3 Da \w floh|strong="H1272"\w* \w er|strong="H3316"\w* \w vor|strong="H4480"\w* seinen \w Brüdern|strong="H0251"\w* und \w wohnte|strong="H3427"\w* im \w Lande|strong="H0776"\w* \w Tob|strong="H2897"\w*. Und es sammelten \w sich|strong="H3950"\w* \w zu|strong="H0413"\w* \w ihm|strong="H3316"\w* \w lose|strong="H7386"\w* \w Leute|strong="H0582"\w* und zogen \w aus|strong="H3318"\w* \w mit|strong="H5973"\w* \w ihm|strong="H3318"\w*.  
\v 4  \w Und|strong="H1961"\w* \w über|strong="H4480"\w* etliche \w Zeit|strong="H3117"\w* hernach \w stritten|strong="H3898"\w* die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* \w mit|strong="H5973"\w* \w Israel|strong="H3478"\w*.  
\v 5  \w Da|strong="H1961"\w* \w nun|strong="H0834"\w* die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* also \w stritten|strong="H3898"\w* \w mit|strong="H5973"\w* \w Israel|strong="H3478"\w*, \w gingen|strong="H3212"\w* die \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w* \w hin|strong="H3212"\w*, daß sie \w Jephthah|strong="H3316"\w* \w holten|strong="H3947"\w* \w aus|strong="H4480"\w* dem \w Lande|strong="H0776"\w* \w Tob|strong="H2897"\w*,  
\v 6 und \w sprachen|strong="H0559"\w* zu \w ihm|strong="H3316"\w*: \w Komm|strong="H3212"\w* und \w sei|strong="H1961"\w* unser \w Hauptmann|strong="H7101"\w*, daß wir \w streiten|strong="H3898"\w* wider die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*.  
\v 7 Aber \w Jephthah|strong="H3316"\w* \w sprach|strong="H0559"\w* zu den \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w*: Seid \w ihr|strong="H0859"\w* es \w nicht|strong="H3808"\w*, die \w mich|strong="H0853"\w* \w hassen|strong="H8130"\w* und \w aus|strong="H4480"\w* meines \w Vaters|strong="H0001"\w* \w Haus|strong="H1004"\w* gestoßen \w haben|strong="H1644"\w*? \w Und|strong="H4069"\w* \w nun|strong="H6258"\w* \w kommt|strong="H0935"\w* ihr \w zu|strong="H0413"\w* mir, \w weil|strong="H0834"\w* ihr in Trübsal \w seid|strong="H6887"\w*?  
\v 8 Die \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w* \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* \w Jephthah|strong="H3316"\w*: \w Darum|strong="H3651"\w* \w kommen|strong="H7725"\w* wir \w nun|strong="H6258"\w* \w wieder|strong="H7725"\w* \w zu|strong="H0413"\w* dir, daß du \w mit|strong="H5973"\w* uns \w ziehst|strong="H1980"\w* und hilfst uns \w streiten|strong="H3898"\w* wider die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* und \w seist|strong="H1961"\w* unser \w Haupt|strong="H7218"\w* über \w alle|strong="H3605"\w*, die in \w Gilead|strong="H1568"\w* \w wohnen|strong="H3427"\w*.  
\v 9  \w Jephthah|strong="H3316"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* den \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w*: \w So|strong="H0518"\w* \w ihr|strong="H0859"\w* \w mich|strong="H0853"\w* wieder \w holet|strong="H7725"\w*, zu \w streiten|strong="H3898"\w* wider die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*, und der \w HERR|strong="H3068"\w* \w sie|strong="H0853"\w* \w vor|strong="H6440"\w* mir dahingeben \w wird|strong="H5414"\w*, soll \w ich|strong="H0595"\w* dann euer \w Haupt|strong="H7218"\w* \w sein|strong="H1961"\w*?  
\v 10 Die \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w* \w sprachen|strong="H0559"\w* \w zu|strong="H0413"\w* \w Jephthah|strong="H3316"\w*: Der \w HERR|strong="H3068"\w* \w sei|strong="H1961"\w* \w Zuhörer|strong="H8085"\w* \w zwischen|strong="H0996"\w* \w uns|strong="H0518"\w*, wo wir \w nicht|strong="H3808"\w* \w tun|strong="H6213"\w*, \w wie|strong="H3651"\w* du gesagt \w hast|strong="H1697"\w*.  
\v 11 Also \w ging|strong="H3212"\w* \w Jephthah|strong="H3316"\w* \w mit|strong="H5973"\w* den \w Ältesten|strong="H2205"\w* von \w Gilead|strong="H1568"\w*; und das \w Volk|strong="H5971"\w* \w setzte|strong="H7760"\w* \w ihn|strong="H0853"\w* zum \w Haupt|strong="H7218"\w* und \w Obersten|strong="H7101"\w* \w über|strong="H5921"\w* sich. Und \w Jephthah|strong="H3316"\w* \w redete|strong="H1696"\w* \w solches|strong="H1697"\w* \w alles|strong="H3605"\w* \w vor|strong="H6440"\w* dem \w HERRN|strong="H3068"\w* zu \w Mizpa|strong="H4709"\w*.  
\v 12 Da \w sandte|strong="H7971"\w* \w Jephthah|strong="H3316"\w* \w Botschaft|strong="H4397"\w* \w zum|strong="H0413"\w* \w König|strong="H4428"\w* der \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* und ließ ihm \w sagen|strong="H0559"\w*: \w Was|strong="H4100"\w* hast du mit mir zu schaffen, \w daß|strong="H3588"\w* du \w kommst|strong="H0935"\w* \w zu|strong="H0413"\w* mir, wider mein \w Land|strong="H0776"\w* zu \w streiten|strong="H3898"\w*?  
\v 13 Der \w König|strong="H4428"\w* der \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* \w antwortete|strong="H0559"\w* \w den|strong="H0413"\w* \w Boten|strong="H4397"\w* \w Jephthahs|strong="H3316"\w*: \w Darum|strong="H3588"\w* daß \w Israel|strong="H3478"\w* mein \w Land|strong="H0776"\w* genommen \w hat|strong="H3947"\w*, da sie \w aus|strong="H4480"\w* \w Ägypten|strong="H4714"\w* \w zogen|strong="H5927"\w*, vom \w Arnon|strong="H0769"\w* an bis \w an|strong="H5704"\w* den \w Jabbok|strong="H2999"\w* und wieder \w bis|strong="H5704"\w* zum \w Jordan|strong="H3383"\w*; so gib mir'\w s|strong="H0853"\w* \w nun|strong="H6258"\w* \w wieder|strong="H7725"\w* mit \w Frieden|strong="H7965"\w*.  
\v 14  \w Jephthah|strong="H3316"\w* aber \w sandte|strong="H7971"\w* \w noch|strong="H3254"\w* \w mehr|strong="H5750"\w* \w Boten|strong="H4397"\w* \w zum|strong="H0413"\w* \w König|strong="H4428"\w* der \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w*,  
