\v 5  \w Und|strong="H3588"\w* die \w Gileaditer|strong="H1568"\w* \w nahmen|strong="H3920"\w* ein die \w Furten|strong="H4569"\w* des \w Jordans|strong="H3383"\w* vor \w Ephraim|strong="H0669"\w*. \w Wenn|strong="H1961"\w* \w nun|strong="H3588"\w* die \w Flüchtigen|strong="H6412"\w* \w Ephraims|strong="H0669"\w* \w sprachen|strong="H0559"\w*: Laß mich \w hinübergehen|strong="H5674"\w*! so \w sprachen|strong="H0559"\w* die \w Männer|strong="H0582"\w* von \w Gilead|strong="H1568"\w* zu Ihm: Bist \w du|strong="H0859"\w* ein \w Ephraimiter|strong="H0673"\w*? Wenn er dann \w antwortete|strong="H0559"\w*: \w Nein|strong="H3808"\w*!  
\v 6 hießen \w sie|strong="H0559"\w* ihn \w sprechen|strong="H0559"\w*: \w Schiboleth|strong="H7641"\w*; \w so|strong="H4994"\w* sprach \w er|strong="H0559"\w* \w Siboleth|strong="H5451"\w* und konnte \w es|strong="H3651"\w* \w nicht|strong="H3808"\w* \w recht|strong="H3559"\w* \w reden|strong="H1696"\w*; alsdann griffen \w sie|strong="H0270"\w* ihn \w schlugen|strong="H7819"\w* \w ihn|strong="H0853"\w* \w an|strong="H0413"\w* den \w Furten|strong="H4569"\w* des \w Jordans|strong="H3383"\w*, daß zu \w der|strong="H1931"\w* \w Zeit|strong="H6256"\w* \w von|strong="H4480"\w* \w Ephraim|strong="H0669"\w* \w fielen|strong="H5307"\w* \w zweiundvierzigtausend|strong="H8147"\w*.  
\v 7  \w Jephthah|strong="H3316"\w* aber \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w sechs|strong="H8337"\w* \w Jahre|strong="H8141"\w*. Und \w Jephthah|strong="H3316"\w*, der \w Gileaditer|strong="H1569"\w*, \w starb|strong="H4191"\w* und ward \w begraben|strong="H6912"\w* in den \w Städten|strong="H5892"\w* zu \w Gilead|strong="H1568"\w*.  
\v 8  \w Nach|strong="H0310"\w* diesem \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w Ebzan|strong="H0078"\w* \w von|strong="H4480"\w* \w Bethlehem|strong="H1035"\w*.  
\v 9 Der \w hatte|strong="H1961"\w* \w dreißig|strong="H7970"\w* \w Söhne|strong="H1121"\w*, und \w dreißig|strong="H7970"\w* \w Töchter|strong="H1323"\w* \w gab|strong="H7971"\w* er \w hinaus|strong="H2351"\w*, und \w dreißig|strong="H7970"\w* \w Töchter|strong="H1323"\w* \w nahm|strong="H0935"\w* er \w von|strong="H4480"\w* \w außen|strong="H2351"\w* seinen \w Söhnen|strong="H1121"\w*; er \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w sieben|strong="H7651"\w* \w Jahre|strong="H8141"\w*  
\v 10 und \w starb|strong="H4191"\w* und ward \w begraben|strong="H6912"\w* zu \w Bethlehem|strong="H1035"\w*.  
\v 11  \w Nach|strong="H0310"\w* diesem \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w Elon|strong="H0356"\w*, ein \w Sebuloniter|strong="H2075"\w*; er \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w zehn|strong="H6235"\w* \w Jahre|strong="H8141"\w*  
\v 12  \w und|strong="H0356"\w* \w starb|strong="H4191"\w* und ward \w begraben|strong="H6912"\w* zu \w Ajalon|strong="H0357"\w* im \w Lande|strong="H0776"\w* \w Sebulon|strong="H2074"\w*.  
\v 13  \w Nach|strong="H0310"\w* diesem \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w Abdon|strong="H5658"\w*, ein \w Sohn|strong="H1121"\w* \w Hillels|strong="H1985"\w*, ein \w Pirathoniter|strong="H6553"\w*.  
\v 14 der \w hatte|strong="H1961"\w* \w vierzig|strong="H0705"\w* \w Söhne|strong="H1121"\w* und \w dreißig|strong="H7970"\w* \w Enkel|strong="H1121"\w*, die \w auf|strong="H5921"\w* \w siebzig|strong="H7657"\w* \w Eselsfüllen|strong="H5895"\w* \w ritten|strong="H7392"\w*; er \w richtete|strong="H8199"\w* \w Israel|strong="H3478"\w* \w acht|strong="H8083"\w* \w Jahre|strong="H8141"\w*  
\v 15 und \w starb|strong="H4191"\w* und ward \w begraben|strong="H6912"\w* zu \w Pirathon|strong="H6552"\w* im \w Lande|strong="H0776"\w* \w Ephraim|strong="H0669"\w* auf dem \w Gebirge|strong="H2022"\w* der \w Amalekiter|strong="H6003"\w*.   
\c 13  
\p
\v 1 Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w taten|strong="H6213"\w* \w fürder|strong="H3254"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w*; und der \w HERR|strong="H3068"\w* \w gab|strong="H5414"\w* sie in die \w Hände|strong="H3027"\w* der \w Philister|strong="H6430"\w* \w vierzig|strong="H0705"\w* \w Jahre|strong="H8141"\w*.  
\v 2 Es \w war|strong="H1961"\w* aber \w ein|strong="H0259"\w* \w Mann|strong="H0376"\w* \w zu|strong="H4480"\w* \w Zora|strong="H6881"\w* \w von|strong="H4480"\w* einem \w Geschlecht|strong="H4940"\w* der \w Daniter|strong="H1839"\w*, mit \w Namen|strong="H8034"\w* \w Manoah|strong="H4495"\w*; und sein \w Weib|strong="H0802"\w* war \w unfruchtbar|strong="H6135"\w* und \w gebar|strong="H3205"\w* \w nicht|strong="H3808"\w*.  
\v 3 Und der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w erschien|strong="H7200"\w* \w dem|strong="H0413"\w* \w Weibe|strong="H0802"\w* und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihr: \w Siehe|strong="H2009"\w*, \w du|strong="H0859"\w* bist \w unfruchtbar|strong="H6135"\w* und \w gebierst|strong="H3205"\w* \w nicht|strong="H3808"\w*; aber du wirst schwanger \w werden|strong="H2029"\w* und einen \w Sohn|strong="H1121"\w* \w gebären|strong="H3205"\w*.  
\v 4 So hüte \w dich|strong="H8104"\w* \w nun|strong="H6258"\w*, daß du \w nicht|strong="H0408"\w* \w Wein|strong="H3196"\w* noch starkes \w Getränk|strong="H7941"\w* \w trinkst|strong="H8354"\w* und \w nichts|strong="H0408"\w* \w Unreines|strong="H2931"\w* \w essest|strong="H0398"\w*;  
\v 5  \w Denn|strong="H3588"\w* du wirst schwanger \w werden|strong="H2030"\w* und einen \w Sohn|strong="H1121"\w* \w gebären|strong="H3205"\w*, dem \w kein|strong="H3808"\w* \w Schermesser|strong="H4177"\w* soll \w aufs|strong="H5921"\w* \w Haupt|strong="H7218"\w* \w kommen|strong="H5927"\w*. \w Denn|strong="H3588"\w* der \w Knabe|strong="H5288"\w* wird ein \w Geweihter|strong="H5139"\w* \w Gottes|strong="H0430"\w* \w sein|strong="H1961"\w* \w von|strong="H4480"\w* \w Mutterleibe|strong="H0990"\w* an; und \w er|strong="H1931"\w* wird \w anfangen|strong="H2490"\w*, \w Israel|strong="H3478"\w* zu \w erlösen|strong="H3467"\w* \w aus|strong="H4480"\w* der \w Philister|strong="H6430"\w* \w Hand|strong="H3027"\w*.  
\v 6 Da \w kam|strong="H0935"\w* das \w Weib|strong="H0802"\w* und \w sagte|strong="H0559"\w* es ihrem \w Mann|strong="H0376"\w* an und \w sprach|strong="H0559"\w*: Es \w kam|strong="H0935"\w* ein \w Mann|strong="H0376"\w* \w Gottes|strong="H0430"\w* \w zu|strong="H0413"\w* mir, und seine \w Gestalt|strong="H4758"\w* war \w anzusehen|strong="H4758"\w* wie ein \w Engel|strong="H4397"\w* \w Gottes|strong="H0430"\w*, \w gar|strong="H3966"\w* \w erschrecklich|strong="H3372"\w*, daß ich ihn \w nicht|strong="H3808"\w* \w fragte|strong="H7592"\w*, \w woher|strong="H4480"\w* oder \w wohin|strong="H1931"\w*; und er \w sagte|strong="H5046"\w* mir \w nicht|strong="H3808"\w*, wie er \w hieße|strong="H8034"\w*.  
\v 7 Er \w sprach|strong="H0559"\w* aber zu mir: \w Siehe|strong="H2009"\w*, du wirst schwanger \w werden|strong="H2030"\w* und einen \w Sohn|strong="H1121"\w* \w gebären|strong="H3205"\w*. So \w trinke|strong="H8354"\w* \w nun|strong="H6258"\w* \w keinen|strong="H0408"\w* \w Wein|strong="H3196"\w* noch starkes \w Getränk|strong="H7941"\w* und \w iß|strong="H0398"\w* \w nichts|strong="H0408"\w* \w Unreines|strong="H2932"\w*; \w denn|strong="H3588"\w* der \w Knabe|strong="H5288"\w* soll ein \w Geweihter|strong="H5139"\w* \w Gottes|strong="H0430"\w* \w sein|strong="H1961"\w* \w von|strong="H4480"\w* \w Mutterleibe|strong="H0990"\w* an \w bis|strong="H5704"\w* an \w seinen|strong="H3117"\w* \w Tod|strong="H4194"\w*.  
\v 8 Da \w bat|strong="H6279"\w* \w Manoah|strong="H4495"\w* \w den|strong="H0413"\w* \w HERRN|strong="H3068"\w* und \w sprach|strong="H0559"\w*: \w Ach|strong="H0994"\w* \w Herr|strong="H0136"\w*, \w laß|strong="H4994"\w* den \w Mann|strong="H0376"\w* \w Gottes|strong="H0430"\w* \w wieder|strong="H5750"\w* \w zu|strong="H0413"\w* uns \w kommen|strong="H0935"\w*, \w den|strong="H0834"\w* du gesandt \w hast|strong="H7971"\w*, daß er uns \w lehre|strong="H3384"\w*, \w was|strong="H4100"\w* wir mit dem \w Knaben|strong="H5288"\w* tun \w sollen|strong="H6213"\w*, der geboren soll \w werden|strong="H3205"\w*.  
\v 9 Und \w Gott|strong="H0430"\w* \w erhörte|strong="H8085"\w* die \w Stimme|strong="H6963"\w* \w Manoahs|strong="H4495"\w*; und der \w Engel|strong="H4397"\w* \w Gottes|strong="H0430"\w* \w kam|strong="H0935"\w* \w wieder|strong="H5750"\w* \w zum|strong="H0413"\w* \w Weibe|strong="H0802"\w*. \w Sie|strong="H1931"\w* \w saß|strong="H3427"\w* aber auf dem \w Felde|strong="H7704"\w*, und ihr \w Mann|strong="H0376"\w* \w Manoah|strong="H4495"\w* war \w nicht|strong="H0369"\w* \w bei|strong="H5973"\w* ihr.  
\v 10 Da \w lief|strong="H7323"\w* \w sie|strong="H0802"\w* \w eilend|strong="H4116"\w* und \w sagte|strong="H5046"\w* es ihrem \w Mann|strong="H0376"\w* an und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihm: \w Siehe|strong="H2009"\w*, der \w Mann|strong="H0376"\w* ist \w mir|strong="H0413"\w* \w erschienen|strong="H7200"\w*, \w der|strong="H0834"\w* jenes \w Tages|strong="H3117"\w* \w zu|strong="H0413"\w* mir \w kam|strong="H0935"\w*.  
\v 11  \w Manoah|strong="H4495"\w* machte sich \w auf|strong="H6965"\w* und \w ging|strong="H3212"\w* seinem \w Weibe|strong="H0802"\w* \w nach|strong="H0310"\w* und \w kam|strong="H0935"\w* \w zu|strong="H0413"\w* dem \w Mann|strong="H0376"\w* und \w sprach|strong="H0559"\w* zu ihm: Bist \w du|strong="H0859"\w* der \w Mann|strong="H0376"\w*, \w der|strong="H0834"\w* \w mit|strong="H0413"\w* dem \w Weibe|strong="H0802"\w* geredet \w hat|strong="H1696"\w*? Er \w sprach|strong="H0559"\w*: \w Ja|strong="H0589"\w*.  
\v 12 Und \w Manoah|strong="H4495"\w* \w sprach|strong="H0559"\w*: Wenn \w nun|strong="H6258"\w* \w kommen|strong="H0935"\w* wird, \w was|strong="H4100"\w* du geredet \w hast|strong="H1697"\w*, welches soll des \w Knaben|strong="H5288"\w* \w Weise|strong="H4941"\w* und \w Werk|strong="H4639"\w* \w sein|strong="H1961"\w*?  
\v 13 Der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* \w Manoah|strong="H4495"\w*: \w Vor|strong="H4480"\w* \w allem|strong="H3605"\w*, \w was|strong="H0834"\w* ich \w dem|strong="H0413"\w* \w Weibe|strong="H0802"\w* gesagt \w habe|strong="H0559"\w*, soll sie sich \w hüten|strong="H8104"\w*.  
\v 14 Sie soll \w nicht|strong="H3808"\w* \w essen|strong="H0398"\w* \w was|strong="H0834"\w* \w aus|strong="H4480"\w* dem \w Weinstock|strong="H1612"\w* \w kommt|strong="H3318"\w*, und soll \w keinen|strong="H3808"\w* \w Wein|strong="H3196"\w* noch starkes \w Getränk|strong="H7941"\w* \w trinken|strong="H8354"\w* \w und|strong="H0408"\w* \w nichts|strong="H3605"\w* \w Unreines|strong="H2932"\w* \w essen|strong="H0398"\w*; \w alles|strong="H3605"\w*, \w was|strong="H0834"\w* ich ihr geboten \w habe|strong="H6680"\w*, soll sie \w halten|strong="H8104"\w*.  
\v 15  \w Manoah|strong="H4495"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* dem \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w*: Laß \w dich|strong="H0853"\w* \w doch|strong="H4994"\w* \w halten|strong="H6113"\w*; wir wollen \w dir|strong="H6440"\w* ein \w Ziegenböcklein|strong="H1423"\w* \w zurichten|strong="H6213"\w*.  
\v 16 Aber der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w antwortete|strong="H0559"\w* \w Manoah|strong="H4495"\w*: \w Wenn|strong="H0518"\w* du gleich mich hier \w hältst|strong="H6113"\w*, so \w esse|strong="H0398"\w* ich doch von deiner \w Speise|strong="H3899"\w* \w nicht|strong="H3808"\w*. \w Willst|strong="H0518"\w* du aber dem \w HERRN|strong="H3068"\w* ein \w Brandopfer|strong="H5930"\w* \w tun|strong="H6213"\w*, so magst du es \w opfern|strong="H5927"\w*. \w Denn|strong="H3588"\w* \w Manoah|strong="H4495"\w* \w wußte|strong="H3045"\w* \w nicht|strong="H3808"\w*, \w daß|strong="H3588"\w* \w es|strong="H1931"\w* der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* war.  
\v 17 Und \w Manoah|strong="H4495"\w* \w sprach|strong="H0559"\w* \w zum|strong="H0413"\w* \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w*: \w Wie|strong="H4310"\w* heißest \w du|strong="H8034"\w*? daß wir dich \w preisen|strong="H3513"\w*, \w wenn|strong="H3588"\w* nun \w kommt|strong="H0935"\w*, was du geredet \w hast|strong="H1697"\w*.  
\v 18 Aber der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w sprach|strong="H0559"\w* zu ihm: \w Warum|strong="H4100"\w* fragst \w du|strong="H7592"\w* nach meinem \w Namen|strong="H8034"\w*, \w der|strong="H1931"\w* doch \w wundersam|strong="H6383"\w* ist?  
\v 19 Da \w nahm|strong="H3947"\w* \w Manoah|strong="H4495"\w* ein \w Ziegenböcklein|strong="H1423"\w* und \w Speisopfer|strong="H4503"\w* und \w opferte|strong="H5927"\w* es \w auf|strong="H5921"\w* einem \w Fels|strong="H6697"\w* dem \w HERRN|strong="H3068"\w*. Und Er \w tat|strong="H6213"\w* Wunderbares-\w Manoah|strong="H6381"\w* aber und sein \w Weib|strong="H0802"\w* sahen \w zu|strong="H7200"\w*;  
\v 20  \w denn|strong="H1961"\w* da die \w Lohe|strong="H3851"\w* \w auffuhr|strong="H5927"\w* \w vom|strong="H4480"\w* \w Altar|strong="H4196"\w* gen \w Himmel|strong="H8064"\w*, \w fuhr|strong="H5927"\w* der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* in der \w Lohe|strong="H3851"\w* des \w Altars|strong="H4196"\w* mit hinauf. Da das \w Manoah|strong="H4495"\w* und sein \w Weib|strong="H0802"\w* \w sahen|strong="H7200"\w*, fielen \w sie|strong="H5307"\w* zur \w Erde|strong="H0776"\w* \w auf|strong="H5921"\w* ihr \w Angesicht|strong="H6440"\w*.  
\v 21 Und der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* \w erschien|strong="H7200"\w* \w nicht|strong="H3808"\w* \w mehr|strong="H3254"\w* \w Manoah|strong="H4495"\w* \w und|strong="H0413"\w* seinem \w Weibe|strong="H0802"\w*. \w Da|strong="H0227"\w* \w erkannte|strong="H3045"\w* \w Manoah|strong="H4495"\w*, \w daß|strong="H3588"\w* \w es|strong="H1931"\w* der \w Engel|strong="H4397"\w* des \w HERRN|strong="H3068"\w* war,  
\v 22  \w und|strong="H4495"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* seinem \w Weibe|strong="H0802"\w*: Wir \w müssen|strong="H4191"\w* des Todes \w sterben|strong="H4191"\w*, \w daß|strong="H3588"\w* wir \w Gott|strong="H0430"\w* gesehen \w haben|strong="H7200"\w*.  
\v 23 Aber sein \w Weib|strong="H0802"\w* \w antwortete|strong="H0559"\w* ihm: \w Wenn|strong="H3863"\w* der \w HERR|strong="H3068"\w* Lust \w hätte|strong="H2654"\w*, uns zu \w töten|strong="H4191"\w*, so hätte er das \w Brandopfer|strong="H5930"\w* und \w Speisopfer|strong="H4503"\w* \w nicht|strong="H3808"\w* \w genommen|strong="H3947"\w* \w von|strong="H4480"\w* unsern \w Händen|strong="H3027"\w*; \w er|strong="H3808"\w* hätte \w uns|strong="H0428"\w* auch nicht \w alles|strong="H3605"\w* solches \w erzeigt|strong="H7200"\w* \w noch|strong="H3808"\w* uns \w solches|strong="H2063"\w* hören \w lassen|strong="H8085"\w*, wie \w jetzt|strong="H6256"\w* geschehen ist.  
\v 24 Und das \w Weib|strong="H0802"\w* \w gebar|strong="H3205"\w* einen \w Sohn|strong="H1121"\w* und \w hieß|strong="H7121"\w* ihn \w Simson|strong="H8123"\w*. Und der \w Knabe|strong="H5288"\w* \w wuchs|strong="H1431"\w*, und der \w HERR|strong="H3068"\w* \w segnete|strong="H1288"\w* ihn.  
\v 25 Und der \w Geist|strong="H7307"\w* des \w HERRN|strong="H3068"\w* fing \w an|strong="H2490"\w*, ihn zu \w treiben|strong="H6470"\w* im \w Lager|strong="H4264"\w* \w Dan|strong="H1835"\w* \w zwischen|strong="H0996"\w* \w Zora|strong="H6881"\w* \w und|strong="H0996"\w* \w Esthaol|strong="H0847"\w*.   
\c 14  
\p
\v 1  \w Simson|strong="H8123"\w* \w ging|strong="H3381"\w* gen \w Thimnath|strong="H8553"\w* und \w sah|strong="H7200"\w* ein \w Weib|strong="H0802"\w* zu \w Thimnath|strong="H8553"\w* \w unter|strong="H4480"\w* den \w Töchtern|strong="H1323"\w* der \w Philister|strong="H6430"\w*.  
\v 2 Und da er \w heraufkam|strong="H5927"\w*, \w sagte|strong="H5046"\w* er's an seinem \w Vater|strong="H0001"\w* und seiner \w Mutter|strong="H0517"\w* und \w sprach|strong="H0559"\w*: Ich habe ein \w Weib|strong="H0802"\w* \w gesehen|strong="H7200"\w* zu \w Thimnath|strong="H8553"\w* \w unter|strong="H4480"\w* den \w Töchtern|strong="H1323"\w* der \w Philister|strong="H6430"\w*; \w gebt|strong="H3947"\w* \w mir|strong="H0853"\w* \w nun|strong="H6258"\w* diese zum \w Weibe|strong="H0802"\w*.  
\v 3 Sein \w Vater|strong="H0001"\w* und sein \w Mutter|strong="H0517"\w* \w sprachen|strong="H0559"\w* zu ihm: Ist denn nun \w kein|strong="H0369"\w* \w Weib|strong="H0802"\w* unter den \w Töchtern|strong="H1323"\w* deiner \w Brüder|strong="H0251"\w* und in \w allem|strong="H3605"\w* deinem \w Volk|strong="H5971"\w*, \w daß|strong="H3588"\w* du \w hingehst|strong="H1980"\w* und \w nimmst|strong="H3947"\w* ein \w Weib|strong="H0802"\w* \w bei|strong="H4480"\w* den \w Philistern|strong="H6430"\w*, die \w unbeschnitten|strong="H6189"\w* sind? \w Simson|strong="H8123"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* seinem \w Vater|strong="H0001"\w*: \w Gib|strong="H3947"\w* mir \w diese|strong="H0853"\w*; \w denn|strong="H3588"\w* \w sie|strong="H1931"\w* \w gefällt|strong="H3474"\w* meinen \w Augen|strong="H5869"\w*.  
\v 4 Aber sein \w Vater|strong="H0001"\w* und seine \w Mutter|strong="H0517"\w* \w wußten|strong="H3045"\w* \w nicht|strong="H3808"\w*, \w daß|strong="H3588"\w* \w es|strong="H1931"\w* von dem \w HERRN|strong="H3068"\w* wäre; \w denn|strong="H3588"\w* \w er|strong="H1931"\w* \w suchte|strong="H1245"\w* \w Ursache|strong="H8385"\w* \w wider|strong="H4480"\w* die \w Philister|strong="H6430"\w*. \w Die|strong="H3588"\w* \w Philister|strong="H6430"\w* aber \w herrschten|strong="H4910"\w* \w zu|strong="H1931"\w* der \w Zeit|strong="H6256"\w* über \w Israel|strong="H3478"\w*.  
