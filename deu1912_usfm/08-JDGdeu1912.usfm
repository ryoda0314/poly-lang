\v 10 Da \w auch|strong="H1571"\w* \w alle|strong="H3605"\w*, \w die|strong="H1755"\w* zu der Zeit gelebt \w hatten|strong="H1931"\w*, \w zu|strong="H0413"\w* ihren \w Vätern|strong="H0001"\w* versammelt \w wurden|strong="H0622"\w*, \w kam|strong="H6965"\w* \w nach|strong="H0310"\w* ihnen ein \w andres|strong="H0312"\w* \w Geschlecht|strong="H1755"\w* \w auf|strong="H6965"\w*, \w das|strong="H0834"\w* den \w HERRN|strong="H3068"\w* \w nicht|strong="H3808"\w* \w kannte|strong="H3045"\w* \w noch|strong="H1571"\w* sein \w Werke|strong="H4639"\w*, \w die|strong="H0834"\w* er an \w Israel|strong="H3478"\w* getan \w hatte|strong="H6213"\w*.  
\v 11 Da \w taten|strong="H6213"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w* und \w dienten|strong="H5647"\w* den \w Baalim|strong="H1168"\w*  
\v 12 und \w verließen|strong="H5800"\w* den \w HERRN|strong="H3068"\w*, ihrer \w Väter|strong="H0001"\w* \w Gott|strong="H0430"\w*, der sie \w aus|strong="H4480"\w* \w Ägyptenland|strong="H0776"\w* geführt \w hatte|strong="H3318"\w*, und \w folgten|strong="H3212"\w* \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w* \w nach|strong="H0310"\w* \w von|strong="H4480"\w* den \w Göttern|strong="H0430"\w* der \w Völker|strong="H5971"\w*, \w die|strong="H0834"\w* \w um|strong="H5439"\w* sie her \w wohnten|strong="H7812"\w*, und beteten sie an und \w erzürnten|strong="H3707"\w* den \w HERRN|strong="H3068"\w*;  
\v 13 denn sie \w verließen|strong="H5800"\w* je und je den \w HERRN|strong="H3068"\w* und \w dienten|strong="H5647"\w* \w Baal|strong="H1168"\w* und den \w Astharoth|strong="H6252"\w*.  
\v 14 So \w ergrimmte|strong="H2734"\w* der \w Zorn|strong="H0639"\w* des \w HERRN|strong="H3068"\w* über \w Israel|strong="H3478"\w* und \w gab|strong="H5414"\w* sie in die \w Hand|strong="H3027"\w* der \w Räuber|strong="H8154"\w*, daß diese sie \w beraubten|strong="H8155"\w*, und \w verkaufte|strong="H4376"\w* sie in die \w Hände|strong="H3027"\w* ihrer \w Feinde|strong="H0341"\w* \w umher|strong="H5439"\w*. Und sie \w konnten|strong="H3201"\w* \w nicht|strong="H3808"\w* \w mehr|strong="H5750"\w* \w ihren|strong="H6440"\w* \w Feinden|strong="H0341"\w* \w widerstehen|strong="H5975"\w*;  
\v 15  \w sondern|strong="H3605"\w* \w wo|strong="H0834"\w* sie hinaus \w wollten|strong="H3318"\w*, da \w war|strong="H1961"\w* des \w HERRN|strong="H3068"\w* \w Hand|strong="H3027"\w* wider sie zum \w Unglück|strong="H7451"\w*, \w wie|strong="H0834"\w* denn der \w HERR|strong="H3068"\w* ihnen \w gesagt|strong="H1696"\w* \w und|strong="H0834"\w* geschworen \w hatte|strong="H7650"\w*. Und sie wurden \w hart|strong="H3966"\w* \w gedrängt|strong="H3334"\w*.  
\v 16 Wenn dann der \w HERR|strong="H3068"\w* \w Richter|strong="H8199"\w* \w auferweckte|strong="H6965"\w*, die ihnen \w halfen|strong="H3467"\w* \w aus|strong="H4480"\w* der \w Räuber|strong="H8154"\w* \w Hand|strong="H3027"\w*,  
\v 17 so \w gehorchten|strong="H8085"\w* sie \w den|strong="H0413"\w* \w Richtern|strong="H8199"\w* \w auch|strong="H1571"\w* \w nicht|strong="H3808"\w*, \w sondern|strong="H3588"\w* \w liefen|strong="H2181"\w* \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w* \w nach|strong="H0310"\w* und beteten \w sie|strong="H7812"\w* an und \w wichen|strong="H5493"\w* \w bald|strong="H4118"\w* \w von|strong="H4480"\w* dem \w Weg|strong="H1870"\w*, \w darauf|strong="H0834"\w* ihre \w Väter|strong="H0001"\w* gegangen \w waren|strong="H1980"\w*, des \w HERRN|strong="H3068"\w* \w Geboten|strong="H4687"\w* zu \w gehorchen|strong="H8085"\w*, und \w taten|strong="H6213"\w* \w nicht|strong="H3808"\w* wie \w dieselben|strong="H3651"\w*.  
\v 18  \w Wenn|strong="H3588"\w* aber der \w HERR|strong="H3068"\w* ihnen \w Richter|strong="H8199"\w* \w erweckte|strong="H6965"\w*, so \w war|strong="H1961"\w* der \w HERR|strong="H3068"\w* \w mit|strong="H5973"\w* dem \w Richter|strong="H8199"\w* und \w half|strong="H3467"\w* ihnen \w aus|strong="H4480"\w* ihrer \w Feinde|strong="H0341"\w* \w Hand|strong="H3027"\w*, \w solange|strong="H3605"\w* der \w Richter|strong="H8199"\w* \w lebte|strong="H3117"\w*. \w Denn|strong="H3588"\w* es \w jammerte|strong="H5162"\w* den \w HERRN|strong="H3068"\w* ihr \w Wehklagen|strong="H5009"\w* über \w die|strong="H4480"\w*, so sie \w zwangen|strong="H3905"\w* und \w drängten|strong="H1766"\w*.  
\v 19 Wenn \w aber|strong="H1961"\w* der \w Richter|strong="H8199"\w* \w starb|strong="H4194"\w*, so wandten sie \w sich|strong="H7725"\w* und \w verderbten|strong="H7843"\w* es mehr \w denn|strong="H4480"\w* ihre \w Väter|strong="H0001"\w*, daß sie \w andern|strong="H0312"\w* \w Göttern|strong="H0430"\w* \w folgten|strong="H3212"\w*, ihnen zu \w dienen|strong="H5647"\w* und sie \w anzubeten|strong="H7812"\w*; sie \w ließen|strong="H5307"\w* \w nicht|strong="H3808"\w* \w von|strong="H4480"\w* ihrem \w Vornehmen|strong="H4611"\w* noch \w von|strong="H4480"\w* ihrem \w halsstarrigen|strong="H7186"\w* \w Wesen|strong="H1870"\w*.  
\v 20 Darum \w ergrimmte|strong="H2734"\w* denn des \w HERRN|strong="H3068"\w* \w Zorn|strong="H0639"\w* über \w Israel|strong="H3478"\w*, daß er \w sprach|strong="H0559"\w*: \w Weil|strong="H3588"\w* \w dies|strong="H2088"\w* \w Volk|strong="H1471"\w* meinen \w Bund|strong="H1285"\w* übertreten \w hat|strong="H5674"\w*, \w den|strong="H0834"\w* ich ihren \w Vätern|strong="H0001"\w* geboten \w habe|strong="H6680"\w*, und \w gehorchen|strong="H8085"\w* meiner \w Stimme|strong="H6963"\w* \w nicht|strong="H3808"\w*,  
\v 21 so will \w ich|strong="H0589"\w* \w auch|strong="H1571"\w* \w hinfort|strong="H3254"\w* die \w Heiden|strong="H1471"\w* \w nicht|strong="H3808"\w* \w vertreiben|strong="H3423"\w*, \w die|strong="H0834"\w* \w Josua|strong="H3091"\w* hat \w gelassen|strong="H5800"\w*, da er \w starb|strong="H4191"\w*,  
\v 22  \w daß|strong="H4616"\w* ich \w Israel|strong="H3478"\w* durch sie \w versuche|strong="H5254"\w*, ob \w sie|strong="H1992"\w* auf dem \w Wege|strong="H1870"\w* des \w HERRN|strong="H3068"\w* \w bleiben|strong="H8104"\w*, daß sie darin \w wandeln|strong="H3212"\w*, \w wie|strong="H0834"\w* ihre \w Väter|strong="H0001"\w* geblieben \w sind|strong="H8104"\w*, \w oder|strong="H0518"\w* \w nicht|strong="H3808"\w*.  
\v 23 Also \w ließ|strong="H3240"\w* der \w HERR|strong="H3068"\w* \w diese|strong="H0428"\w* \w Heiden|strong="H1471"\w*, daß er sie \w nicht|strong="H1115"\w* \w bald|strong="H4118"\w* \w vertrieb|strong="H3423"\w*, die er \w nicht|strong="H3808"\w* hatte in \w Josuas|strong="H3091"\w* \w Hand|strong="H3027"\w* \w übergeben|strong="H5414"\w*.   
\c 3  
\p
\v 1  \w Dies|strong="H0428"\w* sind die \w Heiden|strong="H1471"\w*, \w die|strong="H0834"\w* der \w HERR|strong="H3068"\w* ließ \w bleiben|strong="H3240"\w*, daß er durch sie \w Israel|strong="H3478"\w* \w versuchte|strong="H5254"\w*, \w alle|strong="H3605"\w*, \w die|strong="H0834"\w* \w nicht|strong="H3808"\w* \w wußten|strong="H3045"\w* \w um|strong="H3605"\w* die \w Kriege|strong="H4421"\w* \w Kanaans|strong="H3667"\w*,  
\v 2  \w und|strong="H7535"\w* \w daß|strong="H4616"\w* die \w Geschlechter|strong="H1755"\w* der \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w wüßten|strong="H3045"\w* und \w lernten|strong="H3925"\w* \w streiten|strong="H4421"\w*, \w die|strong="H0834"\w* \w zuvor|strong="H6440"\w* \w nichts|strong="H3808"\w* \w darum|strong="H7535"\w* \w wußten|strong="H3045"\w*,  
\v 3 nämlich die \w fünf|strong="H2568"\w* \w Fürsten|strong="H5633"\w* der \w Philister|strong="H6430"\w* und \w alle|strong="H3605"\w* \w Kanaaniter|strong="H3669"\w* und \w Sidonier|strong="H6722"\w* und \w Heviter|strong="H2340"\w*, die am \w Berg|strong="H2022"\w* \w Libanon|strong="H3844"\w* \w wohnten|strong="H3427"\w*, \w von|strong="H4480"\w* dem \w Berg|strong="H2022"\w* Baal-\w Hermon|strong="H1179"\w* an, \w bis|strong="H5704"\w* wo man \w kommt|strong="H0935"\w* gen \w Hamath|strong="H2574"\w*.  
\v 4 Dieselben \w blieben|strong="H1961"\w*, \w Israel|strong="H3478"\w* durch sie zu \w versuchen|strong="H5254"\w*, daß es kund \w würde|strong="H3045"\w*, ob sie den \w Geboten|strong="H4687"\w* des \w HERRN|strong="H3068"\w* \w gehorchten|strong="H8085"\w*, \w die|strong="H0834"\w* er ihren \w Vätern|strong="H0001"\w* geboten \w hatte|strong="H6680"\w* \w durch|strong="H3027"\w* \w Mose|strong="H4872"\w*.  
\v 5 Da nun die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* also \w wohnten|strong="H3427"\w* \w unter|strong="H7130"\w* den \w Kanaanitern|strong="H3669"\w*, \w Hethitern|strong="H2850"\w*, \w Amoritern|strong="H0567"\w*, \w Pheresitern|strong="H6522"\w*, \w Hevitern|strong="H2340"\w* und \w Jebusitern|strong="H2983"\w*,  
\v 6 nahmen \w sie|strong="H3947"\w* jener \w Töchter|strong="H1323"\w* zu \w Weibern|strong="H0802"\w* und \w gaben|strong="H5414"\w* ihre \w Töchter|strong="H1323"\w* jener \w Söhnen|strong="H1121"\w* und \w dienten|strong="H5647"\w* jener \w Göttern|strong="H0430"\w*.  
\v 7 Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w taten|strong="H6213"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w* und \w vergaßen|strong="H7911"\w* des \w HERRN|strong="H3068"\w*, ihres \w Gottes|strong="H0430"\w*, und \w dienten|strong="H5647"\w* den \w Baalim|strong="H1168"\w* und den \w Ascheroth|strong="H0842"\w*.  
\v 8 Da \w ergrimmte|strong="H2734"\w* der \w Zorn|strong="H0639"\w* des \w HERRN|strong="H3068"\w* über \w Israel|strong="H3478"\w*, und er \w verkaufte|strong="H4376"\w* sie unter die \w Hand|strong="H3027"\w* Kusan-\w Risathaims|strong="H3573"\w*, des \w Königs|strong="H4428"\w* von \w Mesopotamien|strong="H0763"\w*; und \w dienten|strong="H5647"\w* also die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* dem Kusan-\w Risathaim|strong="H3573"\w* \w acht|strong="H8083"\w* \w Jahre|strong="H8141"\w*.  
\v 9 Da \w schrieen|strong="H2199"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w zu|strong="H0413"\w* dem \w HERRN|strong="H3068"\w*; und der \w HERR|strong="H3068"\w* \w erweckte|strong="H6965"\w* \w ihnen|strong="H1121"\w* einen \w Heiland|strong="H3467"\w*, der sie \w erlöste|strong="H3467"\w*; \w Othniel|strong="H6274"\w*, den \w Sohn|strong="H1121"\w* \w Kenas|strong="H7073"\w*, \w Kalebs|strong="H3612"\w* \w jüngsten|strong="H6996"\w* \w Bruder|strong="H0251"\w*.  
\v 10 Und der \w Geist|strong="H7307"\w* des \w HERRN|strong="H3068"\w* \w kam|strong="H1961"\w* \w auf|strong="H5921"\w* ihn, und er ward \w Richter|strong="H8199"\w* in \w Israel|strong="H3478"\w* und zog \w aus|strong="H3318"\w* zum \w Streit|strong="H4421"\w*. Und der \w HERR|strong="H3068"\w* \w gab|strong="H5414"\w* den \w König|strong="H4428"\w* von \w Mesopotamien|strong="H0763"\w*, Kusan-\w Risathaim|strong="H3573"\w*, in seine \w Hand|strong="H3027"\w*, daß seine \w Hand|strong="H3027"\w* \w über|strong="H5921"\w* \w ihn|strong="H3573"\w* zu stark \w ward|strong="H5810"\w*.  
\v 11 Da ward das \w Land|strong="H0776"\w* \w still|strong="H8252"\w* \w vierzig|strong="H0705"\w* \w Jahre|strong="H8141"\w*. Und \w Othniel|strong="H6274"\w*, der \w Sohn|strong="H1121"\w* \w Kenas|strong="H7073"\w*, \w starb|strong="H4191"\w*.  
\v 12 Aber die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w taten|strong="H6213"\w* \w fürder|strong="H3254"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w*. Da \w stärkte|strong="H2388"\w* der \w HERR|strong="H3068"\w* den \w Eglon|strong="H5700"\w*, den \w König|strong="H4428"\w* der \w Moabiter|strong="H4124"\w*, \w wider|strong="H5921"\w* \w Israel|strong="H3478"\w*, \w darum|strong="H5921"\w* daß sie \w übel|strong="H7451"\w* \w taten|strong="H6213"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w*.  
\v 13 Und er \w sammelte|strong="H0622"\w* zu sich die \w Kinder|strong="H1121"\w* \w Ammon|strong="H5983"\w* und die \w Amalekiter|strong="H6002"\w* und zog \w hin|strong="H3212"\w* und \w schlug|strong="H5221"\w* \w Israel|strong="H3478"\w* und nahm \w ein|strong="H3423"\w* die \w Palmenstadt|strong="H5892"\w*.  
\v 14 Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w dienten|strong="H5647"\w* \w Eglon|strong="H5700"\w*, dem \w König|strong="H4428"\w* der \w Moabiter|strong="H4124"\w*, \w achtzehn|strong="H8083"\w* \w Jahre|strong="H8141"\w*.  
\v 15 Da \w schrieen|strong="H2199"\w* \w sie|strong="H1121"\w* \w zu|strong="H0413"\w* dem \w HERRN|strong="H3068"\w*; und der \w HERR|strong="H3068"\w* \w erweckte|strong="H6965"\w* ihnen einen \w Heiland|strong="H3467"\w*: \w Ehud|strong="H0164"\w*, den \w Sohn|strong="H1121"\w* \w Geras|strong="H1617"\w*, den \w Benjaminiten|strong="H1145"\w*, \w der|strong="H0376"\w* war \w links|strong="H3225"\w*. Und da die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w durch|strong="H3027"\w* ihn \w Geschenk|strong="H4503"\w* \w sandten|strong="H7971"\w* \w Eglon|strong="H5700"\w*, dem \w König|strong="H4428"\w* der \w Moabiter|strong="H4124"\w*,  
\v 16  \w machte|strong="H6213"\w* sich \w Ehud|strong="H0164"\w* ein \w zweischneidig|strong="H8147"\w* \w Schwert|strong="H2719"\w*, eine \w Elle|strong="H1574"\w* \w lang|strong="H0753"\w*, und \w gürtete|strong="H2296"\w* es \w unter|strong="H4480"\w* sein \w Kleid|strong="H4055"\w* \w auf|strong="H5921"\w* seine rechte \w Hüfte|strong="H3225"\w*  
\v 17 und \w brachte|strong="H7126"\w* das \w Geschenk|strong="H4503"\w* dem \w Eglon|strong="H5700"\w*, dem \w König|strong="H4428"\w* der \w Moabiter|strong="H4124"\w*. \w Eglon|strong="H5700"\w* aber war ein \w sehr|strong="H3966"\w* \w fetter|strong="H1277"\w* \w Mann|strong="H0376"\w*.  
\v 18  \w Und|strong="H1961"\w* \w da|strong="H0834"\w* er das \w Geschenk|strong="H4503"\w* \w hatte|strong="H3615"\w* \w überantwortet|strong="H7126"\w*, ließ \w er|strong="H7971"\w* das \w Volk|strong="H5971"\w* von sich, die das \w Geschenk|strong="H4503"\w* getragen \w hatten|strong="H5375"\w*,  
\v 19  \w und|strong="H1931"\w* kehrte \w um|strong="H7725"\w* \w von|strong="H4480"\w* den \w Götzen|strong="H6456"\w* \w zu|strong="H0834"\w* \w Gilgal|strong="H1537"\w* und ließ \w ansagen|strong="H0559"\w*: Ich habe, o \w König|strong="H4428"\w*, dir was \w Heimliches|strong="H5643"\w* \w zu|strong="H0413"\w* \w sagen|strong="H0559"\w*. Er aber hieß \w schweigen|strong="H2013"\w*, und gingen \w aus|strong="H3318"\w* \w von|strong="H4480"\w* ihm \w alle|strong="H3605"\w*, die \w um|strong="H5921"\w* ihn \w standen|strong="H5975"\w*.  
\v 20 Und \w Ehud|strong="H0164"\w* kam \w zu|strong="H0413"\w* ihm \w hinein|strong="H0935"\w*. \w Er|strong="H1931"\w* aber \w saß|strong="H3427"\w* oben in der \w Sommerlaube|strong="H4747"\w*, \w die|strong="H0834"\w* für ihn \w allein|strong="H0909"\w* war. Und \w Ehud|strong="H0164"\w* \w sprach|strong="H0559"\w*: Ich habe \w Gottes|strong="H0430"\w* \w Wort|strong="H1697"\w* \w an|strong="H0413"\w* dich. Da stand er \w auf|strong="H6965"\w* \w vom|strong="H4480"\w* \w Stuhl|strong="H3678"\w*.  
\v 21  \w Ehud|strong="H0164"\w* aber \w reckte|strong="H7971"\w* seine linke \w Hand|strong="H8040"\w* \w aus|strong="H7971"\w* und \w nahm|strong="H3947"\w* das \w Schwert|strong="H2719"\w* \w von|strong="H4480"\w* seiner \w rechten|strong="H3225"\w* \w Hüfte|strong="H3409"\w* und \w stieß|strong="H8628"\w* es ihm in seinen \w Bauch|strong="H0990"\w*,  
\v 22 daß \w auch|strong="H1571"\w* das \w Heft|strong="H5325"\w* \w der|strong="H0310"\w* \w Schneide|strong="H3851"\w* nach \w hineinfuhr|strong="H0935"\w* und das \w Fett|strong="H2459"\w* \w das|strong="H1157"\w* \w Heft|strong="H3851"\w* \w verschloß|strong="H5462"\w*; \w denn|strong="H3588"\w* er \w zog|strong="H8025"\w* das \w Schwert|strong="H2719"\w* \w nicht|strong="H3808"\w* \w aus|strong="H4480"\w* seinem \w Bauch|strong="H0990"\w*.  
\v 23 Aber \w Ehud|strong="H0164"\w* \w ging|strong="H3318"\w* zum \w Saal|strong="H4528"\w* hinaus und \w tat|strong="H5462"\w* die \w Tür|strong="H1817"\w* der \w Sommerlaube|strong="H5944"\w* \w hinter|strong="H1157"\w* sich zu und verschloß \w sie|strong="H5274"\w*.  
\v 24 Da \w er|strong="H1931"\w* nun hinaus \w war|strong="H3318"\w*, \w kamen|strong="H0935"\w* seine \w Knechte|strong="H5650"\w* und \w sahen|strong="H7200"\w*, daß die \w Tür|strong="H1817"\w* verschlossen \w war|strong="H5274"\w*, und \w sprachen|strong="H0559"\w*: \w Er|strong="H1931"\w* ist \w vielleicht|strong="H0389"\w* zu Stuhl \w gegangen|strong="H5526"\w* in der \w Kammer|strong="H4747"\w* an der \w Sommerlaube|strong="H7272"\w*.  
\v 25 Da sie aber so lange \w harrten|strong="H2342"\w*, \w bis|strong="H5704"\w* sie sich \w schämten|strong="H0954"\w* (\w denn|strong="H2009"\w* \w niemand|strong="H0369"\w* \w tat|strong="H6605"\w* die \w Tür|strong="H1817"\w* der \w Laube|strong="H5944"\w* \w auf|strong="H6605"\w*), nahmen \w sie|strong="H3947"\w* den \w Schlüssel|strong="H4668"\w* und schlossen \w auf|strong="H6605"\w*; \w siehe|strong="H2009"\w*, da \w lag|strong="H5307"\w* ihr \w Herr|strong="H0113"\w* auf der \w Erde|strong="H0776"\w* \w tot|strong="H4191"\w*.  
\v 26  \w Ehud|strong="H0164"\w* aber war \w entronnen|strong="H4422"\w*, \w dieweil|strong="H5704"\w* sie \w verzogen|strong="H4102"\w*, \w und|strong="H1931"\w* \w ging|strong="H5674"\w* an den \w Götzen|strong="H6456"\w* vorüber und \w entrann|strong="H4422"\w* bis gen \w Seira|strong="H8167"\w*.  
\v 27 Und \w da|strong="H1961"\w* er \w hineinkam|strong="H0935"\w*, blies \w er|strong="H8628"\w* die \w Posaune|strong="H7782"\w* auf dem \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w*. Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* zogen \w mit|strong="H5973"\w* \w ihm|strong="H3381"\w* \w vom|strong="H4480"\w* \w Gebirge|strong="H2022"\w* und \w er|strong="H1931"\w* \w vor|strong="H6440"\w* ihnen her,  
\v 28 und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihnen: \w Jagt|strong="H7291"\w* mir \w nach|strong="H0310"\w*; \w denn|strong="H3588"\w* der \w HERR|strong="H3068"\w* hat euch die \w Moabiter|strong="H4124"\w*, eure \w Feinde|strong="H0341"\w*, in eure \w Hände|strong="H3027"\w* \w gegeben|strong="H5414"\w*! Und sie \w jagten|strong="H3381"\w* ihm \w nach|strong="H0310"\w* und \w gewannen|strong="H3920"\w* die \w Furten|strong="H4569"\w* am \w Jordan|strong="H3383"\w*, die gen \w Moab|strong="H4124"\w* gehen, und \w ließen|strong="H5414"\w* \w niemand|strong="H3808"\w* \w hinüber|strong="H5674"\w*  
\v 29 und \w schlugen|strong="H5221"\w* die \w Moabiter|strong="H4124"\w* \w zu|strong="H1931"\w* der \w Zeit|strong="H6256"\w*, bei \w zehntausend|strong="H6235"\w* \w Mann|strong="H0376"\w*, \w allzumal|strong="H3605"\w* die \w besten|strong="H8082"\w* \w und|strong="H3605"\w* \w streitbare|strong="H2428"\w* \w Männer|strong="H0376"\w*, daß \w nicht|strong="H3808"\w* \w einer|strong="H0376"\w* \w entrann|strong="H4422"\w*.  
\v 30 Also wurden die \w Moabiter|strong="H4124"\w* \w zu|strong="H1931"\w* der \w Zeit|strong="H3117"\w* \w unter|strong="H8478"\w* die \w Hand|strong="H3027"\w* der Kinder \w Israel|strong="H3478"\w* \w gedämpft|strong="H3665"\w*. Und das \w Land|strong="H0776"\w* war \w still|strong="H8252"\w* \w achtzig|strong="H8084"\w* \w Jahre|strong="H8141"\w*.  
\v 31  \w Darnach|strong="H0310"\w* \w war|strong="H1961"\w* \w Samgar|strong="H8044"\w*, der \w Sohn|strong="H1121"\w* \w Anaths|strong="H6067"\w*; der \w schlug|strong="H5221"\w* \w sechshundert|strong="H8337"\w* \w Philister|strong="H6430"\w* mit einem \w Ochsenstecken|strong="H1241"\w*, und \w auch|strong="H1571"\w* er \w erlöste|strong="H3467"\w* \w Israel|strong="H3478"\w*.   
\c 4  
\p
\v 1 Aber die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w taten|strong="H6213"\w* \w fürder|strong="H3254"\w* \w übel|strong="H7451"\w* \w vor|strong="H5869"\w* dem \w HERRN|strong="H3068"\w*, da \w Ehud|strong="H0164"\w* gestorben \w war|strong="H4191"\w*.  
\v 2 Und der \w HERR|strong="H3068"\w* \w verkaufte|strong="H4376"\w* sie in die \w Hand|strong="H3027"\w* \w Jabins|strong="H2985"\w*, des \w Königs|strong="H4428"\w* der \w Kanaaniter|strong="H3667"\w*, \w der|strong="H0834"\w* zu \w Hazor|strong="H2674"\w* \w saß|strong="H4427"\w*; und sein \w Feldhauptmann|strong="H8269"\w* war \w Sisera|strong="H5516"\w*, \w und|strong="H1931"\w* er \w wohnte|strong="H3427"\w* zu \w Haroseth|strong="H2800"\w* der \w Heiden|strong="H1471"\w*.  
\v 3 Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w schrieen|strong="H6817"\w* \w zum|strong="H0413"\w* \w HERRN|strong="H3068"\w*; \w denn|strong="H3588"\w* er hatte \w neunhundert|strong="H8672"\w* \w eiserne|strong="H1270"\w* \w Wagen|strong="H7393"\w* \w und|strong="H1931"\w* \w zwang|strong="H3905"\w* die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* mit \w Gewalt|strong="H2394"\w* \w zwanzig|strong="H6242"\w* \w Jahre|strong="H8141"\w*.  
\v 4  \w Zu|strong="H1931"\w* der \w Zeit|strong="H6256"\w* \w war|strong="H1931"\w* \w Richterin|strong="H8199"\w* in \w Israel|strong="H3478"\w* die \w Prophetin|strong="H5031"\w* \w Debora|strong="H1683"\w*, das \w Weib|strong="H0802"\w* \w Lapidoths|strong="H3941"\w*.  
\v 5 Und \w sie|strong="H1931"\w* \w wohnte|strong="H3427"\w* \w unter|strong="H8478"\w* der \w Palme|strong="H8560"\w* \w Deboras|strong="H1683"\w* \w zwischen|strong="H0996"\w* \w Rama|strong="H7414"\w* \w und|strong="H0996"\w* Beth-\w El|strong="H1008"\w* auf dem \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w*. Und die \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w kamen|strong="H5927"\w* \w zu|strong="H0413"\w* ihr hinauf vor \w Gericht|strong="H4941"\w*.  
\v 6 Diese sandte \w hin|strong="H7971"\w* und ließ \w rufen|strong="H7121"\w* \w Barak|strong="H1301"\w*, den \w Sohn|strong="H1121"\w* \w Abinoams|strong="H0042"\w* von Kedes-\w Naphthali|strong="H6943"\w*, und ließ \w ihm|strong="H0413"\w* \w sagen|strong="H0559"\w*: Hat dir \w nicht|strong="H3808"\w* der \w HERR|strong="H3068"\w*, der \w Gott|strong="H0430"\w* \w Israels|strong="H3478"\w*, \w geboten|strong="H6680"\w*: Gehe \w hin|strong="H3212"\w* und \w zieh|strong="H4900"\w* auf den \w Berg|strong="H2022"\w* \w Thabor|strong="H8396"\w* und \w nimm|strong="H3947"\w* \w zehntausend|strong="H6235"\w* \w Mann|strong="H0376"\w* \w mit|strong="H5973"\w* dir \w von|strong="H4480"\w* den \w Kindern|strong="H1121"\w* \w Naphthali|strong="H5321"\w* \w und|strong="H4480"\w* \w Sebulon|strong="H2074"\w*?  
\v 7 Denn ich will \w Sisera|strong="H5516"\w*, den \w Feldhauptmann|strong="H8269"\w* \w Jabins|strong="H2985"\w*, \w zu|strong="H0413"\w* dir \w ziehen|strong="H4900"\w* \w an|strong="H0413"\w* das \w Wasser|strong="H5158"\w* \w Kison|strong="H7028"\w* mit seinen \w Wagen|strong="H7393"\w* und mit seiner \w Menge|strong="H1995"\w* und will ihn in deine \w Hände|strong="H3027"\w* \w geben|strong="H5414"\w*.  
\v 8  \w Barak|strong="H1301"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihr: \w Wenn|strong="H0518"\w* du \w mit|strong="H5973"\w* mir \w ziehst|strong="H3212"\w*, so will ich \w ziehen|strong="H1980"\w*; \w ziehst|strong="H3212"\w* du \w aber|strong="H0518"\w* \w nicht|strong="H3808"\w* \w mit|strong="H5973"\w* mir, so will ich \w nicht|strong="H3808"\w* \w ziehen|strong="H3212"\w*.  
\v 9 Sie \w sprach|strong="H0559"\w*: Ich \w will|strong="H1980"\w* \w mit|strong="H5973"\w* dir \w ziehen|strong="H3212"\w*, \w aber|strong="H0657"\w* \w der|strong="H3588"\w* \w Preis|strong="H8597"\w* wird \w nicht|strong="H3808"\w* dein \w sein|strong="H1961"\w* \w auf|strong="H5921"\w* dieser \w Reise|strong="H1870"\w*, \w die|strong="H0834"\w* \w du|strong="H0859"\w* \w tust|strong="H1980"\w*, \w sondern|strong="H3588"\w* der \w HERR|strong="H3068"\w* wird \w Sisera|strong="H5516"\w* in eines \w Weibes|strong="H0802"\w* \w Hand|strong="H3027"\w* \w übergeben|strong="H4376"\w*. Also \w machte|strong="H6965"\w* sich \w Debora|strong="H1683"\w* \w auf|strong="H6965"\w* und \w zog|strong="H3212"\w* \w mit|strong="H5973"\w* \w Barak|strong="H1301"\w* gen \w Kedes|strong="H6943"\w*.  
\v 10 Da \w rief|strong="H2199"\w* \w Barak|strong="H1301"\w* \w Sebulon|strong="H2074"\w* und \w Naphthali|strong="H5321"\w* gen \w Kedes|strong="H6943"\w*, und es zogen \w hinauf|strong="H5927"\w* \w ihm|strong="H7272"\w* nach \w zehntausend|strong="H6235"\w* \w Mann|strong="H0376"\w*. \w Debora|strong="H1683"\w* \w zog|strong="H5927"\w* auch \w mit|strong="H5973"\w* \w ihm|strong="H5927"\w*.  
\v 11 (\w Heber|strong="H2268"\w* aber, der \w Keniter|strong="H7017"\w*, war \w von|strong="H4480"\w* den \w Kenitern|strong="H7017"\w*, \w von|strong="H4480"\w* den \w Kindern|strong="H1121"\w* \w Hobabs|strong="H2246"\w*, Mose'\w s|strong="H4872"\w* \w Schwagers|strong="H2859"\w*, \w weggezogen|strong="H6504"\w* und hatte seine \w Hütte|strong="H0168"\w* \w aufgeschlagen|strong="H5186"\w* \w bei|strong="H5704"\w* den \w Eichen|strong="H0436"\w* zu \w Zaanannim|strong="H6815"\w* \w neben|strong="H0834"\w* \w Kedes|strong="H6943"\w*.)  
\v 12 Da ward \w Sisera|strong="H5516"\w* \w angesagt|strong="H5046"\w*, \w daß|strong="H3588"\w* \w Barak|strong="H1301"\w*, der \w Sohn|strong="H1121"\w* \w Abinoams|strong="H0042"\w*, auf den \w Berg|strong="H2022"\w* \w Thabor|strong="H8396"\w* gezogen \w wäre|strong="H5927"\w*.  
\v 13 Und \w er|strong="H5516"\w* rief \w alle|strong="H3605"\w* seine \w Wagen|strong="H7393"\w* \w zusammen|strong="H2199"\w*, \w neunhundert|strong="H8672"\w* \w eiserne|strong="H1270"\w* \w Wagen|strong="H7393"\w*, und \w alles|strong="H3605"\w* \w Volk|strong="H5971"\w*, \w das|strong="H0834"\w* \w mit|strong="H0854"\w* ihm war, \w von|strong="H4480"\w* \w Haroseth|strong="H2800"\w* der \w Heiden|strong="H1471"\w* \w an|strong="H0413"\w* das \w Wasser|strong="H5158"\w* \w Kison|strong="H7028"\w*.  
\v 14  \w Debora|strong="H1683"\w* aber \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* \w Barak|strong="H1301"\w*: \w Auf|strong="H6965"\w*! \w das|strong="H2088"\w* ist der \w Tag|strong="H3117"\w*, \w da|strong="H3588"\w* dir der \w HERR|strong="H3068"\w* den \w Sisera|strong="H5516"\w* hat \w in|strong="H0834"\w* deine \w Hand|strong="H3027"\w* \w gegeben|strong="H5414"\w*; denn der \w HERR|strong="H3068"\w* wird vor \w dir|strong="H6440"\w* her \w ausziehen|strong="H3318"\w*. Also zog \w Barak|strong="H1301"\w* \w von|strong="H4480"\w* dem \w Berge|strong="H2022"\w* \w Thabor|strong="H8396"\w* \w herab|strong="H3381"\w* und die \w zehntausend|strong="H6235"\w* \w Mann|strong="H0376"\w* ihm \w nach|strong="H0310"\w*.  
\v 15 Aber der \w HERR|strong="H3068"\w* \w erschreckte|strong="H2000"\w* den \w Sisera|strong="H5516"\w* samt \w allen|strong="H3605"\w* seinen \w Wagen|strong="H7393"\w* und \w ganzem|strong="H3605"\w* \w Heer|strong="H4264"\w* \w vor|strong="H6440"\w* der \w Schärfe|strong="H6310"\w* des \w Schwertes|strong="H2719"\w* \w Baraks|strong="H1301"\w*, daß \w Sisera|strong="H5516"\w* \w von|strong="H4480"\w* seinem \w Wagen|strong="H4818"\w* \w sprang|strong="H3381"\w* und \w floh|strong="H5127"\w* zu \w Fuß|strong="H7272"\w*.  
\v 16  \w Barak|strong="H1301"\w* aber \w jagte|strong="H7291"\w* \w nach|strong="H0310"\w* den \w Wagen|strong="H7393"\w* \w und|strong="H0310"\w* dem \w Heer|strong="H4264"\w* \w bis|strong="H5704"\w* gen \w Haroseth|strong="H2800"\w* der \w Heiden|strong="H1471"\w*. Und \w alles|strong="H3605"\w* \w Heer|strong="H4264"\w* \w Siseras|strong="H5516"\w* \w fiel|strong="H5307"\w* vor der \w Schärfe|strong="H6310"\w* des \w Schwerts|strong="H2719"\w*, \w daß|strong="H5704"\w* \w nicht|strong="H3808"\w* \w einer|strong="H0259"\w* \w übrigblieb|strong="H7604"\w*.  
\v 17  \w Sisera|strong="H5516"\w* aber \w floh|strong="H5127"\w* zu \w Fuß|strong="H7272"\w* \w in|strong="H0413"\w* die \w Hütte|strong="H0168"\w* \w Jaels|strong="H3278"\w*, des \w Weibes|strong="H0802"\w* \w Hebers|strong="H2268"\w*, des \w Keniters|strong="H7017"\w*. \w Denn|strong="H3588"\w* der \w König|strong="H4428"\w* \w Jabin|strong="H2985"\w* zu \w Hazor|strong="H2674"\w* \w und|strong="H0996"\w* das \w Haus|strong="H1004"\w* \w Hebers|strong="H2268"\w*, des \w Keniters|strong="H7017"\w*, standen miteinander im \w Frieden|strong="H7965"\w*.  
\v 18  \w Jael|strong="H3278"\w* aber ging \w heraus|strong="H3318"\w*, \w Sisera|strong="H5516"\w* \w entgegen|strong="H7125"\w*, und \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihm: \w Weiche|strong="H5493"\w*, mein \w Herr|strong="H0113"\w*, \w weiche|strong="H5493"\w* \w zu|strong="H0413"\w* mir und \w fürchte|strong="H3372"\w* dich \w nicht|strong="H0408"\w*! Und er \w wich|strong="H5493"\w* \w zu|strong="H0413"\w* ihr in die \w Hütte|strong="H0168"\w*, und sie \w deckte|strong="H3680"\w* ihn zu mit einer \w Decke|strong="H8063"\w*.  
\v 19 Er aber \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihr: Gib mir \w doch|strong="H4994"\w* ein \w wenig|strong="H4592"\w* \w Wasser|strong="H4325"\w* zu \w trinken|strong="H8248"\w*, \w denn|strong="H3588"\w* mich \w dürstet|strong="H6770"\w*. Da tat sie \w auf|strong="H6605"\w* einen \w Milchtopf|strong="H4997"\w* und gab ihm zu \w trinken|strong="H8248"\w* und deckte ihn \w zu|strong="H3680"\w*.  
\v 20 Und er \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihr: \w Tritt|strong="H5975"\w* in der \w Hütte|strong="H0168"\w* \w Tür|strong="H6607"\w*, und \w wenn|strong="H1961"\w* \w jemand|strong="H0518"\w* \w kommt|strong="H0935"\w* und \w fragt|strong="H7592"\w*, ob \w jemand|strong="H0376"\w* hier \w sei|strong="H3426"\w*, so \w sprich|strong="H0559"\w*: \w Niemand|strong="H0369"\w*.  
\v 21 Da \w nahm|strong="H3947"\w* \w Jael|strong="H3278"\w*, das \w Weib|strong="H0802"\w* \w Hebers|strong="H2268"\w*, einen \w Nagel|strong="H3489"\w* von der \w Hütte|strong="H0168"\w* \w und|strong="H7760"\w* einen \w Hammer|strong="H4718"\w* in ihre \w Hand|strong="H3027"\w* und \w ging|strong="H0935"\w* \w leise|strong="H3814"\w* \w zu|strong="H0413"\w* ihm hinein und \w schlug|strong="H8628"\w* ihm den \w Nagel|strong="H3489"\w* durch seine \w Schläfe|strong="H7541"\w*, daß er in die \w Erde|strong="H0776"\w* \w drang|strong="H6795"\w*. \w Er|strong="H1931"\w* aber war \w entschlummert|strong="H7290"\w*, ward \w ohnmächtig|strong="H5774"\w* und \w starb|strong="H4191"\w*.  
\v 22  \w Da|strong="H2009"\w* aber \w Barak|strong="H1301"\w* \w Sisera|strong="H5516"\w* \w nachjagte|strong="H7291"\w*, ging \w Jael|strong="H3278"\w* \w heraus|strong="H3318"\w*, ihm \w entgegen|strong="H7125"\w*, und \w sprach|strong="H0559"\w* zu ihm: \w Gehe|strong="H3212"\w* her! ich will dir den \w Mann|strong="H0376"\w* \w zeigen|strong="H7200"\w*, \w den|strong="H0834"\w* \w du|strong="H0859"\w* \w suchst|strong="H1245"\w*. Und da \w er|strong="H0935"\w* \w zu|strong="H0413"\w* ihr \w hineinkam|strong="H2009"\w*, \w lag|strong="H5307"\w* \w Sisera|strong="H5516"\w* \w tot|strong="H4191"\w*, und der \w Nagel|strong="H3489"\w* steckte in seiner \w Schläfe|strong="H7541"\w*.  
\v 23 Also \w dämpfte|strong="H3665"\w* \w Gott|strong="H0430"\w* \w zu|strong="H1931"\w* der \w Zeit|strong="H3117"\w* \w Jabin|strong="H2985"\w*, der \w Kanaaniter|strong="H3667"\w* \w König|strong="H4428"\w*, \w vor|strong="H6440"\w* den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w*.  
\v 24 Und die \w Hand|strong="H3027"\w* der \w Kinder|strong="H1121"\w* \w Israel|strong="H3478"\w* \w ward|strong="H3212"\w* \w immer|strong="H1980"\w* \w stärker|strong="H7186"\w* \w wider|strong="H5921"\w* \w Jabin|strong="H2985"\w*, der \w Kanaaniter|strong="H3667"\w* \w König|strong="H4428"\w*, \w bis|strong="H5704"\w* \w sie|strong="H2985"\w* \w ihn|strong="H4428"\w* \w ausrotteten|strong="H3772"\w*.   
\c 5  
\p
\v 1 Da \w sang|strong="H7891"\w* \w Debora|strong="H1683"\w* und \w Barak|strong="H1301"\w*, der \w Sohn|strong="H1121"\w* \w Abinoams|strong="H0042"\w*, \w zu|strong="H1931"\w* der \w Zeit|strong="H3117"\w* und \w sprachen|strong="H0559"\w*:  
\v 2  \w Lobet|strong="H1288"\w* den \w HERRN|strong="H3068"\w*, daß \w Israel|strong="H3478"\w* \w wieder|strong="H6544"\w* \w frei|strong="H6546"\w* geworden ist und das \w Volk|strong="H5971"\w* willig dazu gewesen \w ist|strong="H5068"\w*.  
\v 3  \w Höret|strong="H8085"\w* zu, ihr \w Könige|strong="H4428"\w*, und merket \w auf|strong="H0238"\w*, ihr \w Fürsten|strong="H7336"\w*! \w Ich|strong="H0595"\w* will, dem \w HERRN|strong="H3068"\w* will \w ich|strong="H0595"\w* \w singen|strong="H7891"\w*; dem \w HERRN|strong="H3068"\w*, dem \w Gott|strong="H0430"\w* \w Israels|strong="H3478"\w*, will ich \w spielen|strong="H2167"\w*.  
\v 4  \w HERR|strong="H3068"\w*, da du \w von|strong="H4480"\w* \w Seir|strong="H8165"\w* \w auszogst|strong="H3318"\w* und \w einhergingst|strong="H6805"\w* \w vom|strong="H4480"\w* \w Felde|strong="H7704"\w* \w Edoms|strong="H0123"\w*, da \w erzitterte|strong="H7493"\w* die \w Erde|strong="H0776"\w*, \w der|strong="H1571"\w* \w Himmel|strong="H8064"\w* \w troff|strong="H5197"\w*, \w und|strong="H1571"\w* die \w Wolken|strong="H5645"\w* \w troffen|strong="H5197"\w* von \w Wasser|strong="H4325"\w*.  
