\v 1  \w Zu|strong="H1992"\w* der \w Zeit|strong="H3117"\w* war \w kein|strong="H0369"\w* \w König|strong="H4428"\w* in \w Israel|strong="H3478"\w*. Und ein \w levitischer|strong="H3881"\w* \w Mann|strong="H0376"\w* \w war|strong="H1961"\w* \w Fremdling|strong="H1481"\w* an der \w Seite|strong="H3411"\w* des \w Gebirges|strong="H2022"\w* \w Ephraim|strong="H0669"\w* und hatte sich ein \w Kebsweib|strong="H0802"\w* \w genommen|strong="H3947"\w* \w von|strong="H4480"\w* Bethlehem-\w Juda|strong="H1035"\w*.  
\v 2 Und da \w sie|strong="H6370"\w* hatte neben ihm \w gehurt|strong="H2181"\w*, \w lief|strong="H4480"\w* sie \w von|strong="H5921"\w* ihm \w zu|strong="H0413"\w* ihres \w Vaters|strong="H0001"\w* \w Haus|strong="H1004"\w* \w gen|strong="H0413"\w* Bethlehem-\w Juda|strong="H1035"\w* und \w war|strong="H1961"\w* \w daselbst|strong="H8033"\w* \w vier|strong="H0702"\w* \w Monate|strong="H2320"\w* \w lang|strong="H3117"\w*.  
\v 3 Und ihr \w Mann|strong="H0376"\w* machte sich \w auf|strong="H6965"\w* und \w zog|strong="H3212"\w* ihr \w nach|strong="H0310"\w*, \w daß|strong="H5921"\w* er \w freundlich|strong="H3820"\w* mit ihr \w redete|strong="H1696"\w* und sie wieder zu sich \w holte|strong="H7725"\w*; und hatte einen \w Knecht|strong="H5288"\w* und ein \w Paar|strong="H6776"\w* \w Esel|strong="H2543"\w* mit \w sich|strong="H5973"\w*. Und sie \w führte|strong="H0935"\w* ihn in ihres \w Vaters|strong="H0001"\w* \w Haus|strong="H1004"\w*. Da ihn aber der \w Vater|strong="H0001"\w* der \w Dirne|strong="H5291"\w* \w sah|strong="H7200"\w*, ward er \w froh|strong="H8055"\w* und empfing \w ihn|strong="H7125"\w*.  
\v 4 Und sein \w Schwiegervater|strong="H2859"\w*, der \w Dirne|strong="H5291"\w* \w Vater|strong="H0001"\w*, \w hielt|strong="H2388"\w* ihn, daß er \w drei|strong="H7969"\w* \w Tage|strong="H3117"\w* \w bei|strong="H0854"\w* ihm \w blieb|strong="H3427"\w*; sie \w aßen|strong="H0398"\w* und \w tranken|strong="H8354"\w* und blieben des \w Nachts|strong="H3885"\w* \w da|strong="H8033"\w*.  
\v 5  \w Des|strong="H1961"\w* \w vierten|strong="H7243"\w* \w Tages|strong="H3117"\w* erhoben sie sich des \w Morgens|strong="H1242"\w* \w früh|strong="H7925"\w*, und er machte sich \w auf|strong="H6965"\w* und wollte \w ziehen|strong="H3212"\w*. Da \w sprach|strong="H0559"\w* der \w Dirne|strong="H5291"\w* \w Vater|strong="H0001"\w* \w zu|strong="H0413"\w* seinem \w Eidam|strong="H2860"\w*: \w Labe|strong="H5582"\w* dein \w Herz|strong="H3820"\w* zuvor mit einem \w Bissen|strong="H6595"\w* \w Brot|strong="H3899"\w*, \w darnach|strong="H0310"\w* sollt ihr \w ziehen|strong="H3212"\w*.  
\v 6 Und sie setzten \w sich|strong="H3427"\w* und \w aßen|strong="H0398"\w* \w beide|strong="H8147"\w* \w miteinander|strong="H3162"\w* und \w tranken|strong="H8354"\w*. Da \w sprach|strong="H0559"\w* der \w Dirne|strong="H5291"\w* \w Vater|strong="H0001"\w* \w zu|strong="H0413"\w* dem \w Mann|strong="H0376"\w*: \w Bleib|strong="H2974"\w* \w doch|strong="H4994"\w* über \w Nacht|strong="H3885"\w* und laß dein \w Herz|strong="H3820"\w* guter Dinge \w sein|strong="H3190"\w*.  
\v 7 Da aber der \w Mann|strong="H0376"\w* \w aufstand|strong="H6965"\w* und wollte \w ziehen|strong="H3212"\w*, \w nötigte|strong="H6484"\w* ihn sein \w Schwiegervater|strong="H2859"\w*, \w daß|strong="H8033"\w* er \w über|strong="H3885"\w* Nacht \w dablieb|strong="H7725"\w*.  
\v 8 Des \w Morgens|strong="H1242"\w* am \w fünften|strong="H2549"\w* \w Tage|strong="H3117"\w* machte er sich früh \w auf|strong="H7925"\w* und wollte \w ziehen|strong="H3212"\w*. Da \w sprach|strong="H0559"\w* der \w Dirne|strong="H5291"\w* \w Vater|strong="H0001"\w*: \w Labe|strong="H5582"\w* \w doch|strong="H4994"\w* dein \w Herz|strong="H3824"\w* und laß uns \w verziehen|strong="H4102"\w*, \w bis|strong="H5704"\w* sich der \w Tag|strong="H3117"\w* \w neigt|strong="H5186"\w*. Und \w aßen|strong="H0398"\w* also die \w beiden|strong="H8147"\w* miteinander.  
\v 9 Und der \w Mann|strong="H0376"\w* machte sich \w auf|strong="H6965"\w* und wollte \w ziehen|strong="H3212"\w* \w mit|strong="H1931"\w* seinem \w Kebsweib|strong="H6370"\w* und mit seinem \w Knechte|strong="H5288"\w*. Aber sein \w Schwiegervater|strong="H2859"\w*, der \w Dirne|strong="H5291"\w* \w Vater|strong="H0001"\w*, \w sprach|strong="H0559"\w* zu ihm: \w Siehe|strong="H2009"\w*, der \w Tag|strong="H3117"\w* hat sich \w geneigt|strong="H7503"\w*, und es will Abend \w werden|strong="H6150"\w*; bleib über \w Nacht|strong="H3885"\w*. \w Siehe|strong="H2009"\w*, \w hier|strong="H6311"\w* ist \w Herberge|strong="H3885"\w* noch diesen \w Tag|strong="H3117"\w*; bleibe hier über \w Nacht|strong="H2583"\w* und laß dein \w Herz|strong="H3824"\w* guter Dinge \w sein|strong="H3190"\w*. \w Morgen|strong="H4279"\w* steht ihr früh \w auf|strong="H7925"\w* und \w zieht|strong="H1980"\w* eures \w Weges|strong="H1870"\w* zu deiner \w Hütte|strong="H0168"\w*.  
\v 10 Aber der \w Mann|strong="H0376"\w* \w wollte|strong="H0014"\w* \w nicht|strong="H3808"\w* über Nacht \w bleiben|strong="H3885"\w*, sondern machte sich \w auf|strong="H6965"\w* und zog \w hin|strong="H3212"\w* und \w kam|strong="H0935"\w* \w bis|strong="H5704"\w* \w vor|strong="H5227"\w* \w Jebus|strong="H2982"\w*, \w das|strong="H1931"\w* ist \w Jerusalem|strong="H3389"\w*, und \w sein|strong="H5973"\w* \w Paar|strong="H6776"\w* \w Esel|strong="H2543"\w* \w beladen|strong="H2280"\w* und sein \w Kebsweib|strong="H6370"\w* \w mit|strong="H5973"\w* ihm.  
\v 11 Da \w sie|strong="H1992"\w* nun nahe \w bei|strong="H5973"\w* \w Jebus|strong="H2982"\w* kamen, \w sank|strong="H7286"\w* der \w Tag|strong="H3117"\w* \w sehr|strong="H3966"\w* \w dahin|strong="H7286"\w*. Und der \w Knecht|strong="H5288"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* seinem \w Herrn|strong="H0113"\w*: \w Komm|strong="H3212"\w* \w doch|strong="H4994"\w* und laß \w uns|strong="H0413"\w* in \w diese|strong="H2063"\w* \w Stadt|strong="H5892"\w* der \w Jebusiter|strong="H2983"\w* \w einkehren|strong="H5493"\w* und über Nacht darin \w bleiben|strong="H3885"\w*.  
\v 12 Aber sein \w Herr|strong="H0113"\w* \w sprach|strong="H0559"\w* \w zu|strong="H0413"\w* ihm: Wir \w wollen|strong="H2008"\w* \w nicht|strong="H3808"\w* \w in|strong="H0413"\w* der \w Fremden|strong="H5237"\w* \w Stadt|strong="H5892"\w* \w einkehren|strong="H5493"\w*, \w die|strong="H0834"\w* \w nicht|strong="H3808"\w* \w sind|strong="H4480"\w* von den \w Kindern|strong="H1121"\w* \w Israel|strong="H3478"\w*, sondern wollen \w hinüber|strong="H5674"\w* \w gen|strong="H5704"\w* \w Gibea|strong="H1390"\w*.  
\v 13 Und \w sprach|strong="H0559"\w* zu seinem \w Knecht|strong="H5288"\w*: Gehe \w weiter|strong="H3212"\w*, daß wir \w hinzukommen|strong="H7126"\w* an \w einen|strong="H0259"\w* \w Ort|strong="H4725"\w* und \w über|strong="H3885"\w* Nacht zu \w Gibea|strong="H1390"\w* oder zu \w Rama|strong="H7414"\w* \w bleiben|strong="H3885"\w*.  
\v 14 Und sie zogen \w weiter|strong="H5674"\w* und \w wandelten|strong="H3212"\w*, und die \w Sonne|strong="H8121"\w* ging ihnen \w unter|strong="H0935"\w*, hart \w bei|strong="H0681"\w* \w Gibea|strong="H1390"\w*, \w das|strong="H0834"\w* liegt in \w Benjamin|strong="H1144"\w*.  
\v 15 Und sie \w kehrten|strong="H5493"\w* \w daselbst|strong="H8033"\w* ein, daß sie \w hineinkämen|strong="H0935"\w* und über Nacht zu \w Gibea|strong="H1390"\w* \w blieben|strong="H3885"\w*. Da er aber \w hineinkam|strong="H0935"\w*, setzte \w er|strong="H3427"\w* sich in der \w Stadt|strong="H5892"\w* \w Gasse|strong="H7339"\w*; \w denn|strong="H3588"\w* es \w war|strong="H0369"\w* \w niemand|strong="H0376"\w*, der \w sie|strong="H0853"\w* bei \w Nacht|strong="H3885"\w* im \w Hause|strong="H1004"\w* \w herbergen|strong="H3885"\w* \w wollte|strong="H0622"\w*.  
\v 16 Und \w siehe|strong="H2009"\w*, da \w kam|strong="H0935"\w* ein \w alter|strong="H2205"\w* \w Mann|strong="H0376"\w* \w von|strong="H4480"\w* seiner \w Arbeit|strong="H4639"\w* \w vom|strong="H4480"\w* \w Felde|strong="H7704"\w* am \w Abend|strong="H6153"\w*, und \w er|strong="H0376"\w* war auch \w vom|strong="H4480"\w* \w Gebirge|strong="H2022"\w* \w Ephraim|strong="H0669"\w* \w und|strong="H1931"\w* ein \w Fremdling|strong="H1481"\w* zu \w Gibea|strong="H1390"\w*; aber die \w Leute|strong="H0582"\w* des \w Orts|strong="H4725"\w* waren \w Benjaminiter|strong="H1145"\w*.  
\v 17 Und da er seine \w Augen|strong="H5869"\w* \w aufhob|strong="H5375"\w* und \w sah|strong="H7200"\w* \w den|strong="H0732"\w* \w Gast|strong="H0376"\w* auf der \w Gasse|strong="H7339"\w*, \w sprach|strong="H0559"\w* \w er|strong="H2205"\w* zu ihm: \w Wo|strong="H0575"\w* willst du \w hin|strong="H3212"\w*? \w und|strong="H4480"\w* \w wo|strong="H0370"\w* kommst du \w her|strong="H0935"\w*?  
\v 18 Er aber \w antwortete|strong="H0559"\w* ihm: \w Wir|strong="H0587"\w* \w reisen|strong="H5674"\w* \w von|strong="H4480"\w* Bethlehem-\w Juda|strong="H1035"\w*, \w bis|strong="H5704"\w* wir kommen an die \w Seite|strong="H3411"\w* des \w Gebirges|strong="H2022"\w* \w Ephraim|strong="H0669"\w*, \w daher|strong="H4480"\w* \w ich|strong="H0595"\w* bin; und bin \w gen|strong="H5704"\w* Bethlehem-\w Juda|strong="H1035"\w* \w gezogen|strong="H3212"\w* \w und|strong="H0589"\w* \w ziehe|strong="H1980"\w* jetzt zum \w Hause|strong="H1004"\w* des \w HERRN|strong="H3068"\w*, \w und|strong="H0369"\w* \w niemand|strong="H0376"\w* will \w mich|strong="H0853"\w* \w beherbergen|strong="H1004"\w*.  
\v 19 Wir \w haben|strong="H3426"\w* \w Stroh|strong="H8401"\w* \w und|strong="H1571"\w* \w Futter|strong="H4554"\w* für unsre \w Esel|strong="H2543"\w* \w und|strong="H1571"\w* \w Brot|strong="H3899"\w* \w und|strong="H1571"\w* \w Wein|strong="H3196"\w* \w für|strong="H3426"\w* mich und deine \w Magd|strong="H0519"\w* und für den \w Knecht|strong="H5288"\w*, der \w mit|strong="H5973"\w* deinem \w Diener|strong="H5650"\w* \w ist|strong="H0369"\w*, daß uns \w nichts|strong="H3605"\w* \w gebricht|strong="H4270"\w*.  
\v 20 Der \w alte|strong="H2205"\w* \w Mann|strong="H0376"\w* \w sprach|strong="H0559"\w*: \w Friede|strong="H7965"\w* sei mit \w dir|strong="H7535"\w*! \w Alles|strong="H3605"\w* was dir \w mangelt|strong="H4270"\w* findest du \w bei|strong="H5921"\w* \w mir|strong="H7535"\w*; bleibe nur \w nicht|strong="H0408"\w* über \w Nacht|strong="H3885"\w* auf der \w Gasse|strong="H7339"\w*.  
\v 21 Und \w führte|strong="H0935"\w* ihn in sein \w Haus|strong="H1004"\w* und gab den \w Eseln|strong="H2543"\w* \w Futter|strong="H1101"\w*, und sie \w wuschen|strong="H7364"\w* ihre \w Füße|strong="H7272"\w* und \w aßen|strong="H0398"\w* und \w tranken|strong="H8354"\w*.  
